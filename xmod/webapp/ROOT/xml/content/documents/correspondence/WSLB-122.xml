   <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~ H+JSchenker</tei:rs>
   <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~ HSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002748">Telegram</tei:rs>

   <dc:title>Handwritten telegram draft from Schenker to Hertzka (UE), dated June 24,
    1912</dc:title>

   <dcterms:created>1912-06-24T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2023-01-26</dcterms:dateSubmitted>
   <dc:description>Schenker asks why Beethovens neunte Sinfonie has not yet
    appeared.</dc:description>
   <dc:subject/>

   <marcrel:correspondent key="entity-000632">Heinrich Schenker</marcrel:correspondent>
   <marcrel:recipient key="entity-000356">Emil Hertzka</marcrel:recipient>
   <marcrel:recipient key="entity-002408">Universal Edition</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">

   <tei:note type="editorial">Roman type = printed; Italic type = handwritten</tei:note>
   <tei:lb/>
   <tei:note type="editorial">top-left panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Die Daten im dienstlichen Eingange der mittels
    Typendruckapparates ausgefertigten Telegramme bedeuten: 1. den Namen des Aufgabeamtes, 2. die
    Aufgabenummer, 3. die Wortzahl (eventuell in Bruchform), 4. den Monatstag und 5. die Stunde und
    Minute der Aufgabe.</tei:ab>

   <tei:ab type="formpart" rend="printed">Dienstliche Angaben:</tei:ab>

   <tei:note type="editorial">top-center panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Gattung: <tei:handShift new="#unk"/><tei:seg type="entry"
     rend="ms">10</tei:seg> – Eingangsnummer: <tei:handShift new="#unk"/><tei:seg type="entry"
     rend="ms">139/49</tei:seg></tei:ab>

   <tei:ab type="formpart" rend="printed">
    <tei:address>
     <tei:addrLine>
      <tei:seg type="entry" rend="ms"><tei:handShift new="#HS"/>Direktor Hertzka</tei:seg>
     </tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="ms">I Reichrathstraße 9</tei:seg></tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="stamp">Universal Edition</tei:seg></tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="ms">Wien</tei:seg></tei:addrLine>
    </tei:address>
   </tei:ab>

   <tei:ab type="formpart" rend="printed">Telegramm aus <tei:note type="editorial" place="inline"
     >[blank]</tei:note></tei:ab>

   <tei:note type="editorial">top-right panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Die Telegraphenverwaltung übernimmt hinsichtlich der ihr
    zur Beförderung oder Bestellung übergebenen Telegramme keine wie immer geartete
    Verantwortung.</tei:ab>

   <tei:ab type="formpart" rend="printed">Aufgenommen von ... auf Ltg. Nr. ... am ... 191 ... um ...
    Uhr ... M. ... Mitt durch <tei:handShift new="#unk"/><tei:seg type="entry" rend="ms"
      ><tei:unclear reason="reading">Cophok</tei:unclear></tei:seg></tei:ab>

   <tei:note type="editorial">middle panel:</tei:note>

   <tei:ab type="formpart" rend="printed"><tei:handShift new="#HS"/><tei:seg type="entry" rend="ms"
     >Wien 49 </tei:seg> Nr. <tei:seg type="entry" rend="ms">139</tei:seg> Taxw. <tei:seg
     type="entry" rend="ms">22</tei:seg> (W .... Ch ...) aufgegeben am <tei:seg type="entry"
     rend="ms">24/6</tei:seg> 191<tei:seg type="entry" rend="ms">12</tei:seg> um ... Uhr 15 M.
     <tei:seg type="entry" rend="ms">02</tei:seg> Mittag.</tei:ab>

   <tei:note type="editorial">bottom panel:</tei:note>

   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms"><tei:rs type="person" key="entity-000484">Kretzschmar</tei:rs>
     ist schon da aber wo bleibt das soeben erschienene Werk<tei:note xml:id="r0001-fn01"
      type="editorial" place="foot"><tei:bibl><tei:author>Heinrich Schenker</tei:author>, <tei:rs
        type="work" key="entity-001731"><tei:title level="m">Beethovens Neunte
        Sinfonie</tei:title></tei:rs> (Vienna: UE, 1912)</tei:bibl>, which had gone into production
      on June 11 and was released on July 2, 1912 (UE Verlagsdruckbuch UE 2540). It is unclear to
      which of Hermann Kretzschmar’s publications Schenker is referring.</tei:note></tei:seg>
   </tei:ab>

   <tei:closer>
    <tei:salute rend="indent-30"><tei:handShift new="#HS"/>Gruss</tei:salute>
    <tei:signed rend="indent-50">Schenker</tei:signed><tei:lb/>
    <tei:add place="inline"><tei:handShift new="#stamp"/>WIEN 49 — initialled
      "<tei:unclear>FVK</tei:unclear>"</tei:add><tei:lb/>
    <tei:fw type="footer">D. S. Nr. 759. (1.1910.)</tei:fw>
   </tei:closer>

  </tei:div>


  <tei:div type="translation">

   <tei:note type="editorial">Roman type = printed; Italic type = handwritten</tei:note>
   <tei:lb/>
   <tei:note type="editorial">top-left panel:</tei:note>

   <tei:ab type="formpart" rend="printed">The dates in the official receipt of the telegram prepared
    using the teletype mean: 1. the name of the issuing officer; 2. the issue number, 3. the number
    of words (perhaps in form of a fraction[?]), 4. the day of the week, and 5. the hour and minute
    of issue.</tei:ab>

   <tei:ab type="formpart" rend="printed">Official instructions:</tei:ab>

   <tei:note type="editorial">top-center panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Category: <tei:handShift new="#unk"/><tei:seg type="entry"
     rend="ms">10</tei:seg> – Receipt number: <tei:handShift new="#unk"/><tei:seg type="entry"
     rend="ms">139/49</tei:seg></tei:ab>

   <tei:ab type="formpart" rend="printed">
    <tei:address>
     <tei:addrLine>
      <tei:seg type="entry" rend="ms"><tei:handShift new="#HS"/>Director Hertzka</tei:seg>
     </tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="ms">I, Reichrathstraße 9</tei:seg></tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="stamp">Universal Edition</tei:seg></tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="ms">Vienna</tei:seg></tei:addrLine>
    </tei:address>
   </tei:ab>

   <tei:ab type="formpart" rend="printed">Telegramm from <tei:note type="editorial" place="inline"
     >[blank]</tei:note></tei:ab>

   <tei:note type="editorial">top-right panel:</tei:note>

   <tei:ab type="formpart" rend="printed">The telegram administration accepts no responsibility
    whatsoever for the telegrams given to them for transmission or delivery.</tei:ab>

   <tei:ab type="formpart" rend="printed">Received from ... on line No. ... At ... 191 ... at ...
    Hour ... Min. ... Midday via <tei:handShift new="#unk"/><tei:seg type="entry" rend="ms"
      ><tei:unclear reason="reading">Cophok</tei:unclear></tei:seg></tei:ab>

   <tei:note type="editorial">middle panel:</tei:note>

   <tei:ab type="formpart" rend="printed"><tei:handShift new="#HS"/><tei:seg type="entry" rend="ms"
     >Vienna 49 </tei:seg> No. <tei:seg type="entry" rend="ms">139</tei:seg> Est. value: <tei:seg
     type="entry" rend="ms">22</tei:seg> (val .... Ch ...) Sent on <tei:seg type="entry" rend="ms"
     >June 24,</tei:seg> 191<tei:seg type="entry" rend="ms">12</tei:seg> at ... Hour 15 Min.
     <tei:seg type="entry" rend="ms">02</tei:seg> Midday.</tei:ab>

   <tei:note type="editorial">bottom panel:</tei:note>

   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms"><tei:rs type="person" key="entity-000484">Kretzschmar</tei:rs>
     is already out there, but where is the work that has just appeared<tei:ptr
      corresp="#r0001-fn01" type="footnote"/> languishing<tei:supplied>?</tei:supplied></tei:seg>
   </tei:ab>

   <tei:closer>
    <tei:salute rend="indent-30"><tei:handShift new="#HS"/>Greetings</tei:salute>
    <tei:signed rend="indent-50">Schenker</tei:signed><tei:lb/>
    <tei:add place="inline"><tei:handShift new="#stamp"/>VIENNA 49 — initialled
      "<tei:unclear>FVK</tei:unclear>"</tei:add><tei:lb/>
    <tei:fw type="footer">D. S. Nr. 759. (1.1910.)</tei:fw>
   </tei:closer>

  </tei:div>
 </sdo:record>
