      <tei:rs role="correspondence" type="correspondence" key="entity-006164">WeinbergerJ ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003332">Calling card</tei:rs>



      <dc:title>Handwritten calling card from Weinberger to Schenker, undated</dc:title>

      <dcterms:created>1899-11-01T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2016-02-15</dcterms:dateSubmitted>
      <dc:description>Weinberger offers his services to Schenker.</dc:description>
      <dc:subject> </dc:subject>




      <marcrel:correspondent key="entity-006149">WeinbergerCo</marcrel:correspondent>
      <marcrel:correspondent key="entity-000946">WeinbergerJ</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">printed in Jugendstil lettering embedded within
          decorative Secession-style borders:</tei:note><tei:lb/>
        <tei:persName><tei:handShift new="#printed"/><tei:rs key="entity-000946" type="person">JOSEF
            WEINBERGER</tei:rs></tei:persName><tei:lb/>
        <tei:salute><tei:handShift new="#JW"/>Sehr verehrter Herr <tei:rs type="person"
            key="entity-000632">Doctor</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p rend="first">Bis zum kommenden Samstag den 6<tei:hi rend="supralinear">ten</tei:hi>
        bleibe ich unter allen Umständen wieder in <tei:rs key="entity-002484" type="place"
          >Wien</tei:rs> –. Ich stehe in diesen Tagen gerne zu Ihren Dienste.</tei:p>

      <tei:closer>
        <tei:salute>In Hochachtung</tei:salute>
        <tei:salute>ganz ergeben</tei:salute><tei:lb/>
        <tei:note type="editorial" place="inline">unsigned</tei:note>
      </tei:closer>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">printed in Jugendstil lettering embedded within
          decorative Secession-style borders:</tei:note><tei:lb/>
        <tei:persName><tei:handShift new="#printed"/><tei:rs key="entity-000946" type="person">JOSEF
            WEINBERGER</tei:rs></tei:persName><tei:lb/>
        <tei:salute><tei:handShift new="#JW"/>Revered Dr. <tei:supplied reason="identity"><tei:rs
              type="person" key="entity-000632">Schenker</tei:rs></tei:supplied>,</tei:salute>
      </tei:opener>

      <tei:p rend="first">Come what may, I am in <tei:rs key="entity-002484" type="place"
          >Vienna</tei:rs> until this Saturday, the 6th.— I shall be happy to be at your
        service.</tei:p>

      <tei:closer>
        <tei:salute>With kind regards,</tei:salute>
        <tei:salute><tei:supplied reason="clarity">Your</tei:supplied> most devoted</tei:salute><tei:lb/>
        <tei:note type="editorial" place="inline">unsigned</tei:note>
      </tei:closer>


    </tei:div>
