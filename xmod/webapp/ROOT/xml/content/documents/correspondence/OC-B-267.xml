  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-005575">KochL ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">letter</tei:rs>

      <dc:title>Typewritten letter from Louis Koch to Schenker, dated February 27, 1920</dc:title>
      <dcterms:created>1920-02-27T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2015-02-16</dcterms:dateSubmitted>
      <dc:description>Koch is sending photographs separately.</dc:description>
      <dc:subject>Photographie; photograph; Photograph; photographer; Rechnung; invoice</dc:subject>



      <marcrel:correspondent key="entity-000454">Louis Koch</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:byline><tei:handShift new="#TPfa"/>i/Fa.<tei:note xml:id="r0001-fn01" type="editorial"
            place="foot">Typist's reference, identity unknown.</tei:note></tei:byline>
        <tei:dateline><tei:handShift new="#printed"/>FRANKFURT A. M. <tei:handShift new="#TPfa"
            /><tei:date when="1920-02-27">27. Febr. 1920</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine>Louis Koch</tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/>ROBERT KOCH</tei:addrLine>
          <tei:addrLine>HOF-JUWELIER</tei:addrLine>
          <tei:addrLine>SR. MAJESTÄT DES KAISERS U. KÖNIGS</tei:addrLine>
          <tei:addrLine>FRANKFURT A. M.</tei:addrLine>
          <tei:addrLine>TELEFON HANSA, 848 U. 6408</tei:addrLine>
          <tei:addrLine>BADEN-BADEN</tei:addrLine>
          <tei:addrLine>TELEFON 205</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPfa"/>Hochwohlgeboren Herrn <tei:hi rend="sperr">Dr.
              H. Schenker,</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="underline"><tei:hi rend="sperr"
                >Wien.</tei:hi></tei:hi></tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute rend="indent-20">Sehr geehrter <tei:rs key="entity-000632" type="person"
            >Herr</tei:rs>!<tei:note xml:id="r0001-fn02" type="editorial" place="foot">Receipt of
            this letter is recorded in Schenker's diary at OJ 3/1, pp. 2218–2219, March 4, 1920:
            "Von Koch, Frankfurt, Autographe u. dazu Brief mit Ankündigung der Rechnung des
            Photographen." ("From Koch, Frankfurt, the autograph manuscript, together with letter
            with notice of photographer's bill.")</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Erst heute bin ich in der Lage, Ihnen die gewünschten Photographien von
          <tei:rs key="entity-001201" type="composition"><tei:title>op.
          101</tei:title></tei:rs><tei:note xml:id="r0001-fn03" type="editorial" place="foot">The
          photographs supplied by Koch do not appear to be extant.</tei:note> separat in 2
        Einschreiben übersenden zu können und hoffe, Ihnen damit dienlich zu sein. Die Rechnung wird
        Ihnen der Photograph direkt zugehen lassen.<tei:note xml:id="r0001-fn04" type="editorial"
          place="foot">See <tei:ref type="correspondence" cRef="OC-B-261">OC B/261</tei:ref> and <tei:ref type="correspondence" cRef="OC-B-260">OC B/260</tei:ref> (respectively March 8 and 9, 1920).</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">Ich empfehle mich Ihnen</tei:salute>
        <tei:salute rend="indent-40">mit vorzüglicher Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#LKo"/><tei:rs key="entity-000454"
            type="person">Louis Koch</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:byline><tei:handShift new="#TPfa"/>i/Fa.<tei:ptr corresp="#r0001-fn01" type="footnote"
          /></tei:byline>
        <tei:dateline><tei:handShift new="#printed"/>FRANKFURT AM MAIN <tei:handShift new="#TPfa"
            /><tei:date when="1920-02-27">February 27, 1920</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine>Louis Koch</tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/>ROBERT KOCH</tei:addrLine>
          <tei:addrLine>COURT JEWELLER</tei:addrLine>
          <tei:addrLine>TO HIS MAJESTY THE KAISER AND KING</tei:addrLine>
          <tei:addrLine>FRANKFURT AM MAIN</tei:addrLine>
          <tei:addrLine>TELEFON HANSA, 848 and 6408</tei:addrLine>
          <tei:addrLine>BADEN-BADEN</tei:addrLine>
          <tei:addrLine>TELEPHONE 205</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:hi rend="sperr"><tei:handShift new="#TPfa"/>Dr. H. Schenker,</tei:hi>
            Esq.,</tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="underline"><tei:hi rend="sperr"
                >Vienna</tei:hi></tei:hi></tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute rend="indent-20">Dear <tei:rs key="entity-000632" type="person"
            >Sir</tei:rs>,<tei:ptr corresp="#r0001-fn02" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">Only today am I in a position to send you the requested photographs of
          <tei:rs key="entity-001201" type="composition"><tei:title>Op.
          101</tei:title></tei:rs>,<tei:ptr corresp="#r0001-fn03" type="footnote"/> in two separate
        registered packages, and hope to be of service to you thereby. The photographer will have
        the invoice sent to you direct.<tei:ptr corresp="#r0001-fn04" type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">With my compliments</tei:salute>
        <tei:salute rend="indent-40"><tei:supplied reason="clarity">and</tei:supplied> with kind
          regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#LKo"/><tei:rs key="entity-000454"
            type="person">Louis Koch</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>
