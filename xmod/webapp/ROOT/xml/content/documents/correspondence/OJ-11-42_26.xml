      <tei:rs role="correspondence" type="correspondence" key="entity-005540">ZukunftD ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Handwritten postcard from Maximilian Harden to Schenker, dated March 27,
        1896</dc:title>
      <dcterms:created>1896-03-27T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-12-12</dcterms:dateSubmitted>
      <dc:description>Harden welcomes Schenker's submission of a review of Karl Goldmark's Das
        Heimchen am Herd.</dc:description>
      <dc:subject/>



      <marcrel:correspondent key="entity-000333">Maximilian Harden</marcrel:correspondent>
      <marcrel:correspondent key="entity-002566">Die Zukunft</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Deutsche
          Reichspost</tei:fw>
        <tei:fw type="header" rend="center">Postkarte</tei:fw>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>An: <tei:handShift new="#MH"/>Herrn Dr. Heinr. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>in <tei:handShift
                new="#MH"/>Wien</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>Wohnung <tei:handShift
                new="#MH"/>III Traungasse 1</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| BERLIN W. | 27.3.96.5-6N. | 9 ||</tei:fw>
          <tei:fw type="postmark">|| WIEN 3/3 | 49 | BESTELLT | 4.11.95 | 6-7N. ||</tei:fw><tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/><tei:lb/>
        </tei:fw>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>DIE ZUKUNFT.</tei:addrLine>
          <tei:addrLine>HERAUSGEBER: MAXIMILIAN HARDEN.</tei:addrLine>
          <tei:addrLine rend="indent-70">BERLIN W. 9, <tei:handShift new="#MH"/><tei:date
              when="1896-03-27">27 3 <tei:handShift new="#printed"/><tei:supplied reason="clarity"
                >18</tei:supplied>9<tei:handShift new="#MH"/>6</tei:date></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:handShift new="#printed"/>8 KÖNIGIN
            AUGUSTASTR.</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute><tei:handShift new="#MH"/>Sehr geehrter <tei:rs key="entity-000632"
            type="person">Herr</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p>„<tei:title>Heimchen</tei:title>“<tei:note xml:id="r0001-fn01" type="editorial"
          place="foot"><tei:title level="m">Das Heimchen am Herd</tei:title>, opera by Karl
          Goldmark, first performed Vienna, March 21, 1896. Schenker's article
              <tei:bibl><tei:title level="a">Das Heimchen am Herd (Karl Goldmark)</tei:title>
            appeared in <tei:title level="j">Die Zukunft</tei:title> 15 (April 18, 1896),
            132‒34</tei:bibl>, and is reprinted in <tei:bibl><tei:editor>Hellmut
              Federhofer</tei:editor>, <tei:title level="m">Heinrich Schenker als Essayist und
              Kritiker</tei:title> (Hildesheim: Georg Olms Verlag, 1990), pp.
          185–88</tei:bibl>.</tei:note> wird sehr willkommen sein.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">Mit höflichem Dank u. Gruß</tei:salute>
        <tei:salute rend="indent-70">Ihr ergebener</tei:salute>
        <tei:signed rend="indent-80"><tei:rs key="entity-000333" type="person">
          Harden</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>German Imperial
          Post</tei:fw>
        <tei:fw type="header" rend="center">Postcard</tei:fw>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>To: <tei:handShift new="#MH"/>Dr. Heinr. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>in: <tei:handShift
                new="#MH"/>Vienna III</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>Address: <tei:handShift
                new="#MH"/>Traungasse 1</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| BERLIN W. | 27.3.96.5-6 P.M. | 9 ||</tei:fw>
          <tei:fw type="postmark">|| VIENNA 3/3 | 49 | DELIVERED | 4.11.95 | 6-7 P.M. ||</tei:fw><tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/><tei:lb/>
        </tei:fw>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>DIE ZUKUNFT</tei:addrLine>
          <tei:addrLine>EDITOR: MAXIMILIAN HARDEN</tei:addrLine>
          <tei:addrLine rend="indent-70">BERLIN W. 9, <tei:handShift new="#MH"/><tei:date
              when="1896-03-27">March 27, <tei:handShift new="#printed"/><tei:supplied
                reason="clarity">18</tei:supplied>9<tei:handShift new="#MH"
            />6</tei:date></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:handShift new="#printed"/>8 KÖNIGIN
            AUGUSTASTRAßE</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute><tei:handShift new="#MH"/>Dear <tei:rs key="entity-000632" type="person"
            >Sir</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p><tei:title level="m">Heimchen</tei:title><tei:ptr corresp="#r0001-fn01" type="footnote"
        /> will be very welcome.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">With respectful thanks and greetings,</tei:salute>
        <tei:salute rend="indent-70">Yours truly,</tei:salute>
        <tei:signed rend="indent-80"><tei:rs key="entity-000333" type="person">
          Harden</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>
