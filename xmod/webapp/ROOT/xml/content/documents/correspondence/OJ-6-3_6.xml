      </tei:figure>
      <tei:lb/>
      <tei:addrLine rend="indent-10">VI. Gumpendorferstr. 124</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| WIEN 3/3 | 49 | 8. 5. 99 | 6–7 V ||</tei:fw>
