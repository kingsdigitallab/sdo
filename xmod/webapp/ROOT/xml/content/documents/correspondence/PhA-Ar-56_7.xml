  <sdo:record ID="r0001">
    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-003319">Kromer ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-003320">Kromer ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-003315">Photogrammarchiv ~
