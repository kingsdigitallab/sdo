      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002751">Picture
        postcard</tei:rs>

      <dc:title>Handwritten picture postcard from Heinrich to Wilhelm Schenker, dated September 5,
        1934</dc:title>

      <dcterms:created>1934-09-05T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2024-11-13</dcterms:dateSubmitted>
      <dc:description>Heinrich sends his elder brother greetings from Salzburg.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000632">HSchenker</marcrel:correspondent>
      <marcrel:recipient key="entity-000775">Wilhelm Schenker</marcrel:recipient>

    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:note type="editorial" place="inline">picture: curving river, with city on either bank,
          and mountains behind: captioned on recto: “Salzburg von Mülln”</tei:note><tei:lb/>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">Absender:</tei:supplied>
              <tei:handShift new="#HS"/>Schenker, Wien III</tei:addrLine>
            <tei:addrLine>Keilgasse 8</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied> H D<tei:hi
                rend="supralinear">r.</tei:hi> Wilhelm Schenker</tei:addrLine>
            <tei:addrLine rend="ident-10">St. Bernhard b/ Horn</tei:addrLine>
            <tei:addrLine>Nd: Öst.</tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| SALZBURG || 5.IX.34 17 | * ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:salute><tei:handShift new="#HS"/>Mein lieber <tei:rs type="person" key="entity-000775"
            >Wilhelm</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">
            Schenker’s diary entry for September 5, 1934, records: “ Im Hotel schreiben wir vor
            Tisch Ansichtskarten an Weil, Rothberger, Wilhelm” (“In the hotel before lunch, we write
            picture postcards to Weil, Rothberger, and Wilhelm.”).</tei:note></tei:salute>
      </tei:opener>

      <tei:p>Unterwegs in <tei:rs type="place" key="entity-002578">Salzburg</tei:rs><tei:note
          xml:id="r0001-fn02" type="editorial" place="foot">Heinrich and Jeanette were on their way
          from Böckstein to Vienna, arriving home the next day.</tei:note> senden wir Dir u. der l.
          <tei:rs type="person" key="entity-008450">Frau <tei:hi rend="lateinschr"
          >Marie</tei:hi></tei:rs> viele Küsse u. Grüße!</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Die Deinen</tei:salute>
        <tei:signed rend="indent-70"><tei:rs type="person" key="entity-000632"
          >Heinrich</tei:rs></tei:signed> u. <tei:rs type="person" key="entity-000771"
          >LieLie</tei:rs><tei:lb/>
        <tei:dateline><tei:date when="1934-09-05">5. 9. 34</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:note type="editorial" place="inline">picture: curving river, with city on either bank,
          and mountains behind: captioned on recto: “Salzburg from Mülln”</tei:note><tei:lb/>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">From:</tei:supplied>
              <tei:handShift new="#HS"/>Schenker, Vienna III</tei:addrLine>
            <tei:addrLine>Keilgasse 8</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied> Dr. Wilhelm
              Schenker</tei:addrLine>
            <tei:addrLine rend="ident-10">St. Bernhard, near Horn</tei:addrLine>
            <tei:addrLine>Lower Austria</tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| SALZBURG || 5.IX.34 17 | * ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:salute><tei:handShift new="#HS"/>My dear <tei:rs type="person" key="entity-000775"
            >Wilhelm</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p>In transit in <tei:rs type="place" key="entity-002578">Salzburg</tei:rs>,<tei:ptr
          corresp="#r0001-fn02" type="footnote"/> we send you and your dear <tei:rs type="person"
          key="entity-008450">wife Marie</tei:rs> abundant kisses and greetings!</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Your</tei:salute>
        <tei:signed rend="indent-70"><tei:rs type="person" key="entity-000632"
          >Heinrich</tei:rs></tei:signed> and <tei:rs type="person" key="entity-000771"
          >LieLie</tei:rs><tei:lb/>
        <tei:dateline><tei:date when="1934-09-05">September 5, 1934</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>

