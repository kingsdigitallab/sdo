      <dc:title>Postal receipt from Schenker to Rinn, dated January 23, 1930</dc:title>
      <dcterms:created>1930-01-23T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2018-02-21</dcterms:dateSubmitted>
      <dc:description>Schenker sends the Hammer portrait to Rinn.</dc:description>
      <dc:subject>Bildchen; portrait</dc:subject>


      <marcrel:correspondent key="entity-006727">Der Kunstwart</marcrel:correspondent>
      <marcrel:correspondent key="entity-000717">Hermann Rinn</marcrel:correspondent>
