 <sdo:record ID="r0001">

  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-005502"><tei:name
     key="entity-005596" type="correspondence">AltmannW ~ HSchenker</tei:name></tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

   <dc:title>Handwritten postcard from Wilhelm Altmann to Schenker, dated April 29, 1933</dc:title>

   <dcterms:created>1933-04-29T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2016-06-15</dcterms:dateSubmitted>
   <dc:description>Altmann thanks Schenker for sending a copy of his Brahms Octaven und Quinten
    article.</dc:description>
   <dc:subject>Zusendung; sending; Veröffentlichung; publication</dc:subject>

   <marcrel:correspondent key="entity-000013">Wilhelm Altmann</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:fw type="header" rend="center">
     <tei:handShift new="#printed"/><tei:hi rend="bold">Postkarte</tei:hi></tei:fw>
    <tei:lb/>
    <tei:fw type="envelope">
     <tei:fw type="byeline">Goethekarte v. K. Franke, Eisenach<tei:lb/><tei:note type="editorial"
       place="inline">printed vertically: </tei:note>Druck: Laue, Eisenach</tei:fw><tei:lb/><tei:lb/>
     <tei:note type="editorial" place="inline">oval stamp, top LH corner:</tei:note>
     <tei:address>
      <tei:addrLine><tei:handShift new="#stamp"/>Prof. Dr. Wilh. Altmann</tei:addrLine>
      <tei:addrLine>* Berlin-Friedenau *</tei:addrLine>
      <tei:addrLine>Lauterstr. 38<tei:hi rend="supralinear">II</tei:hi></tei:addrLine>
     </tei:address>
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity"><tei:handShift new="#printed"/>An</tei:supplied>
       <tei:handShift new="#WA"/>Herrn Professor Dr.<tei:hi rend="underline">H.
       Schenker</tei:hi></tei:addrLine>
      <tei:addrLine rend="indent-10">Wien III</tei:addrLine>
      <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| BERLIN-FRIEDENAU 1 | 29. 4. 33 16–17 | 2 ||</tei:fw><tei:lb/>
     <tei:fw type="stamp">Benutzt die Luftpost</tei:fw><tei:lb/>
     <tei:lb/>
    </tei:fw>
    <tei:milestone unit="folio" n="verso"/><tei:lb/>
    <tei:note type="editorial" place="inline">half-portrait of Goethe, facing L, ‘1832 + 1932’
     underneath in ornamental banner</tei:note><tei:lb/>
    <tei:note type="editorial" place="inline">Gothic type, enlarged blue initial:</tei:note>
    <tei:byline>Wem die Natur ihr offenbares Geheimnis zu enthüllen anfängt, der empfindet eine
     unwiderstehliche Sehnsucht nach ihrer würdigsten Auslegerin der Kunst.<tei:note
      xml:id="r0001-fn01" type="editorial" place="foot"><tei:bibl><tei:author>Johann Wolfgang von
        Goethe</tei:author>, <tei:title level="m">Ueber Kunst und Alterthum</tei:title> (Stuttgart:
       Cottaische Buchhandlung, 1823), vol. 4, Heft 1, p. 43. Translation
      original.</tei:bibl></tei:note></tei:byline><tei:lb/>
    <tei:dateline><tei:date when="1933-04-29">29. 4. 33</tei:date></tei:dateline>
    <tei:salute><tei:handShift new="#WA"/>Hochverehrter Herr <tei:rs key="entity-000632"
      type="person">Professor</tei:rs>.<tei:note xml:id="r0001-fn02" type="editorial" place="foot"
      >Receipt of this postcard is recorded in Schenker's diary at OJ 4/6, p. 3830, May 2, 1933:
      "Von Altmann (Ansichtsk.) Dank für Brahms." ("From Altmann (picture postcard): thanks for
      Brahms.").</tei:note></tei:salute>
   </tei:opener>

   <tei:p>Verbindlichsten Dank für die Zusendung Ihrer hochinteressanten <tei:rs key="entity-003331"
     type="work">Brahms-Veröffentlichung</tei:rs><tei:note xml:id="r0001-fn03" type="editorial"
     place="foot">Schenker’s diary appears not to record his sending Altmann a copy of this
     publication.</tei:note> und herzlichste Grüsse auch an Ihre Frau <tei:rs key="entity-000771"
     type="person">Gemahlin</tei:rs>.</tei:p>

   <tei:closer>
    <tei:salute rend="indent-40">Ihr sehr ergebener</tei:salute>
    <tei:signed rend="indent-50"><tei:rs key="entity-000013" type="person">Wilh
     Altmann</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>


  <tei:div type="translation">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:fw type="header" rend="center">
     <tei:handShift new="#printed"/><tei:hi rend="bold">Postcard</tei:hi></tei:fw>
    <tei:lb/>
    <tei:fw type="envelope">
     <tei:fw type="byeline">Goethe card by K. Franke, Eisenach<tei:lb/><tei:note type="editorial"
       place="inline">printed vertically: </tei:note>printed by: Laue, Eisenach</tei:fw><tei:lb/><tei:lb/>
     <tei:note type="editorial" place="inline">oval stamp, top LH corner:</tei:note>
     <tei:address>
      <tei:addrLine><tei:handShift new="#stamp"/>Prof. Wilhelm Altmann</tei:addrLine>
      <tei:addrLine>* Berlin-Friedenau *</tei:addrLine>
      <tei:addrLine>Lauterstrasse 38<tei:hi rend="supralinear">II</tei:hi></tei:addrLine>
     </tei:address>
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity"><tei:handShift new="#printed"/>To: </tei:supplied>
       <tei:handShift new="#WA"/>Professor <tei:hi rend="underline">H.
       Schenker</tei:hi></tei:addrLine>
      <tei:addrLine rend="indent-10">Vienna III</tei:addrLine>
      <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| BERLIN-FRIEDENAU 1 | 29. 4. 33 16–17 | 2 ||</tei:fw><tei:lb/>
     <tei:fw type="stamp">Use airmail</tei:fw><tei:lb/>
     <tei:lb/>
    </tei:fw>
    <tei:milestone unit="folio" n="verso"/><tei:lb/>
    <tei:note type="editorial" place="inline">half-portrait of Goethe, facing L, ‘1832 + 1932’
     underneath in ornamental banner</tei:note><tei:lb/>
    <tei:note type="editorial" place="inline">Gothic type, enlarged blue initial:</tei:note>
    <tei:byline>The person to whom Nature begins to divulge her manifest secret conceives an
     irrepressible longing for her most worthy interpreter, Art.<tei:ptr corresp="#r0001-fn01"
      type="footnote"/></tei:byline><tei:lb/>
    <tei:dateline><tei:date when="1933-04-29">April 29, 1933</tei:date></tei:dateline>
    <tei:salute><tei:handShift new="#WA"/>Highly revered <tei:rs key="entity-000632" type="person"
      >Professor</tei:rs>,<tei:ptr corresp="#r0001-fn02" type="footnote"/></tei:salute>
   </tei:opener>

   <tei:p>My warmest thanks for sending your highly interesting <tei:rs key="entity-003331"
     type="work">Brahms publication</tei:rs>,<tei:ptr corresp="#r0001-fn03" type="footnote"/> and my
    most cordial greetings, to your <tei:rs key="entity-000771" type="person">wife</tei:rs>
    also</tei:p>

   <tei:closer>
    <tei:salute rend="indent-40">Your very devoted</tei:salute>
    <tei:signed rend="indent-50"><tei:rs key="entity-000013" type="person">Wilhelm
     Altmann</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>

