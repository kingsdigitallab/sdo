  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs type="correspondence" role="correspondence" key="entity-005439">Waldheim-Eberle ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002750">Note</tei:rs>

      <dc:title>Handwritten note from Waldheim to Schenker, dated April 19, 1932</dc:title>
      <dcterms:created>1932-04-19T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-13</dcterms:dateSubmitted>

      <dc:description>Waldheim-Eberle gives number of sheets, format, and price-per-print-run for
        Fünf Urlinie-Tafeln.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002402">Waldheim-Eberle</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">


      <tei:milestone unit="folio" n="recto"/>
      <tei:p><tei:handShift new="#unkWE1"/>Durch noch erhaltenen Lichtpausen</tei:p>

      <tei:p>
        <tei:table rows="3" cols="3">
          <tei:row n="1">
            <tei:cell role="data">in Bogen</tei:cell>
            <tei:cell role="data">4 u. 1 Seite</tei:cell>
            <tei:cell role="data"/>
          </tei:row>
          <tei:row n="2">
            <tei:cell role="data"/>
            <tei:cell role="data">1 u. 4 Seiten</tei:cell>
            <tei:cell role="data">= 12 Seiten</tei:cell>
          </tei:row>
          <tei:row n="3">
            <tei:cell role="data"/>
            <tei:cell role="data">1 u. 3 Seiten</tei:cell>
            <tei:cell role="data">im Format 27:34 cm</tei:cell>
          </tei:row>
        </tei:table>
      </tei:p>

      <tei:p>auf holzfreiem Papier mit Umschlag aus farbigem Karton 1seitig <tei:unclear
          reason="original_unclear">bidmelz</tei:unclear>, gefalzt und nicht geheftet<tei:note
          xml:id="r0001-fn01" type="editorial" place="foot">Schenker’s diary for April 19, 1932
          records: “Nach der Jause bei Piller u. Waldheim einen Kostenvoranschlag geholt;” (“After
          teatime, at Piller’s and Waldheim’s, cost estimates collected;”). — On April 14, 1932,
            <tei:ref type="correspondence" cRef="OC-54-363">OC 54/363</tei:ref>, Otto Erich Deutsch
          suggests to Schenker that Waldheim might be brought in for the <tei:rs type="work"
            key="entity-001755"><tei:title level="m">Fünf Urlinie-Tafeln</tei:title></tei:rs>, which
          is clearly what this set of figures concerns.</tei:note></tei:p>

      <tei:p>
        <tei:table rows="3" cols="3">
          <tei:row n="1">
            <tei:cell role="data">in Auflage</tei:cell>
            <tei:cell role="data">500</tei:cell>
            <tei:cell role="data">S. 260.-</tei:cell>
          </tei:row>
          <tei:row n="2">
            <tei:cell role="data">in Auflage</tei:cell>
            <tei:cell role="data">600</tei:cell>
            <tei:cell role="data">S. 285.-</tei:cell>
          </tei:row>
          <tei:row n="3">
            <tei:cell role="data">jedes weitere</tei:cell>
            <tei:cell role="data">100</tei:cell>
            <tei:cell role="data">S. 25.- mehr</tei:cell>
          </tei:row>
        </tei:table>
      </tei:p>



      <tei:closer>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-80"><tei:handShift new="#unkWE2"/><tei:note type="editorial"
            place="inline">illegible initials</tei:note></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Musikabteilung</tei:signed>
        <tei:dateline><tei:handShift new="#unkWE1"/>XXII/173, <tei:date when="1932-04-19">19./4.
            1932</tei:date></tei:dateline>
      </tei:closer>

      <tei:postscript>

        <tei:milestone unit="folio" n="verso"/>

        <tei:p>
          <tei:list type="litany" rend="simple">
            <tei:item><tei:handShift new="#unkWE1"/>1000 Expl. <tei:hi rend="supralinear"
              >m</tei:hi>/24 Seiten</tei:item>
            <tei:item>im Format 27:34</tei:item>
            <tei:item>Preis S770.–</tei:item>
          </tei:list>
        </tei:p>
      </tei:postscript>

      <tei:postscript>
        <tei:note type="editorial" place="inline">bottom of page:</tei:note>
        <tei:lb/>
        <tei:p><tei:handShift new="#HS"/>160 <tei:supplied>+</tei:supplied> 125
            <tei:supplied>=</tei:supplied> 385 <tei:supplied>x 2 =</tei:supplied> 770</tei:p>
      </tei:postscript>

    </tei:div>



    <tei:div type="translation">

      <tei:milestone unit="folio" n="recto"/>

      <tei:p><tei:handShift new="#unkWE1"/>Using in-stock blueprints:</tei:p>

      <tei:p>
        <tei:table rows="3" cols="3">
          <tei:row n="1">
            <tei:cell role="data">in gatherings</tei:cell>
            <tei:cell role="data">4 and 1 page</tei:cell>
            <tei:cell role="data"/>
          </tei:row>
          <tei:row n="2">
            <tei:cell role="data"/>
            <tei:cell role="data">1 and 4 pages</tei:cell>
            <tei:cell role="data">= 12 pages</tei:cell>
          </tei:row>
          <tei:row n="3">
            <tei:cell role="data"/>
            <tei:cell role="data">1 u. 3 pages</tei:cell>
            <tei:cell role="data">in format 27 x 34 cm</tei:cell>
          </tei:row>
        </tei:table>
      </tei:p>

      <tei:p>on woodfree paper with wrapper of card one-sided <tei:unclear reason="original_unclear"
          >bidmelz</tei:unclear>, folded and not sewn.<tei:ptr corresp="#r0001-fn01" type="footnote"
        /></tei:p>

      <tei:p>
        <tei:table rows="3" cols="3">
          <tei:row n="1">
            <tei:cell role="data">in print-run of</tei:cell>
            <tei:cell role="data">500</tei:cell>
            <tei:cell role="data">S. 260.-</tei:cell>
          </tei:row>
          <tei:row n="2">
            <tei:cell role="data">in print-run of</tei:cell>
            <tei:cell role="data">600</tei:cell>
            <tei:cell role="data">S. 285.-</tei:cell>
          </tei:row>
          <tei:row n="3">
            <tei:cell role="data">each additional</tei:cell>
            <tei:cell role="data">100</tei:cell>
            <tei:cell role="data">S. 25.- more</tei:cell>
          </tei:row>
        </tei:table>
      </tei:p>

      <tei:closer>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-80"><tei:handShift new="#unkWE2"/><tei:note type="editorial"
            place="inline">illegible initials</tei:note></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Music Division</tei:signed>
        <tei:dateline><tei:handShift new="#unkWE1"/>XXII/173, <tei:date when="1932-04-19">April 19,
            1932</tei:date></tei:dateline>
      </tei:closer>

      <tei:postscript>

        <tei:milestone unit="folio" n="verso"/>

        <tei:p>
          <tei:list type="litany" rend="simple">
            <tei:item><tei:handShift new="#unkWE1"/>1,000 copies per 24 pages</tei:item>
            <tei:item>in format 27 x 34 <tei:supplied reason="measure">cms</tei:supplied></tei:item>
            <tei:item>Price S. 770.–</tei:item>
          </tei:list>
        </tei:p>
      </tei:postscript>

      <tei:postscript>
        <tei:note type="editorial" place="inline">bottom of page:</tei:note>
        <tei:lb/>
        <tei:p><tei:handShift new="#HS"/>160 <tei:supplied>+</tei:supplied> 125
            <tei:supplied>=</tei:supplied> 385 <tei:supplied>x 2 =</tei:supplied> 770</tei:p>
      </tei:postscript>

    </tei:div>
  </sdo:record>
