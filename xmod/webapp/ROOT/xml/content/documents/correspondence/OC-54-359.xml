      <dc:title>Postal receipt for consignment from Schenker to Waldheim, postmarked [July 16],
        1932</dc:title>
      <dcterms:created>1932-07-16T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-14</dcterms:dateSubmitted>
      <dc:description>Schenker gives his imprimatur to the whole item, and instructions for
        distribution.</dc:description>
      <dc:subject/>


      <marcrel:correspondent key="entity-0000632">HSchenker</marcrel:correspondent>
      <marcrel:recipient key="entity-002402">Waldheim-Eberle</marcrel:recipient>
    </sdo:itemDesc>


