  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002700">Hertzka ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">postcard</tei:rs>



      <dc:title>Typewritten postcard (book slip) from UE (unidentified sign) to Schenker, dated
        January 14, 1909</dc:title>

      <dcterms:created>1909-01-14T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2016-08-24</dcterms:dateSubmitted>
      <dc:description>UE confirms sending two items to Elise Rudorff.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:correspondent key="entity-000356">Emil Hertzka</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:supplied reason="format">all in decorative rule-frame</tei:supplied>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
            >Bücherzettel.</tei:hi></tei:fw><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:hi rend="italic">An</tei:hi>
            <tei:handShift new="#TPue"/>Wohlgeboren Herrn Professor Dr. Heinrich
            Schenker,</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:handShift new="#printed"/><tei:hi rend="italic"
              >in</tei:hi>
            <tei:handShift new="#TPue"/><tei:hi rend="underline sperr">Wien</tei:hi>
            III</tei:addrLine>
          <tei:addrLine rend="indent-40">Reisnerstr. 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 1/1 WIEN <tei:gap unit="chars" extent="2" reason="illegible"/> |
          14. I. <tei:gap unit="chars" extent="4" reason="illegible"/> | * <tei:gap unit="chars"
            extent="4" reason="illegible"/> * ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:hi rend="underline"><tei:persName>Herrn Prof. Dr. H.
                Schenker</tei:persName></tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-50">
            <tei:hi rend="underline sperr">Wien.</tei:hi></tei:addrLine>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs key="entity-000632" type="person"
            >Doktor</tei:rs><tei:add place="inline"><tei:handShift new="#unk"/>!</tei:add><tei:note
            xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this postcard is not
            recorded in Schenker's diary.</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first"><tei:handShift new="#TPue"/>Wir erlauben uns Ihnen höflichst mitzuteilen,
        dass Ihrem Wunsch gemäss an Frl. Elise Rudorff<tei:note xml:id="r0001-fn02" type="editorial"
          place="foot">Elisabeth Rudorff, daughter of <tei:rs key="entity-000750" type="person"
            >Ernst Rudorff</tei:rs>. A letter from her dated January 11, 1909 (OJ 13/36, [1])
          inquires about an Urtext edition of keyboard works of C. P. E. Bach, to which the present
          letter is undoubtedly an indirect response.</tei:note> 2 Werke unserer Universal Edition
        u.z. U.E. No. 548<tei:note xml:id="r0001-fn03" type="editorial" place="foot">UE548 =
              <tei:bibl><tei:editor>Schenker</tei:editor>, ed., <tei:rs key="entity-001720"
              type="work"><tei:title level="m">Phil. Em. Bach,
            Klavierwerke</tei:title></tei:rs></tei:bibl>.</tei:note> und 936<tei:note
          xml:id="r0001-fn04" type="editorial" place="foot">UE936 =
              <tei:bibl><tei:editor>Schenker</tei:editor>, ed., <tei:rs key="entity-001722"
              type="work"><tei:title level="m">G. F. Händel,
            Orgelkonzerte</tei:title></tei:rs></tei:bibl>, vol.1.</tei:note> abgegangen
        sind.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-10">Wir empfehlen uns Ihnen</tei:salute><tei:lb/><tei:lb/>
        <tei:note type="editorial" place="inline">right side:</tei:note>
        <tei:salute><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:signed><tei:rs key="entity-002408" type="organization">„Universal-Edition“
            A-G.</tei:rs><tei:lb/>(Jos. Aibl Verlag, G.m.b.H.)</tei:signed>
        <tei:signed><tei:handShift new="#unk"/><tei:note type="editorial" place="inline">illegible
            initials</tei:note></tei:signed><tei:lb/><tei:lb/>
        <tei:note type="editorial" place="inline">left side:</tei:note>
        <tei:dateline><tei:handShift new="#printed"/>Wien, am <tei:handShift new="#TPue"/>14./1
            <tei:handShift new="#printed"/>190<tei:handShift new="#TPue"/>9</tei:dateline>
        <tei:address>
          <tei:addrLine><tei:del rend="overstrike">I. Maximilianstraße 11.</tei:del></tei:addrLine>
          <tei:addrLine><tei:del rend="overstrike">(Telefon 4921, lokal u.
            interurban).</tei:del></tei:addrLine>
          <tei:addrLine><tei:handShift new="#stamp"/>I. Wipplingerstrasse 32</tei:addrLine>
        </tei:address>
      </tei:closer>


    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:supplied reason="format">all in decorative rule-frame</tei:supplied>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold">Book
            Slip</tei:hi></tei:fw><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:hi rend="italic">To</tei:hi>
            <tei:handShift new="#TPue"/>Professor Heinrich Schenker, Esq.</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:handShift new="#printed"/><tei:hi rend="italic"
              >in</tei:hi>
            <tei:handShift new="#TPue"/><tei:hi rend="underline sperr">Vienna</tei:hi>
            III</tei:addrLine>
          <tei:addrLine rend="indent-40">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 1/1 VIENNA <tei:gap unit="chars" extent="2" reason="illegible"/>
          | 14. I. <tei:gap unit="chars" extent="4" reason="illegible"/> | * <tei:gap unit="chars"
            extent="4" reason="illegible"/> * ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:hi rend="underline"><tei:persName>Professor H.
              Schenker</tei:persName></tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-50">
            <tei:hi rend="underline sperr">Vienna</tei:hi></tei:addrLine>
        </tei:address>
        <tei:salute>Dear Dr. <tei:supplied reason="identity"><tei:rs key="entity-000632"
              type="person">Schenker</tei:rs></tei:supplied>,<tei:ptr corresp="#r0001-fn01"
            type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first"><tei:handShift new="#TPue"/>We take the liberty of informing you that, in
        accordance with your wishes, two works from our Universal Edition, <tei:hi rend="italic"
          >viz</tei:hi>. Nos. 548<tei:ptr corresp="#r0001-fn03" type="footnote"/> and 936,<tei:ptr
          corresp="#r0001-fn04" type="footnote"/> have been dispatched to Miss Elise
          Rudorff.<tei:ptr corresp="#r0001-fn02" type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-10">We remain,</tei:salute><tei:lb/><tei:lb/>
        <tei:note type="editorial" place="inline">right side:</tei:note>
        <tei:salute><tei:handShift new="#printed"/>With kind regards,</tei:salute>
        <tei:signed><tei:rs key="entity-002408" type="organization">Universal Edition &amp;
            Co.</tei:rs><tei:lb/>(Jos. Aibl Verlag, &amp; Co.)</tei:signed>
        <tei:signed><tei:handShift new="#unk"/><tei:note type="editorial" place="inline">illegible
            initials</tei:note></tei:signed><tei:lb/><tei:lb/>
        <tei:note type="editorial" place="inline">left side:</tei:note>
        <tei:dateline><tei:handShift new="#printed"/>Vienna <tei:handShift new="#TPue"/>January 14,
            <tei:handShift new="#printed"/>190<tei:handShift new="#TPue"/>9</tei:dateline>
        <tei:address>
          <tei:addrLine><tei:del rend="overstrike">I. Maximilianstraße 11</tei:del></tei:addrLine>
          <tei:addrLine><tei:del rend="overstrike">(Telephone 4921, local and
            interurban).</tei:del></tei:addrLine>
          <tei:addrLine><tei:handShift new="#stamp"/>I. Wipplingerstrasse 32</tei:addrLine>
        </tei:address>
      </tei:closer>


    </tei:div>
