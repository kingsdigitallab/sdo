   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002748">Telegram</tei:rs>

   <dc:title>Handwritten telegram from Arnold Weil to Heinrich Schenker, dated June 20,
    [1933]</dc:title>

   <dcterms:created>1933-06-20T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2024-07-23</dcterms:dateSubmitted>
   <dc:description>Arnold sends Heinrich belated birthday greetings.</dc:description>
   <dc:subject/>

   <marcrel:correspondent key="entity-000943">Arnold Weil</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">

   <tei:note type="editorial">Roman type = printed; Italic type = handwritten (Arnold Weil's hand)</tei:note>
   <tei:lb/>
   <tei:note type="editorial">top-left panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Dienstliche Angaben:</tei:ab>

   <tei:note type="editorial">top-center panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Gattung: … <tei:hi rend="bold">Telegramm</tei:hi> Eing-Nr:
     <tei:seg type="entry" rend="ms">16</tei:seg></tei:ab>

   <tei:ab type="formpart" rend="printed">
    <tei:address>
     <tei:addrLine>
      <tei:seg type="entry" rend="ms">Doktor Heinrich</tei:seg>
     </tei:addrLine>
     <tei:addrLine>
      <tei:seg type="entry" rend="ms">Schenker Schloß <tei:sic>Reigenberg</tei:sic></tei:seg>
     </tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="ms">Ilz</tei:seg><tei:note xml:id="r0001-fn01"
       type="editorial" place="foot">Receipt of this telegram is recorded in Heinrich’s diary for
       June 20, 1933: “Von Weil (Telegramm): Gratulation!” (“From Weil (telegram):
       congratulations!”). — 1933 was the only summer in which the Schenkers vacationed in Schloß
       Reigersberg, Ilz, Styria.</tei:note></tei:addrLine>
    </tei:address>
   </tei:ab>

   <tei:note type="editorial">top-right panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Die Telegraphenverwaltung übernimmt hinsichtlich der ihr
    zur Beförderung oder Bestellung übergebenen Telegramme keine wie immer geartete
    Verantwortung.</tei:ab>

   <tei:note type="editorial">middle panel: left:</tei:note>

   <tei:ab type="formpart" rend="printed">Aufgenommen von <tei:seg type="entry" rend="ms"
     >Gs</tei:seg> auf Ltg. Nr.<tei:seg type="entry" rend="ms">1338</tei:seg> am <tei:seg
     type="entry" rend="ms">20/6</tei:seg> 19<tei:seg type="entry" rend="ms">...</tei:seg> um
     <tei:hi rend="ms">14</tei:hi> Uhr <tei:seg type="entry" rend="ms">50</tei:seg> M. durch
     <tei:seg type="entry" rend="ms"><tei:gap reason="illegible"/></tei:seg>
   </tei:ab>

   <tei:note type="editorial">middle panel: center-right:</tei:note>

   <tei:ab type="formpart" rend="printed">Aus <tei:seg type="entry" rend="ms">Ustí Nad
     Labem</tei:seg> Nr. <tei:seg type="entry" rend="ms">148</tei:seg> Worte <tei:seg type="entry"
     rend="ms">16</tei:seg> Aufgegeben am <tei:seg type="entry" rend="ms">20/6</tei:seg> 192 <tei:hi
     rend="ms">...</tei:hi> um <tei:seg type="entry" rend="ms">13</tei:seg> Uhr <tei:seg
     type="entry" rend="ms">30</tei:seg> M.</tei:ab>

   <tei:ab type="formpart" rend="printed">In mittels Typendruckers ausgefertigten Telegrammen
    bedeuten vorstehende Angaben: 1. den Namen des Aufgabeamtes, 2. die Aufgabenummer, 3.die
    Wortzahl (allenfalls in Bruchform), 4. den Monat&amp;tag, 5. die Stunde nun Minute der
    Aufgabe.</tei:ab>

   <tei:note type="editorial">message area:</tei:note>

   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">Allerherzlichste Glückwünsche zum gestrigen Tage <tei:lb/> Gruß
     von <tei:rs type="orgnization" key="entity-000943">Arnold Weil</tei:rs><tei:note
      xml:id="r0001-fn02" type="editorial" place="foot">June 19, 1933 was Schenker’s 65th
      birthday.</tei:note></tei:seg>
   </tei:ab>

   <tei:closer>
    <tei:fw type="footer">D. G. Nr. 769. 1925. Druck der Österreichischen Staatsdruckerei in Wien
     (St.) 19?? 25</tei:fw>
   </tei:closer>

  </tei:div>


  <tei:div type="translation">

   <tei:note type="editorial">Roman type = printed; Italic type = handwritten (Arnold Weil's hand)</tei:note>
   <tei:lb/>
   <tei:note type="editorial">top-left panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Official remarks:</tei:ab>

   <tei:note type="editorial">top-center panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Type: … <tei:hi rend="bold">Telegram</tei:hi> Entry No:
     <tei:seg type="entry" rend="ms">16</tei:seg></tei:ab>

   <tei:ab type="formpart" rend="printed">
    <tei:address>
     <tei:addrLine>
      <tei:seg type="entry" rend="ms">Doctor Heinrich</tei:seg>
     </tei:addrLine>
     <tei:addrLine>
      <tei:seg type="entry" rend="ms">Schenker <tei:sic>Reigenberg</tei:sic> Castle</tei:seg>
     </tei:addrLine>
     <tei:addrLine><tei:seg type="entry" rend="ms">Ilz</tei:seg><tei:ptr corresp="#r0001-fn01"
       type="footnote"/></tei:addrLine>
    </tei:address>
   </tei:ab>

   <tei:note type="editorial">top-right panel:</tei:note>

   <tei:ab type="formpart" rend="printed">The telegram administration accepts no responsibility
    whatsoever for the telegrams givien to them for transmission or delivery.</tei:ab>

   <tei:note type="editorial">middle panel: left:</tei:note>

   <tei:ab type="formpart" rend="printed">Received by <tei:seg type="entry" rend="ms">Gs</tei:seg>
    on line No. <tei:seg type="entry" rend="ms">1338</tei:seg> on <tei:seg type="entry" rend="ms"
     >20/6</tei:seg> 192<tei:seg type="entry" rend="ms">...</tei:seg> at <tei:seg type="entry"
      ><tei:hi rend="ms">14</tei:hi></tei:seg> hours <tei:seg type="entry"><tei:hi rend="ms"
      >50</tei:hi></tei:seg> minutes, by <tei:seg type="entry" rend="ms"><tei:gap reason="illegible"
     /></tei:seg>
   </tei:ab>

   <tei:note type="editorial">middle panel: center-right:</tei:note>

   <tei:ab type="formpart" rend="printed">From <tei:seg type="entry" rend="ms">Ustí Nad
     Labem</tei:seg> No. <tei:seg type="entry" rend="ms">148</tei:seg> Words <tei:seg type="entry"
     rend="ms">16</tei:seg> surrendered on the <tei:seg type="entry" rend="ms">20/6</tei:seg> 192
     <tei:hi rend="ms">...</tei:hi> at <tei:seg type="entry" rend="ms">13</tei:seg> hours <tei:seg
     type="entry" rend="ms">30</tei:seg> minutes</tei:ab>

   <tei:ab type="formpart" rend="printed">In the telegrams prepared by the type-setters the
    following indications denote: 1. the name of the official, 2. the assignment number, 3. the
    number of words (if need be, in fractional form), 4. the month and day, 5. the hour and minute
    of handing in. </tei:ab>


   <tei:note type="editorial">message area:</tei:note>

   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">Most cordial congratulations on yesterday<tei:lb/> Greetings
      from<tei:rs type="person" key="entity-000943">Arnold Weil</tei:rs><tei:ptr
      corresp="#r0001-fn02" type="footnote"/></tei:seg>
   </tei:ab>

   <tei:closer>
    <tei:fw type="footer">D. G. Nr. 769. 1925. Printed by the Austrian State Stationery Office in
     Vienna (St.) 19?? 25</tei:fw>
   </tei:closer>

  </tei:div>
