  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-005533">KopfermannA ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-005534">Königliche Bibliothek
        ~ HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">letter</tei:rs>

      <dc:title>Handwritten letter from Albert Kopfermann (Königliche Bibliothek zu Berlin) to
        Schenker, dated December 22, 1913</dc:title>
      <dcterms:created>1913-12-22T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-11-29</dcterms:dateSubmitted>
      <dc:description>Kopfermann sends five photographic prints.</dc:description>
      <dc:subject>Abzüge; prints; Doubletten; duplicates</dc:subject>



      <marcrel:correspondent key="entity-000464">Albert Kopfermann</marcrel:correspondent>
      <marcrel:correspondent key="entity-003292">Königliche Bibliothek zu
        Berlin</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:address>
          <tei:addrLine rend="indent-20"><tei:handShift new="#printed"/>KÖNIGLICHE
            BIBLIOTHEK</tei:addrLine>
          <tei:addrLine rend="indent-30"><tei:handShift new="#stamp"/>Musik-Abteilung</tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>Berlin NW7, <tei:date
              when="1913-12-22"><tei:handShift new="#AKo"/>22. XII <tei:handShift new="#printed"
                />191<tei:handShift new="#AKo"/>3</tei:date></tei:addrLine>
          <tei:lb/>
          <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>Dorotheenstraße
            81.</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute><tei:handShift new="#AKo"/>Sehr geehrter Herr <tei:rs key="entity-000632"
            type="person"><tei:hi rend="lateinschr">Doctor</tei:hi></tei:rs>!<tei:note
            xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this letter is recorded in
            Schenker's diary at OJ 1/13, p. 496, December 24, 1913: "Von Kopfermann langen 5 Blätter
            an, die ich aber sofort als Duplikate mit seiner eigenen Erlaubnis zurückstelle." ("From
            Kopfermann there come five leaves which I immediately return as duplicates, with his
            particular permission.")</tei:note></tei:salute>
      </tei:opener>


      <tei:p rend="first">Für freundl. Auskunft bestens dankend, sende ich Ihnen beiliegend die
        gewünschten <tei:add place="interlinear-above">5</tei:add> Abzüge. Senden Sie sie aber
        bitte, falls sie nur <tei:foreign xml:lang="fre">Doublette</tei:foreign>n für Sie sind,
        ungeniert zurück.<tei:note xml:id="r0001-fn02" type="editorial" place="foot">There are two
          diagonal pen-strokes drawn in the left margin, the first beside "[gewünsch-]ten 5 Abzüge"
          ("five requested prints"), the second beside "ungeniert zurück" ("return at your
          leisure").</tei:note></tei:p>

      <tei:p rend="first">In Eile</tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">Ihr hochachtungsvoll</tei:salute>
        <tei:salute rend="indent-70">ergebener</tei:salute>
        <tei:signed rend="indent-50"><tei:rs key="entity-000464" type="person">
            <tei:hi rend="lateinschr">Kopfermann</tei:hi></tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:address>
          <tei:addrLine rend="indent-20"><tei:handShift new="#printed"/>ROYAL LIBRARY</tei:addrLine>
          <tei:addrLine rend="indent-30"><tei:handShift new="#stamp"/>Music
            Department</tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>Berlin NW7, <tei:date
              when="1913-10-30"><tei:handShift new="#AKo"/>December 22, <tei:handShift
                new="#printed"/>191<tei:handShift new="#AKo"/>3</tei:date></tei:addrLine>
          <tei:lb/>
          <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>Dorotheenstraße
            81</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute><tei:handShift new="#AKo"/>Dear Dr. <tei:supplied reason="identity"><tei:rs
              key="entity-000632" type="person">Schenker</tei:rs></tei:supplied>,<tei:ptr
            corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">With many thanks for <tei:supplied reason="clarity">your</tei:supplied>
        kind information, I send enclosed to you here the requested <tei:add
          place="interlinear-above">five</tei:add> prints. However, please feel free to return them
        if they are merely duplicates for you.<tei:ptr corresp="#r0001-fn02" type="footnote"
        /></tei:p>

      <tei:p rend="first">In haste,</tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">With kind regards,</tei:salute>
        <tei:salute rend="indent-70">Yours truly,</tei:salute>
        <tei:signed rend="indent-50"><tei:rs key="entity-000464" type="person">
          Kopfermann</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>
  </sdo:record>
