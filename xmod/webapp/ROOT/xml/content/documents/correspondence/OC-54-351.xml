  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs type="correspondence" role="correspondence" key="entity-005439">Waldheim-Eberle ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typewritten postcard from Waldheim to Schenker, postmarked June 21, 1932</dc:title>
      <dcterms:created>1932-06-21T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-14</dcterms:dateSubmitted>

      <dc:description>Waldheim-Eberle has sent copies of the Urlinie-Tafeln to Otto Erich
        Deutsch.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002402">Waldheim-Eberle</marcrel:correspondent>
      <marcrel:correspondent key="entity-008402">Stefan Darnau</marcrel:correspondent>
      <marcrel:correspondent key="entity-008403">Karl Fleischmann</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:note type="editorial" place="inline">publisher’s emblem: WEAG</tei:note>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>GROSSDRUCKEREI UND VERLAG</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">Waldheim-Eberle A.G.</tei:hi></tei:addrLine>
          <tei:addrLine>WIEN, VII., SEIDENGASSE NR. 3–11</tei:addrLine>
        </tei:address>
        <tei:fw type="header" rend="inline">POSTKARTE</tei:fw>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPwe"/>Herrn Prof. Dr. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:hi rend="sperr">Igls</tei:hi> bei
            Innsbruck</tei:addrLine>
          <tei:addrLine rend="indent-50">Iglerhof Tirol.</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">||7 WIEN 62 | 21. VI. 32. 20 | * 4c * ||</tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:byline><tei:handShift new="#TPwe"/><tei:hi rend="underline"
          >710.M/P.</tei:hi></tei:byline>
        <tei:address>
          <tei:addrLine rend="indent-30">Herrn Prof.Dr. Heinrich Schenker,</tei:addrLine>
          <tei:addrLine rend="indent-75"><tei:hi rend="sperr underline">Igls</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-65">b. Innsbruck, Iglerhof.</tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">Bezugnehmend auf Ihre w. Karte<tei:note xml:id="r0001-fn01"
          type="editorial" place="foot">The sending of the postcard in question is recorded in
          Schenker’s diary for June 19, 1932: “An Waldheim (K.): bitte an Deutsch ein Exemplar zu
          leiten.” (“To Waldheim (postcard): I ask them to send a copy to Deutsch.”). — Receipt of
          Waldheim’s postcard is recorded in the diary for June 23, 1932: “Von Waldheim (K.): haben
          ein Exemplar an Deutsch geschickt.” (“Waldheim (postcard): they have sent Deutsch a
          copy.”).</tei:note> teilen wir Ihnen höfl. mit, dass wir heute 1 Exemplare der <tei:rs
          type="work" key="entity-001755">Thementafeln</tei:rs> an Herrn <tei:rs type="person"
          key="entity-000164">Prof. O. E. Deutsch</tei:rs>, Wien II Böcklinstrasse 26 gesandt
          haben.<tei:note xml:id="r0001-fn02" type="editorial" place="foot">Deutsch’s letter of June
          22, <tei:ref type="correspondence" cRef="OC-54-366">OC 54/366</tei:ref>, records the
          receipt of the graphs, but disappointment that no sample wrapper has been
        sent.</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">Uns bestens empfehlend, zeichnen wir</tei:salute>
        <tei:salute rend="indent-70">hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Musikabteilung</tei:signed>
        <tei:signed rend="indent-50"><tei:handShift new="#SD"/>Darnau <tei:handShift new="#KF"/>ppa.
          Fleischmann</tei:signed>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:note type="editorial" place="inline">publisher’s emblem: WEAG</tei:note>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MAJOR PRINTING HOUSE AND
            PUBLISHER</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">Waldheim-Eberle &amp; Co.</tei:hi></tei:addrLine>
          <tei:addrLine>VIENNA VII, SEIDENGASSE 3–11</tei:addrLine>
        </tei:address>
        <tei:fw type="header" rend="inline">POSTCARD</tei:fw>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPwe"/>Prof. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:hi rend="sperr">Igls</tei:hi> nr.
            Innsbruck</tei:addrLine>
          <tei:addrLine rend="indent-50">Iglerhof Tyrol</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">||7 VIENNA 62 | 21. VI. 32. 20 | * 4c * ||</tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:byline><tei:handShift new="#TPwe"/><tei:hi rend="underline"
          >710.M/P.</tei:hi></tei:byline>
        <tei:address>
          <tei:addrLine rend="indent-30">Prof. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-75"><tei:hi rend="sperr underline">Igls</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-65">nr. Innsbruck, Iglerhof</tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">In reference to your esteemed postcard,<tei:ptr corresp="#r0001-fn01"
          type="footnote"/> we would respectfully inform you that we have today sent one
          <tei:supplied reason="clarity">set of</tei:supplied> copies of the <tei:rs type="work"
          key="entity-001755">thematic graphs</tei:rs> to <tei:rs type="person" key="entity-000164"
          >Prof. O. E. Deutsch</tei:rs>, Vienna II, Böcklinstrasse 26.<tei:ptr corresp="#r0001-fn02"
          type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">With our compliments, we remain,</tei:salute>
        <tei:salute rend="indent-70">with kind regards,</tei:salute>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Music Division</tei:signed>
        <tei:signed rend="indent-50"><tei:handShift new="#SD"/>Darnau <tei:handShift new="#KF"/>ppa.
          Fleischmann</tei:signed>
      </tei:closer>

    </tei:div>
