      <dc:title>Handwritten picture postcard from Anna Schiff to Jeanette Schenker, dated June 25,
        1927</dc:title>

      <dcterms:created>1927-06-25T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2025-02-13</dcterms:dateSubmitted>
      <dc:description>Paul, Anna, and Lisa Schiff send greetings from their summer holiday in Frain
        an der Thaya [now Vranov nad Dyjí in the Czech Republic], not far from the border with Lower
        Austria.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000781">Anna Schiff</marcrel:correspondent>
      <marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:note type="editorial" place="inline">picture: castle high above a river with waterfall,
          captioned on recto: “Frain a.d. Thaya. Schloß.”</tei:note><tei:lb/>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied>An:</tei:supplied>
              <tei:handShift new="#AS"/>Frau Lie-Lie Schenker</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:del rend="overstrike"><tei:hi rend="underline">Wien
                  III.,</tei:hi></tei:del></tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:del rend="overstrike">Keilgasse
              8</tei:del></tei:addrLine>
            <tei:addrLine><tei:handShift new="#unk1"/><tei:del rend="overstrike"
              >Galtür</tei:del></tei:addrLine>
            <tei:addrLine><tei:handShift new="#unk2"/><tei:note type="editorial" place="inline"
                >beside postage stamp:</tei:note>Galtür</tei:addrLine>
            <tei:addrLine><tei:note type="editorial" place="inline">beside postage
              stamp:</tei:note>Tirol</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| VRANOV NAD DYJ | 25.V1 – 27 | a | * FRAIN * ||</tei:fw>
        </tei:fw><tei:lb/>
        <tei:dateline rend="indent-50"><tei:handShift new="#AS"/><tei:date when="1927-06-25">25/VI.
            27</tei:date></tei:dateline>
      </tei:opener>

      <tei:p>Von unserem -- INSERT --</tei:p>

      <tei:closer>
