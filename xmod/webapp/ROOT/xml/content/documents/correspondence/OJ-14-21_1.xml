            </tei:subst>
          </tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/>Telephon Nr. 1288.</tei:addrLine>
        </tei:address>
        <tei:lb/>Zahlbar und klagbar in Wien I.<tei:lb/>
        <tei:dateline rend="indent-45"><tei:rs key="entity-002484" type="place">Wien</tei:rs>, <tei:handShift new="#unk"/>
