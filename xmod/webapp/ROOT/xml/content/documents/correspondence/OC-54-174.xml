  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs key="entity-005444" type="correspondence" role="correspondence">Mandruck ~ HSchenker</tei:rs>
      <tei:rs key="entity-002744" type="corr-item-type" role="corr-item-type">Letter</tei:rs>

      <dc:title>Typed postcard from Mandruck to Schenker, dated June 14, 1927</dc:title>
      <dcterms:created>1927-06-14T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-04-03</dcterms:dateSubmitted>
      <dc:description>Mandruck have enclosed the page proofs for the second Meisterwerk volume, along
        with a printout of the galley proofs and gatherings 5–6.</dc:description>
      <dc:subject>page proof; Anlage; gathering; Bogen; galley proof; Fahnen</dc:subject>

    <marcrel:correspondent key="entity-003129">Mandruck</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK AG</tei:addrLine>
          <tei:addrLine>MÜNCHEN</tei:addrLine>
          <tei:addrLine>THERESIENSTRASSE 75</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:byline><tei:handShift new="#TPhla"/><tei:hi rend="italic">H/La.</tei:hi></tei:byline>
        <tei:dateline rend="indent-70"><tei:placeName>München</tei:placeName>, <tei:date when="1927-06-14">14. Juni 1927</tei:date>.</tei:dateline>
        <tei:address>
          <tei:lb/>

          <tei:addrLine><tei:rs key="entity-000632" type="person">Herrn Dr. <tei:hi rend="sperr">Heinrich Schenker</tei:hi></tei:rs>,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this letter is recorded
              in Schenker's diary at OJ 3/9, p. 3077, June 15, 1927: "Die Bogen 5 u. 6 kommen."
              ("Gatherings 5 and 6 arrive.").</tei:note></tei:addrLine>

          <tei:addrLine rend="indent-60"><tei:hi rend="double-underline"><tei:hi rend="sperr">Wien</tei:hi>-III.</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse <tei:sic>3</tei:sic>.</tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/>
      <tei:lb/>
      <tei:lb/>

      <tei:p rend="first">Vom <tei:rs key="entity-001751" type="work">Musik-Jahrbuch</tei:rs>
        überreichen wir Ihnen in der Anlage die umbrochenen Bogen 5 &amp; 6 in dreifacher
        Ausfertigung nebst den dazu gehörigen Fahnen-Abzügen zur gefl. Bedienung. Wir empfehlen uns
        Ihnen</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">hochachtungsvoll!</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129" type="organization">MANDRUCK A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK &amp; Co.</tei:addrLine>
          <tei:addrLine>MUNICH</tei:addrLine>
          <tei:addrLine>THERESIENSTRAßE 75</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:byline><tei:handShift new="#TPhla"/><tei:hi rend="italic">H/La.</tei:hi></tei:byline>
        <tei:dateline rend="indent-70"><tei:placeName>Munich</tei:placeName>, <tei:date when="1927-06-14">June 14, 1927</tei:date>.</tei:dateline>
        <tei:address>
          <tei:lb/>

          <tei:addrLine><tei:rs key="entity-000632" type="person">Dr. <tei:hi rend="sperr">Heinrich Schenker</tei:hi></tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:addrLine>

          <tei:addrLine rend="indent-60"><tei:hi rend="double-underline"><tei:hi rend="sperr">Vienna</tei:hi>-III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse <tei:sic>3</tei:sic>.</tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/>
      <tei:lb/>
      <tei:lb/>

      <tei:p rend="first">In the enclosures we send you the page proofs of your <tei:rs key="entity-001751" type="work">music yearbook</tei:rs>, gatherings 5 and 6 in
        triplicate, together with a printout of the galley proofs, for you kind consideration. We
        remain</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">yours respectfully,</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129" type="organization">MANDRUCK &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

    </tei:div>
