  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002700">Hertzka ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">postcard</tei:rs>



      <dc:title>Typewritten postcard from UE to Schenker, dated October 1, 1912</dc:title>

      <dcterms:created>1912-10-01T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-03-10</dcterms:dateSubmitted>
      <dc:description>As instructed, UE has sent Schenker's Beethovens neunte Sinfonie to Ludwig
        Karpath.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:correspondent key="entity-000356">Emil Hertzka</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">advertisement for "Excelsior" in left third of
            <tei:hi rend="italic">recto</tei:hi></tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPue"/>Wohlg. Herrn Prof. Dr. Heinrich
            Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:hi rend="sperr underline">Wien</tei:hi> III. </tei:addrLine>
          <tei:addrLine rend="indent-40">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| <tei:gap reason="illegible"/> | 31.V.12 | * 3a * ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine rend="indent-10"><tei:handShift new="#TPue"/><tei:hi rend="underline"
              >Wohlgeboren Herrn Prof. Dr. <tei:rs type="person" key="entity-000632">H.
                Schenker</tei:rs></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:hi rend="sperr underline"><tei:rs type="place"
                key="entity-002256">Wien</tei:rs></tei:hi>.</tei:addrLine>
          <tei:lb/>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
          >Professor</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p>Im Besitze Ihrer gesch. Karte vom <tei:date when="1912-09-30"
          >30.v.M.</tei:date><tei:note xml:id="r0001-fn01" type="editorial" place="foot">= <tei:ref
            type="correspondence" cRef="WSLB-138">WSLB 138</tei:ref>, September 30, 1912.</tei:note>
        teilen wir Ihnen höfl.mit, dass wir an Herrn <tei:rs type="person" key="entity-000424"
          >Karpath</tei:rs> Ihr Werk über die <tei:rs type="work" key="entity-001731"><tei:title>IX.
            Symphonie Beethoven</tei:title></tei:rs><tei:add place="inline">’s</tei:add> gesandt
        haben und zeichnen </tei:p>

      <tei:closer>
        <tei:dateline><tei:handShift new="#printed"/>Wien, am <tei:date when="1912-10-01"
              ><tei:handShift new="#TPue"/>1. Oktober <tei:handShift new="#printed"
              />191<tei:handShift new="#TPue"/>2</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>I, Reichsratsstraße 9</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-002408" type="organization"
            >„Universal-Edition“ A-G.</tei:rs></tei:signed>
        <tei:signed rend="indent-70">(Jos. Aibl Verlag, G.m.b.H.)</tei:signed>
        <tei:note type="editorial" place="inline">unsigned</tei:note>
      </tei:closer>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">advertisement for "Excelsior" in left third of
            <tei:hi rend="italic">recto</tei:hi></tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPue"/>Prof. Heinrich Schenker, Esq.</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:hi rend="sperr underline">Vienna</tei:hi>
            III</tei:addrLine>
          <tei:addrLine rend="indent-40">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| <tei:gap reason="illegible"/> | 31.V.12 | * 3a * ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine rend="indent-10"><tei:handShift new="#TPue"/><tei:hi rend="underline">Prof.
                <tei:rs type="person" key="entity-000632">H. Schenker</tei:rs></tei:hi>,
            Esq.</tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:hi rend="sperr underline"><tei:rs type="place"
                key="entity-002256">Vienna</tei:rs></tei:hi></tei:addrLine>
          <tei:lb/>
        </tei:address>
        <tei:salute>Dear <tei:rs type="person" key="entity-000632">Professor</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p>In receipt of your esteemed postcard of the <tei:date when="1912-09-30">30th of last
          month</tei:date>,<tei:ptr corresp="#r0001-fn01" type="footnote"/> we respectfully inform
        you that we have sent Mr. <tei:rs type="person" key="entity-000424">Karpath</tei:rs> your
        book about <tei:rs type="work" key="entity-001731"><tei:title level="m">Beethoven's Ninth
            Symphony</tei:title></tei:rs>. We remain</tei:p>

      <tei:closer>
        <tei:dateline><tei:handShift new="#printed"/>Vienna, <tei:date when="1912-10-01"
              ><tei:handShift new="#TPue"/>October 1, <tei:handShift new="#printed"
              />191<tei:handShift new="#TPue"/>2</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>I, Reichsratsstraße 9</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>With kind regards,</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-002408" type="organization">Universal
            Edition &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70">(Jos. Aibl Publishers &amp; Co.)</tei:signed>
        <tei:note type="editorial" place="inline">unsigned</tei:note>
      </tei:closer>



    </tei:div>
