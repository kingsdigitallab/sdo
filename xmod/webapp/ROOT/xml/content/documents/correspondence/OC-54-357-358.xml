  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs type="correspondence" role="correspondence" key="entity-005439">Waldheim-Eberle ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003349">Invoice</tei:rs>

      <dc:title>Typewritten letter and invoice from Waldheim to Schenker, dated July 14,
        1932</dc:title>
      <dcterms:created>1932-07-14T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-14</dcterms:dateSubmitted>

      <dc:description>Waldheim-Eberle submits its pre-invoice for 1,000 copies of Schenker's Fünf
        Urlinie-Tafeln.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002402">Waldheim-Eberle</marcrel:correspondent>
      <marcrel:correspondent key="entity-008402">Stefan Darnau</marcrel:correspondent>
      <marcrel:correspondent key="entity-008404">Rudolf Kindler</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:div type="part_1">
        <tei:opener>
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>GROSSDRUCKEREI UND VERLAG</tei:addrLine>
            <tei:addrLine><tei:hi rend="bold">WALDHEIM-EBERLE A.G.</tei:hi></tei:addrLine>
            <tei:addrLine><tei:hi rend="bold">WIEN VII. SEIDENGASSE 3–11</tei:hi></tei:addrLine>
          </tei:address>
          <tei:dateline rend="indent-50">
            <tei:handShift new="#TPwe"/><tei:placeName>Wien</tei:placeName>, <tei:date
              when="1932-07-14">14. Juli 1932</tei:date>.</tei:dateline><tei:lb/>
          <tei:byline><tei:handShift new="#printed"/>MUSIKALIENDRUCK</tei:byline>
          <tei:byline><tei:handShift new="#TPwe"/>M/G/Bo1/XXIII/81</tei:byline>
          <tei:address>
            <tei:addrLine rend="indent-30">Herrn Prof. Dr. Heinrich Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-60"> dzt. <tei:hi rend="underline">Igls in
              Tirol</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-70">Iglerhof.</tei:addrLine>
          </tei:address>
          <tei:lb/>
        </tei:opener>

        <tei:p rend="first">Wir sind im Besitze Ihrer gesch. Karte vom <tei:date when="1932-07-12"
            >12. ds.</tei:date>[1] und senden Ihnen in der Anlage gewünschte Vorfaktura über die
          1000 Expl. „<tei:rs type="work" key="entity-001755"
            ><tei:title>Urlinie-Tafeln</tei:title></tei:rs>“ zu Ihrer gefl. Bedienung.[2] </tei:p>

        <tei:p rend="first">Ordregemäss haben wir heute die Lichtpause-Originale an die <tei:rs
            type="organization" key="entity-002408">Universal-Edition</tei:rs> abgeliefert,
          erhielten diese jedoch wieder zurück, mit dem Bemerken, sie in unserem Archiv, so wie alle
          bisherigen Lichtpausen und Stichplatten, für die <tei:rs type="organization"
            key="entity-002408">U. E.</tei:rs> einzulagern. </tei:p>

        <tei:closer>
          <tei:salute rend="indent-50">mit vorzüglicher Hochachtung</tei:salute>
          <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
              type="organization">Waldheim-Eberle A.G.</tei:rs></tei:signed>
          <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Musikabteilung</tei:signed>
          <tei:signed rend="indent-50"><tei:handShift new="#SD"/><tei:rs type="person"
              key="entity-008402">Darnau</tei:rs>
            <tei:handShift new="#RK"/>ppa. <tei:rs type="person" key="entity-008404"
              >Kindler</tei:rs></tei:signed>
        </tei:closer>

        <tei:postscript>
          <tei:p>1 Faktura<tei:lb/>1 Erlagschein</tei:p>

        </tei:postscript>
      </tei:div>

      <tei:div type="part_2">
        <tei:opener>
          <tei:address>
            <tei:addrLine><tei:note type="editorial" place="inline">printer's emblem with
                "WEAG"</tei:note></tei:addrLine>
            <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="bold">WALDHEIM-EBERLE
                A.G.</tei:hi></tei:addrLine>
            <tei:addrLine>GROSSDRUCKEREI UND VERLAG</tei:addrLine>
            <tei:addrLine><tei:hi rend="bold">WIEN VII. SEIDENGASSE 3–11</tei:hi></tei:addrLine>
          </tei:address>
          <tei:dateline rend="indent-70">
            <tei:handShift new="#TPwe"/><tei:placeName>Wien</tei:placeName>, <tei:date
              when="1932-07-14">14. Juli 1932</tei:date></tei:dateline><tei:lb/>
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>VORFAKTURA für <tei:handShift new="#TPwe"
              />Herrn Prof. Dr. Heinrich Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-60"> dzt. <tei:hi rend="underline">IGLS in
              Tirol</tei:hi></tei:addrLine>
          </tei:address>
          <tei:lb/>
        </tei:opener>

        <tei:p>AUF IHRE RECHNUNG UND GEFAHR – ZAHLBAR UND KLAGBAR IN WIEN</tei:p>

        <tei:p><tei:table rows="8" cols="4">

            <tei:row role="label" n="1">
              <tei:cell role="data"><tei:supplied reason="clarity"
                >Gegenstand</tei:supplied></tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data">Preis</tei:cell>
              <tei:cell role="data">Betrag</tei:cell>
            </tei:row>

            <tei:row role="data" n="2">
              <tei:cell role="data"><tei:hi rend="underline">1000 Exemplare
                Urlinientafeln</tei:hi></tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="3">
              <tei:cell role="data"/>
              <tei:cell role="data">laut Offert </tei:cell>
              <tei:cell role="data">S 700.–</tei:cell>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="4">
              <tei:cell role="data"/>
              <tei:cell role="data">1000 Umschläge </tei:cell>
              <tei:cell role="data">S 170.–</tei:cell>
              <tei:cell role="data"/>
            </tei:row>
            <tei:row role="data" n="5">
              <tei:cell role="data"/>
              <tei:cell role="data">Frankatur und Anfertigung</tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="6">
              <tei:cell role="data"/>
              <tei:cell role="data">von 4 Kisten für Versendung</tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="7">
              <tei:cell role="data"/>
              <tei:cell role="data">von 800 Exempl.n Amerika </tei:cell>
              <tei:cell role="data">S 160.–</tei:cell>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="8">
              <tei:cell role="data"/>
              <tei:cell role="data">Summe</tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data">S 1.030.–</tei:cell>
            </tei:row>

          </tei:table></tei:p>

        <tei:closer>
          <tei:salute rend="indent-35">hochachtungsvoll</tei:salute>
          <tei:signed rend="indent-30"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
              type="organization">Waldheim-Eberle A.G.</tei:rs></tei:signed>
          <tei:signed rend="indent-40"><tei:handShift new="#stamp"/>Musikabteilung</tei:signed>
          <tei:signed rend="indent-50"><tei:handShift new="#RK"/>ppa. <tei:rs type="person"
              key="entity-008404">Kindler</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

    </tei:div>



    <tei:div type="translation">

      <tei:div type="part_1">
        <tei:opener>
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>MAJOR PRINTING HOUSE AND
              PUBLISHER</tei:addrLine>
            <tei:addrLine><tei:hi rend="bold">WALDHEIM-EBERLE &amp; CO.</tei:hi></tei:addrLine>
            <tei:addrLine><tei:hi rend="bold">VIENNA VII, SEIDENGASSE 3–11</tei:hi></tei:addrLine>
          </tei:address>
          <tei:dateline rend="indent-50">
            <tei:handShift new="#TPwe"/><tei:placeName>Vienna</tei:placeName>, <tei:date
              when="1932-07-14">July 14, 1932</tei:date></tei:dateline><tei:lb/>
          <tei:byline><tei:handShift new="#printed"/>MUSIC PRINTING</tei:byline>
          <tei:byline><tei:handShift new="#TPwe"/>M/G/Bo1/XXIII/81</tei:byline>
          <tei:address>
            <tei:addrLine rend="indent-30">Prof. Heinrich Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-60"> currently at <tei:hi rend="underline">Igls in the
                Tyrol</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-70">Iglerhof</tei:addrLine>
          </tei:address>
          <tei:lb/>
        </tei:opener>

        <tei:p rend="first">We are in receipt of your esteemed postcard of the <tei:date
            when="1932-07-12">12th of this month</tei:date>,[1] and send you in the inclosed the
          requested pre-invoice for the 1,000 copies of <tei:rs type="work" key="entity-001755"
              ><tei:title level="m">Urlinie-Tafeln</tei:title></tei:rs> at your
          convenience.[2]</tei:p>

        <tei:p rend="first">As is proper, we have today delivered the blueprint originals to <tei:rs
            type="organization" key="entity-002408">Universal Edition</tei:rs>, but they have been
          returned to us with the request that we store them for the <tei:rs type="organization"
            key="entity-002408">U.E.</tei:rs> in our archive, as with all previous blueprints and
          engraved plates.</tei:p>

        <tei:closer>
          <tei:salute rend="indent-50">with kind regards,</tei:salute>
          <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
              type="organization">Waldheim-Eberle &amp; Co.</tei:rs></tei:signed>
          <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Music Division</tei:signed>
          <tei:signed rend="indent-50"><tei:handShift new="#SD"/><tei:rs type="person"
              key="entity-008402">Darnau</tei:rs>
            <tei:handShift new="#RK"/>ppa. <tei:rs type="person" key="entity-008404"
              >Kindler</tei:rs></tei:signed>
        </tei:closer>

        <tei:postscript>
          <tei:p>1 invoice<tei:lb/>1 payment slip</tei:p>
        </tei:postscript>
      </tei:div>

      <tei:div type="part_2">
        <tei:opener>
          <tei:address>
            <tei:addrLine><tei:note type="editorial" place="inline">printer's emblem with
                "WEAG"</tei:note></tei:addrLine>
            <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="bold">WALDHEIM-EBERLE &amp;
                Co.</tei:hi></tei:addrLine>
            <tei:addrLine>MAJOR PRINTING HOUSE AND PUBLISHER</tei:addrLine>
            <tei:addrLine><tei:hi rend="bold">VIENNA VII, SEIDENGASSE 3–11</tei:hi></tei:addrLine>
          </tei:address>
          <tei:dateline rend="indent-70">
            <tei:handShift new="#TPwe"/><tei:placeName>Vienna</tei:placeName>, <tei:date
              when="1932-07-14">14. July, 1932</tei:date></tei:dateline><tei:lb/>
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>PRE-INVOICE for <tei:handShift new="#TPwe"
              />Prof. Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-60"> currently at <tei:hi rend="underline">IGLS in the
                Tyrol</tei:hi></tei:addrLine>
          </tei:address>
          <tei:lb/>
        </tei:opener>

        <tei:p>AT YOUR ACCOUNT AND OWN RISK – PAYABLE AND ACTIONABLE IN VIENNA</tei:p>


        <tei:p><tei:table rows="8" cols="4">

            <tei:row role="label" n="1">
              <tei:cell role="data"><tei:supplied reason="clarity">Item</tei:supplied></tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data">Price</tei:cell>
              <tei:cell role="data">Amount</tei:cell>
            </tei:row>

            <tei:row role="data" n="2">
              <tei:cell role="data"><tei:hi rend="underline">1,000 copies of
                Urlinie-tafeln</tei:hi></tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="3">
              <tei:cell role="data"/>
              <tei:cell role="data">as per offer </tei:cell>
              <tei:cell role="data">S 700.–</tei:cell>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="4">
              <tei:cell role="data"/>
              <tei:cell role="data">1,000 wrappers </tei:cell>
              <tei:cell role="data">S 170.–</tei:cell>
              <tei:cell role="data"/>
            </tei:row>
            <tei:row role="data" n="5">
              <tei:cell role="data"/>
              <tei:cell role="data">Postage and packing</tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="6">
              <tei:cell role="data"/>
              <tei:cell role="data">of 4 crates for shipping</tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="7">
              <tei:cell role="data"/>
              <tei:cell role="data">of 800 copies to America </tei:cell>
              <tei:cell role="data">S 160.–</tei:cell>
              <tei:cell role="data"/>
            </tei:row>

            <tei:row role="data" n="8">
              <tei:cell role="data"/>
              <tei:cell role="data">Total</tei:cell>
              <tei:cell role="data"/>
              <tei:cell role="data">S 1,030.–</tei:cell>
            </tei:row>

          </tei:table></tei:p>

        <tei:closer>
          <tei:salute rend="indent-35">with kind regards,</tei:salute>
          <tei:signed rend="indent-30"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
              type="organization">Waldheim-Eberle &amp; Co.</tei:rs></tei:signed>
          <tei:signed rend="indent-40"><tei:handShift new="#stamp"/>Music Division</tei:signed>
          <tei:signed rend="indent-50"><tei:handShift new="#RK"/>ppa. <tei:rs type="person"
              key="entity-008404">Kindler</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

    </tei:div>
  </sdo:record>
