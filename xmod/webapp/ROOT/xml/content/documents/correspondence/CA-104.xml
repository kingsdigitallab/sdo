  <sdo:record ID="r0001">
    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002724">Cotta ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>


      <dc:title>Handwritten postcard from Schenker to Cotta, dated August 30, 1909</dc:title>
      <dcterms:created>1909-08-30T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2018-08-01</dcterms:dateSubmitted>
      <dc:description>Schenker has returned to Vienna.</dc:description>
      <dc:subject>Korrekturen; corrections; Fahnen; galley-proofs</dc:subject>

      <marcrel:correspondent key="entity-000632">HSchenker</marcrel:correspondent>
      <marcrel:recipient key="entity-002405">Cotta</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:note type="editorial" place="inline">All in a decorated ruleframe</tei:note><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">Absender:</tei:supplied>
              <tei:handShift new="#HS"/><tei:hi rend="lateinsch">H Schenker</tei:hi>
            </tei:addrLine>
            <tei:addrLine>III., <tei:hi rend="lateinschr">Reisnerstr.</tei:hi> 38</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine>An die <tei:hi rend="lateinschr">J. G. Cotta</tei:hi>'sche
              Bhdg-Nfg.</tei:addrLine>
            <tei:addrLine><tei:hi rend="lateinschr">Stuttgart</tei:hi>
            </tei:addrLine>
            <tei:addrLine><tei:hi rend="lateinschr">Deutschland</tei:hi></tei:addrLine>
            <tei:lb/>
          </tei:address>
          <tei:fw type="postmark">|| <tei:gap reason="illeg"/> WIEN 18 | 30.<tei:gap reason="illeg"
            />.09 ‒ 8 | * C * ||</tei:fw><tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/>
        </tei:fw>
        <tei:salute><tei:handShift new="#HS"/>Sehr geehrter <tei:rs key="entity-002405"
            type="organization">Herr</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p>Hiemit theile ich mit, daß ich wieder in <tei:hi rend="lateinschr">Wien</tei:hi>
          eingetroffen,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Schenker traveled
          to Steinach on July 6, 1909 (OJ 1/5, p.102; <tei:ref type="correspondence" cRef="CA-103"
            >CA 103</tei:ref>), and was back in Vienna by August 30.</tei:note> u. alsbald die
        Korrekturen der mir nach <tei:rs type="place" key="entity-002396"><tei:hi rend="lateinschr"
            >Steinach</tei:hi></tei:rs> zugekommenen Fahnen einsenden werde.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">Mit ausgezeichneter Hochachtung</tei:salute>
        <tei:signed rend="indent-40">
          <tei:rs key="entity-000632" type="person">D<tei:hi rend="supralinear">r</tei:hi>
            <tei:hi rend="lateinschr">Heinrich Schenker</tei:hi></tei:rs>
        </tei:signed><tei:lb/>
        <tei:dateline><tei:date when="1909-08-30">30 August 09</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:note type="editorial" place="inline">All in a decorated ruleframe</tei:note><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">From:</tei:supplied>
              <tei:handShift new="#HS"/>H. Schenker</tei:addrLine>
            <tei:addrLine><tei:supplied reason="identity">Vienna</tei:supplied> III, Reisnerstraße
              38</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine>To the Successors to J. G. Cotta's Book Dealership</tei:addrLine>
            <tei:addrLine>Stuttgart </tei:addrLine>
            <tei:addrLine>Germany</tei:addrLine>
            <tei:lb/>
          </tei:address>
          <tei:fw type="postmark">|| <tei:gap reason="illeg"/> VIENNA 18 | 30.<tei:gap
              reason="illeg"/>.09 ‒ 8 | * C * ||</tei:fw><tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/>
        </tei:fw>
        <tei:salute><tei:handShift new="#HS"/>Dear <tei:rs key="entity-002405" type="organization"
            >Sir</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p>This is to let you know that I am back again in Vienna,<tei:ptr corresp="#r0001-fn01"
          type="footnote"/> and will send my corrections to the galley-proofs that reached me in
          <tei:rs type="place" key="entity-002396">Steinach</tei:rs> as soon as possible.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">With kind regards,</tei:salute>
        <tei:signed rend="indent-40">
          <tei:rs key="entity-000632" type="person">Dr. Heinrich Schenker</tei:rs>
        </tei:signed><tei:lb/>
        <tei:dateline><tei:date when="1909-08-30">August 30, 1909</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>

  </sdo:record>
