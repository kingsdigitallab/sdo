 <sdo:record ID="r0001">

  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~ HSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>



   <dc:title>Handwritten postcard from Stedler(?) (UE) to Schenker, dated July 4, 1902</dc:title>

   <dcterms:created>1902-07-04T10:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2016-02-04</dcterms:dateSubmitted>
   <dc:description>Annotation on p. VII is impossible.</dc:description>
   <dc:subject>Anmerkung; annotation; Platte; plate; Raum; room</dc:subject>


   <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
  </sdo:itemDesc>

  <tei:div type="transcription">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:note type="editorial" place="inline">All in a decorated rule-frame</tei:note><tei:lb/>
    <tei:fw type="envelope">
     <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
       >Correspondenz-Karte</tei:hi></tei:fw><tei:lb/>
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied>
       <tei:handShift new="#UEsten1"/>Sr Wolgeboren Herrn Professor Heinrich Schenker</tei:addrLine>
      <tei:addrLine><tei:handShift new="#printed"/>in <tei:handShift new="#UEsten1"/>Baden
       b/Wien</tei:addrLine>
      <tei:addrLine>Johannesgasse 5.</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| WIEN <tei:gap reason="illegible"/> | a <tei:gap reason="illegible"/>
      | 4. 7. 03[?] ] 6–7N ||</tei:fw>
     <tei:fw type="postmark">|| BADEN 1 | N.Ö. | BESTELLT | 5. 7. 02 | 7—10V ||</tei:fw>
    </tei:fw>
    <tei:lb/><tei:lb/>
    <tei:milestone unit="folio" n="verso"/><tei:lb/>
    <tei:note type="editorial" place="inline">handwritten on printed lines</tei:note><tei:lb/>
    <tei:address>
     <tei:addrLine><tei:handShift new="#UEsten2"/>Sr. Wolgeboren Herrn Professor Heinrich
      Schenker.</tei:addrLine>
    </tei:address>
    <tei:salute>Hochverehrter Herr <tei:rs type="person" key="entity-000632"
     >Professor</tei:rs>!</tei:salute>
   </tei:opener>

   <tei:p rend="first">Im Anschlusse an unsere heutige Karte<tei:note xml:id="r0001-fn01"
     type="editorial" place="foot"><tei:ref type="correspondence" cRef="OC-52-4">OC
     52/4</tei:ref></tei:note> beehren wir uns ordnungshalber zu erwähnen, dass die von Ihnen auf
     <tei:rs key="entity-001720" type="work">Seite VII.</tei:rs> neu hinzugefügte Anmerkung
    unmöglich untergebracht werden kann, da hiezu auf der Platte absolut kein Raum mehr vorhanden
    ist.</tei:p>

   <tei:closer>
    <tei:salute rend="indent-10">Mit dem Ausdrucke vorzüglichster Hochachtung</tei:salute>
    <tei:signed rend="indent-70"><tei:handShift new="#printed"/>„<tei:rs key="entity-002408"
      type="organization">Universal-Edition</tei:rs>“ Actiengesellschaft</tei:signed>
    <tei:signed><tei:handShift new="#St"/><tei:unclear reason="reading"
     >Stedler</tei:unclear></tei:signed><tei:lb/>
    <tei:address>
     <tei:addrLine><tei:handShift new="#printed"/>Wien, am <tei:handShift new="#UEsten1"/>4. Juli
       <tei:handShift new="#printed"/>190<tei:handShift new="#UEsten2"/>2.</tei:addrLine>
     <tei:addrLine><tei:handShift new="#printed"/>I., Maximilianstrasse Nr. 11.</tei:addrLine>
    </tei:address>
   </tei:closer>


  </tei:div>

  <tei:div type="translation">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:note type="editorial" place="inline">All in a decorated rule-frame</tei:note><tei:lb/>
    <tei:fw type="envelope">
     <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
       >Postcard</tei:hi></tei:fw><tei:lb/>
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied>
       <tei:handShift new="#UEsten1"/>Professor Heinrich Schenker, Esq.</tei:addrLine>
      <tei:addrLine><tei:handShift new="#printed"/>in <tei:handShift new="#UEsten1"/>Baden near
       Vienna</tei:addrLine>
      <tei:addrLine>Johannesgasse 5</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| VIENNA <tei:gap reason="illegible"/> | a <tei:gap reason="illegible"
      /> | 4. 7. 03[?] ] 6–7N ||</tei:fw>
     <tei:fw type="postmark">|| BADEN 1 | Lower Austria | DELIVERED | 5. 7. 02 | 7—10 A.M.
      ||</tei:fw>
    </tei:fw>
    <tei:lb/><tei:lb/>
    <tei:milestone unit="folio" n="verso"/><tei:lb/>
    <tei:note type="editorial" place="inline">handwritten on printed lines</tei:note><tei:lb/>
    <tei:address>
     <tei:addrLine><tei:handShift new="#UEsten2"/>Professor Heinrich Schenker, Esq.</tei:addrLine>
    </tei:address>
    <tei:salute>Highly revered <tei:rs type="person" key="entity-000632"
     >Professor</tei:rs>,</tei:salute>
   </tei:opener>

   <tei:p rend="first">In connection with our postcard of today,<tei:ptr corresp="#r0001-fn01"
     type="footnote"/> we have the honor of mentioning, for the sake of formality, that the
    annotation recently inserted by you on <tei:rs key="entity-001720" type="work">p.VII</tei:rs> is
    impossible to do, since there is absolutely no room left on the plate for it. </tei:p>

   <tei:closer>
    <tei:salute rend="indent-10">With expression of highest regards</tei:salute>
    <tei:signed rend="indent-70"><tei:handShift new="#printed"/><tei:rs key="entity-002408"
      type="organization">Universal Edition</tei:rs> &amp; Co.</tei:signed>
    <tei:signed><tei:handShift new="#St"/><tei:unclear reason="reading"
     >Stedler</tei:unclear></tei:signed><tei:lb/>
    <tei:address>
     <tei:addrLine><tei:handShift new="#printed"/>Vienna, <tei:handShift new="#UEsten1"/>July 4,
       <tei:handShift new="#printed"/>190<tei:handShift new="#UEsten2"/>2.</tei:addrLine>
     <tei:addrLine><tei:handShift new="#printed"/>I., Maximilianstrasse Nr. 11.</tei:addrLine>
    </tei:address>
   </tei:closer>

  </tei:div>
