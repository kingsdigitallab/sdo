  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-004414">DMV ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Handwritten
        Note</tei:rs>




      <dc:title>Handwritten Note from DMV to Schenker, dated June 17, 1926</dc:title>

      <dcterms:created>1926-06-17T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-17</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag ask Schenker to exchange five paper-bound copies of the
        first Meisterwerk Yearbook for five of the hard-bound copies he had recently received from
        them.</dc:description>
      <dc:subject>return; zurückgeben; Broschur; paper-bound copy; hard-bound copy;
        Ganzleinenband</dc:subject>

    <marcrel:correspondent key="entity-002627">DMV</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:del rend="overstrike">Firma</tei:del>
            <tei:handShift new="#unk"/><tei:hi rend="underline">Herrn Dr. Hch. Schenker,
              Wien.</tei:hi></tei:addrLine>
        </tei:address><tei:lb/><tei:lb/>
      </tei:opener>        

      <tei:p rend="first">Wir bitten höflichst, unserem Boten an Stelle der anbei folgenden 5
        Broschuren die Ihnen übersandten 5 Ganzleinenbände <tei:rs key="entity-001750" type="work">Ihres Werkes</tei:rs> zurückzugeben.<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this letter is not recorded in Schenker's diary.</tei:note></tei:p>

      <tei:closer><tei:lb/>
        <tei:date><tei:handShift new="#printed"/>Wien I, <tei:handShift new="#unk"/><tei:date when="1926-06-17">17.VI.1926</tei:date>.<tei:lb/><tei:handShift new="#printed"/>Wallnerstraße 4 </tei:date><tei:lb/>
        <tei:salute rend="indent-50">Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-50"><tei:rs key="entity-002627" type="organization">DREI MASKEN
            VERLAG Ges. m. B. H.</tei:rs></tei:signed>
        <tei:signed rend="indent-50"><tei:handShift new="#unk"/><tei:rs key="entity-004930" type="person"><tei:note type="editorial">Illegible
          signature</tei:note></tei:rs></tei:signed>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:del rend="overstrike">Firm</tei:del>
            <tei:handShift new="#unk"/><tei:hi rend="underline">Dr. Hch. Schenker,
            Vienna.</tei:hi></tei:addrLine>
        </tei:address><tei:lb/><tei:lb/>
      </tei:opener>

      <tei:p rend="first">We kindly request that you return five linen-bound copies of <tei:rs key="entity-001750" type="work">your work</tei:rs> to our messenger, in return for these
        five paperback copies.<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:p>

      <tei:closer><tei:lb/>
        <tei:date><tei:handShift new="#printed"/>Vienna I, <tei:handShift new="#unk"/><tei:date when="1926-06-17">17.VI.1926</tei:date>.<tei:lb/><tei:handShift new="#printed"/>Wallnerstraße 4 </tei:date><tei:lb/>
        <tei:salute rend="indent-50">Yours faithfully,</tei:salute>
        <tei:signed rend="indent-50"><tei:rs key="entity-002627" type="organization">DREI MASKEN
            VERLAG &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-50"><tei:handShift new="#unk"/><tei:rs key="entity-004930" type="person"><tei:note type="editorial">Illegible
          signature</tei:note></tei:rs></tei:signed>
      </tei:closer>


    </tei:div>
