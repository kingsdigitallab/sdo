 <sdo:record ID="r0001">

  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-002730">Hoboken ~
    HSchenker</tei:rs>
   <tei:rs role="correspondence" type="correspondence" key="entity-002731">Hoboken ~
    H+JSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002751">Picture postcard</tei:rs>


   <dc:title>Handwritten picture postcard from Hoboken to Schenker, dated April 7, 1932</dc:title>

   <dcterms:created>1932-04-07T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2016-11-04</dcterms:dateSubmitted>
   <dc:description>Hoboken will come for a lesson on Friday.</dc:description>
   <dc:subject>Stunde; lesson</dc:subject>



   <marcrel:correspondent key="entity-000380">Hoboken</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
   <marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient>
  </sdo:itemDesc>



  <tei:div type="transcription">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:note type="editorial" place="inline">All in decorative rule-frame</tei:note><tei:lb/>
    <tei:note type="editorial" place="inline">small picture, caption: Trisannaschlucht,
     Tirol</tei:note><tei:lb/>
    <tei:fw type="envelope"><tei:lb/>
     <tei:add resp="unknown"><tei:handShift new="#unk"/>Rohr post</tei:add><tei:lb/>
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity">An: </tei:supplied><tei:handShift new="#AvH"
       />Herrn D<tei:hi rend="supralinear">r.</tei:hi> Heinrich Schenker</tei:addrLine>
      <tei:addrLine rend="indent-10">Wien</tei:addrLine>
      <tei:addrLine>III, Keilgasse 8.</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark"> || 3 WIEN 128 | 7.IV.32 1210 | * R * ||</tei:fw>
     <tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="2"/> WIEN 50 |
      7.IV.32 12 | * R/a * ||</tei:fw>
     <tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="24"/> ||</tei:fw><tei:lb/>
     <tei:fw type="label"><tei:note type="editorial" place="inline">printed
       label:</tei:note><tei:handShift new="#printed"/>|| Durch Eilboten. | <tei:hi rend="sperr"
       >Expres</tei:hi>. ||</tei:fw><tei:lb/><tei:lb/>
     <tei:milestone unit="folio" n="verso"/><tei:lb/>
     <tei:handShift new="#AvH"/><tei:date when="1932-04-07" rend="ident-70">7. IV. 32</tei:date>
    </tei:fw>
    <tei:salute rend="indent-10">Sehr verehrter Herr <tei:rs type="person" key="entity-000632"
       >D<tei:hi rend="supralinear">r.</tei:hi></tei:rs><tei:note xml:id="r0001-fn01"
      type="editorial" place="foot">Receipt of this picture postcard is recorded in Schenker's diary
      at OJ 4/5, p. 3718: "Von v. Hoboken (K. express): ob er morgen kommen kann." ("From v. Hoboken
      (express letter): could he come tomorrow?").</tei:note></tei:salute>
   </tei:opener>

   <tei:p rend="first">Falls ich keine gegenteilige Nachricht mehr von Ihnen erhalte, so werde ich
    mir erlauben, morgen, Freitag, zur gewohnten Stunde zu Ihnen zu kommen.<tei:note
     xml:id="r0001-fn02" type="editorial" place="foot">There are no Hoboken lessons recorded for
     April 1932 (indeed none for any of Schenker's pupils or the seminar).</tei:note></tei:p>

   <tei:closer>
    <tei:salute rend="indent-30">Mit bestem Gruss</tei:salute>
    <tei:salute rend="indent-70">Ihr</tei:salute>
    <tei:signed rend="indent-80"><tei:rs key="entity-000380" type="person"
     >AvHoboken</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>



  <tei:div type="translation">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:note type="editorial" place="inline">All in decorative rule-frame</tei:note><tei:lb/>
    <tei:note type="editorial" place="inline">small picture, caption: Trisanna Gorge,
     Tyrol</tei:note><tei:lb/>
    <tei:fw type="envelope"><tei:lb/>
     <tei:add resp="unknown"><tei:handShift new="#unk"/>Pneumatic post</tei:add><tei:lb/>
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity">To: </tei:supplied><tei:handShift new="#AvH"/>Dr.
       Heinrich Schenker</tei:addrLine>
      <tei:addrLine rend="indent-10">Vienna III</tei:addrLine>
      <tei:addrLine>Keilgasse 8</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark"> || 3 VIENNA 128 | 7.IV.32 1210 | * R * ||</tei:fw>
     <tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="2"/> VIENNA 50 |
      7.IV.32 12 | * R/a * ||</tei:fw>
     <tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="24"/> ||</tei:fw><tei:lb/>
     <tei:fw type="label"><tei:note type="editorial" place="inline">printed
       label:</tei:note><tei:handShift new="#printed"/>|| By special messenger | <tei:hi
       rend="sperr">Express</tei:hi> ||</tei:fw><tei:lb/><tei:lb/>
     <tei:milestone unit="folio" n="verso"/><tei:lb/>
     <tei:handShift new="#AvH"/><tei:date when="1932-04-07" rend="ident-70">April 7, 1932</tei:date>
    </tei:fw>
    <tei:salute rend="indent-10">Dear Dr. <tei:supplied reason="identity"><tei:rs type="person"
       key="entity-000632">Schenker</tei:rs></tei:supplied>,<tei:ptr corresp="#r0001-fn01"
      type="footnote"/></tei:salute>
   </tei:opener>

   <tei:p rend="first">If I hear nothing from you to the contrary, I will take the liberty of coming
    tomorrow, Friday, at the usual hour.<tei:ptr corresp="#r0001-fn02" type="footnote"/></tei:p>

   <tei:closer>
    <tei:salute rend="indent-30">With best greetings,</tei:salute>
    <tei:salute rend="indent-70">Your</tei:salute>
    <tei:signed rend="indent-80"><tei:rs key="entity-000380" type="person">A. v.
     Hoboken</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>
 </sdo:record>
