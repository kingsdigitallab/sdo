  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence">Schenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-004924">SchenkerM ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>


      <dc:title>Typewritten letter from Moriz Schenker to Heinrich Schenker, dated December 15,
        1922</dc:title>

      <dcterms:created>1922-12-15T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2024-02-10</dcterms:dateSubmitted>
      <dc:description>Moriz Schenker presents an account of investment sales, with enclosures, and
        reassures Heinrich that money is available on demand.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000774">Moriz Schenker</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>

    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>ITALO-WIENER CREDITBANK</tei:addrLine>
          <tei:addrLine rend="indent-10">DIREKTION</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:byline><tei:handShift new="#TPms"/>WO</tei:byline>
        <tei:address rend="indent-60">
          <tei:addrLine><tei:handShift new="#printed"/>>WIEN I.</tei:addrLine>
          <tei:addrLine>SCHOTTENRING 9.</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-60"><tei:handShift new="#TPms"/><tei:date when="1922-12-15">15.
            XII. 1922</tei:date>.</tei:dateline><tei:lb/>
        <tei:salute rend="indent-10">Lieber <tei:rs type="person" key="entity-000632"
            >Heinrich</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this letter is recorded in Heinrich’s diary for December 16, 1922: “Von Mozio (Br.)
            Rechnungsbelege.” (“From Mozio (letter) receipts”).</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Anbei die Abrechnungen über die verkauften: </tei:p>

      <tei:p>
        <tei:table rows="4" cols="5">
          <tei:row n="1">
            <tei:cell>Dollar 52<tei:note xml:id="r0001-fn02" type="editorial" place="foot">“52”:
                underlined by Schenker in crayon.</tei:note></tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>70.600</tei:cell>
            <tei:cell>K</tei:cell>
            <tei:cell>3,671.200.-</tei:cell>
          </tei:row>
          <tei:row n="2">
            <tei:cell>cz. Kr. 1200</tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>2.210</tei:cell>
            <tei:cell>K</tei:cell>
            <tei:cell>2,652.000.–</tei:cell>
          </tei:row>
          <tei:row n="3">
            <tei:cell>sch.F 205</tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>13.345</tei:cell>
            <tei:cell>K</tei:cell>
            <tei:cell>2,735.725.–</tei:cell>
          </tei:row>
          <tei:row n="4">
            <tei:cell>Dollar 8<tei:note xml:id="r0001-fn03" type="editorial" place="foot">“8”:
                written in and underlined by Schenker in crayon.</tei:note></tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>7.060</tei:cell>
            <tei:cell>K</tei:cell>
            <tei:cell>564.800.–</tei:cell>
          </tei:row>
        </tei:table>
      </tei:p>

      <tei:p rend="first">und eine Bestätigung.– Solltest Du irgendwelche Beträge in der
        Zwischenzeit benötigen, so kannst Du jederzeit über jeden Betrag sofort verfügen.<tei:note
          xml:id="r0001-fn04" type="editorial" place="foot">“kannst ... verfügen” (“you can .....”)
          underlined and sidelined by Schenker in crayon.</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">Euch Beiden herzliche Grüsse <tei:handShift new="#MS"/>u.
          Küsse</tei:salute>
        <tei:salute rend="indent-70"><tei:handShift new="TPms"/>Euer</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#MS"/><tei:rs type="person"
            key="entity-000774">Moriz</tei:rs></tei:signed><tei:note xml:id="r0001-fn05"
          type="editorial" place="foot">Below Moriz’s signature Schenker writes in crayon:
          “9593.725.”</tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPms"/>Herrn Professor Dr. Heinrich <tei:hi
              rend="underline">SCHENKER</tei:hi>,</tei:addrLine>
          <tei:addrLine><tei:hi rend="underline">WIEN III</tei:hi></tei:addrLine>
          <tei:addrLine>Keilgasse 8.</tei:addrLine>
        </tei:address>
      </tei:closer>

      <tei:postscript>
        <tei:p>Mit Beilagen</tei:p>
      </tei:postscript>


    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>ITALO-WIENER CREDITBANK</tei:addrLine>
          <tei:addrLine rend="indent-10">ADINISTRATION</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:byline><tei:handShift new="#TPms"/>WO</tei:byline>
        <tei:address rend="indent-60">
          <tei:addrLine><tei:handShift new="#printed"/>>VIENNA I</tei:addrLine>
          <tei:addrLine>SCHOTTENRING 9</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-60"><tei:handShift new="#TPms"/><tei:date when="1922-12-15"
            >December 15, 1922</tei:date></tei:dateline><tei:lb/>
        <tei:salute rend="indent-10">Dear <tei:rs type="person" key="entity-000632"
            >Heinrich</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">Enclosed are the accounts for the items sold:</tei:p>

      <tei:p>
        <tei:table rows="4" cols="5">
          <tei:row n="1">
            <tei:cell>Dollars 52<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>70.600</tei:cell>
            <tei:cell>Kr</tei:cell>
            <tei:cell>3,671.200.-</tei:cell>
          </tei:row>
          <tei:row n="2">
            <tei:cell>Czech Kr. 1200</tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>2.210</tei:cell>
            <tei:cell>Kr</tei:cell>
            <tei:cell>2,652.000.–</tei:cell>
          </tei:row>
          <tei:row n="3">
            <tei:cell>Swiss Fr. 205</tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>13.345</tei:cell>
            <tei:cell>Kr</tei:cell>
            <tei:cell>2,735.725.–</tei:cell>
          </tei:row>
          <tei:row n="4">
            <tei:cell>Dollars 8<tei:ptr corresp="#r0001-fn03" type="footnote"/></tei:cell>
            <tei:cell>à</tei:cell>
            <tei:cell>7.060</tei:cell>
            <tei:cell>Kr</tei:cell>
            <tei:cell>564.800.–</tei:cell>
          </tei:row>
        </tei:table>
      </tei:p>

      <tei:p rend="first">and a reassurance – Should you need any sums of money in the interim, you
        can at any time immediately demand each amount.<tei:ptr corresp="#r0001-fn04"
          type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">To both of you cordial greetings <tei:handShift new="#MS"/>and
          kisses</tei:salute>
        <tei:salute rend="indent-70"><tei:handShift new="TPms"/>Your</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#MS"/><tei:rs type="person"
            key="entity-000774">Moriz</tei:rs></tei:signed><tei:ptr corresp="#r0001-fn05"
          type="footnote"/><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPms"/>Professor Heinrich <tei:hi rend="underline"
              >SCHENKER</tei:hi></tei:addrLine>
          <tei:addrLine><tei:hi rend="underline">Vienna III</tei:hi></tei:addrLine>
          <tei:addrLine>Keilgasse 8</tei:addrLine>
        </tei:address>
      </tei:closer>

      <tei:postscript>
        <tei:p>With enclosures</tei:p>
      </tei:postscript>



    </tei:div>
  </sdo:record>
</sdo:recordCollection>
