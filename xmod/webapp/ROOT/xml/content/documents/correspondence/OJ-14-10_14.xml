      <dc:title>Handwritten picture postcard from Arnold &amp; Rosa Weil with Lisl, Anna &amp; Paul
        Schiff to Heinrich &amp; Jeanette Schenker, dated July 16, 1934</dc:title>

      <dcterms:created>1934-07-16T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2024-07-24</dcterms:dateSubmitted>
      <dc:description>Arnold and Rosa Weil, Lisl, Anna, and Paul Schiff, send best wishes from
        Karlsbad to Heinrich and Jeanette Schenker.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000944">Rosa Weil</marcrel:correspondent>
      <marcrel:correspondent key="entity-000943">Arnold Weil</marcrel:correspondent>
      <marcrel:correspondent key="entity-007617">Lisl Schiff</marcrel:correspondent>
      <marcrel:correspondent key="entity-000781">Anna Schiff</marcrel:correspondent>
      <marcrel:correspondent key="entity-000782">Paul Schiff</marcrel:correspondent>
      <marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:note type="editorial" place="pre-text">Receipt of this picture postcard is recorded in
        Heinrich’s diary for July 29, 1934: “Von Weil (Ansichtsk.): Gruß aus Karlsbad, auch von
        Paul, Anna, Lisl Schiff! datiert vom 16. VII.?” (“From Weil (picture postcard): greetings
        from Karlsbad, also from Paul, Anna, Lisl Schiff! dated July 16?”).</tei:note>

      <tei:div type="part_1">
        <tei:opener>
          <tei:milestone unit="folio" n="verso"/>
          <tei:note type="editorial" place="inline">picture: townscape with hills,
            captioned:”Karlsbad [–] Blick zum Hotel Imperial”</tei:note><tei:lb/>
          <tei:milestone unit="folio" n="recto"/><tei:lb/>
          <tei:fw type="envelope">
            <tei:address>
              <tei:addrLine><tei:supplied>Absender:</tei:supplied>
                <tei:handShift new="AW"/>Karslbad, Hotel Post</tei:addrLine>
              <tei:addrLine><tei:date when="1934-07-16">16. Juli 1934</tei:date>.</tei:addrLine>
            </tei:address><tei:lb/>
            <tei:address>
              <tei:addrLine><tei:supplied>An:</tei:supplied> Wohlgeboren Herrn &amp; Frau D<tei:hi
                  rend="supralinear">r.</tei:hi> Heinrich Schenker</tei:addrLine>
              <tei:addrLine><tei:handShift new="#RW"/>aus Wien</tei:addrLine>
              <tei:addrLine><tei:handShift new="#AW"/><tei:rs type="place" key="entity-003099"
                  >Böckstein bei Gastein</tei:rs></tei:addrLine>
              <tei:addrLine>Österreich</tei:addrLine>
            </tei:address>
            <tei:fw type="postmark">|| <tei:gap reason="illegible"/> | <tei:gap reason="illegible"/>
              A 36 | KARLSBAD ||</tei:fw><tei:lb/>
          </tei:fw>
        </tei:opener>

        <tei:p/>

        <tei:closer>
          <tei:salute>Herzlichste Grüße</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000943"
            >Arnold</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_2">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#RW"/>Viele Grüße beste Wünsche für Eueren
            Urlaub!!</tei:salute>
          <tei:salute rend="indent-50">Euere</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000944"
            >Rosa</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_3">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#LS"/>Herzliche Grüße</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-007617"
            >Lisl</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_4">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#AS"/>Viele Grüße</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000781"
            >Anna</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_5">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#PS"/>Beste Grüße</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000782"
            >Paul</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>


    </tei:div>



    <tei:div type="translation">

      <tei:div type="part_1">
        <tei:opener>
          <tei:milestone unit="folio" n="verso"/>
          <tei:note type="editorial" place="inline">picture: townscape with hills,
            captioned:”Karlsbad [–] View across the Hotel Imperial”</tei:note><tei:lb/>
          <tei:milestone unit="folio" n="recto"/><tei:lb/>
          <tei:fw type="envelope">
            <tei:address>
              <tei:addrLine><tei:supplied>From:</tei:supplied>
                <tei:handShift new="AW"/>Karslbad, Hotel Post</tei:addrLine>
              <tei:addrLine><tei:date when="1934-07-16">July 16, 1934</tei:date></tei:addrLine>
            </tei:address><tei:lb/>
            <tei:address>
              <tei:addrLine><tei:supplied>To:</tei:supplied> Dr. and Mrs. Heinrich
                Schenker</tei:addrLine>
              <tei:addrLine><tei:handShift new="#RW"/>of Vienna</tei:addrLine>
              <tei:addrLine><tei:handShift new="#AW"/><tei:rs type="place" key="entity-003099"
                  >Böckstein near Gastein</tei:rs></tei:addrLine>
              <tei:addrLine>Austria</tei:addrLine>
            </tei:address>
            <tei:fw type="postmark">|| <tei:gap reason="illegible"/> | <tei:gap reason="illegible"/>
              A 36 | KARLSBAD ||</tei:fw><tei:lb/>
          </tei:fw>
        </tei:opener>

        <tei:p/>

        <tei:closer>
          <tei:salute>Most cordial greetings,</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000943"
            >Arnold</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_2">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#RW"/>Cordial greetings and best wishes for your
            vacation!!</tei:salute>
          <tei:salute rend="indent-50">Your</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000944"
            >Rosa</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_3">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#LS"/>Cordial greetings</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-007617"
            >Lisl</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_4">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#AS"/>Ample greetings</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000781"
            >Anna</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

      <tei:div type="part_5">

        <tei:p/>

        <tei:closer>
          <tei:salute><tei:handShift new="#PS"/>Best greetings</tei:salute>
          <tei:signed rend="indent-60"><tei:rs type="person" key="entity-000782"
            >Paul</tei:rs></tei:signed>
        </tei:closer>
      </tei:div>

    </tei:div>

