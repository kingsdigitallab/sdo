  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-006258"
        >Rothschild~Cotta</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002748">Telegram</tei:rs>

      <dc:title>Telegram from Alphons von Rothschild to Cotta, dated November 23, 1906</dc:title>

      <dcterms:created>1906-11-23T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2017-06-29</dcterms:dateSubmitted>
      <dc:description>Rothschild has sent 4300.30 Marks.</dc:description>
      <dc:subject>Rechnung; account</dc:subject>

      <marcrel:correspondent key="entity-000743">ARothschild</marcrel:correspondent>
      <marcrel:recipient key="entity-002405">Cotta</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:note type="editorial" place="inline"><tei:hi rend="bold">Note:</tei:hi> Roman = printed;
          <tei:hi rend="italic">italic</tei:hi> = handwritten; <tei:hi rend="bold">bold
          Roman</tei:hi> = teletype-printed; [/] = new line</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:note type="editorial">top-left panel:</tei:note>
      <tei:note xml:id="r0001-fn01" type="editorial" place="foot">The previous day, Schenker's diary
        (OJ 1/5, p. 27) records receipt of a letter from Rothschild: "Von R. 4300 M. erhalten, bzw.
        eigenhändigen Brief über Absendung der Summe." ("4,300 marks received from Rothschild, as
        well as a letter in his own hand about the sending of the money.").</tei:note>

      <tei:ab type="formpart" rend="printed">No. 494 [/] Leitung Nr. 4 [/] Telegramm Nr. ....
        <tei:lb/>Aufgenommen von Innsbruck [/] den <tei:hi rend="italic">23</tei:hi> | <tei:hi
          rend="italic">11</tei:hi> um <tei:hi rend="italic">10</tei:hi> Uhr <tei:hi rend="italic"
          >20</tei:hi> M. [/] durch <tei:gap unit="chars" extent="5" reason="illegible"/>
      </tei:ab>
      <tei:note type="editorial" place="inline">top-center panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed"><tei:hi rend="bold">j g cottasche buchhandlung
          stuttgart = </tei:hi> [/] Stuttgart [/] Telegramm [/] Amt Stuttgart</tei:ab>
      <tei:note type="editorial" place="inline">top-right panel: left blank</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:note type="editorial" place="inline">middle panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed">Telegramm aus <tei:hi rend="bold">fr wien 6663 24 23 10
          m =</tei:hi> ...W. 190 ... den ... ten ... um ... Uhr ... Min. ...</tei:ab>
      <tei:note type="editorial" place="inline">bottom panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed"><tei:hi rend="bold">sende ihnen mit heutiger post fuer
          rechnung dr heinrich schenker einen check ueber viertausenddreihundert mark 30 pfennig =
            <tei:rs type="person" key="entity-000743">rothschild</tei:rs> ="=</tei:hi><tei:note
          xml:id="r0001-fn02" type="editorial" place="foot">This is the amount for which Schenker
          was invoiced by Cotta on November 10 (<tei:ref type="correspondence" cRef="CA-54-55">CA
            54–55</tei:ref>) for the publication costs of <tei:rs type="work" key="entity-001724"
              ><tei:title level="m">Harmonielehre</tei:title></tei:rs>, and which Alphons von
          Rothschild had guaranteed to pay. </tei:note></tei:ab>
    </tei:div>


    <tei:div type="translation">
      <tei:note type="editorial" place="inline"><tei:hi rend="bold">Note:</tei:hi> Roman = printed;
          <tei:hi rend="italic">italic</tei:hi> = handwritten; <tei:hi rend="bold">bold
          Roman</tei:hi> = teletype-printed; [/] = new line</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed">
        <tei:note type="editorial">top-left panel:</tei:note>
        <tei:ptr corresp="#r0001-fn01" type="footnote"/><tei:lb/> No. 494 [/] Line No. 4 [/]
        Telegram No. .... <tei:lb/>Received from Innsbruck [/] <tei:hi rend="italic">23</tei:hi> |
          <tei:hi rend="italic">11</tei:hi> at <tei:hi rend="italic">10.20</tei:hi> [/] via <tei:gap
          unit="chars" extent="5" reason="illegible"/>
      </tei:ab>

      <tei:ab type="formpart" rend="printed">
        <tei:note type="editorial" place="inline">top-center panel:</tei:note>
        <tei:lb/>
        <tei:hi rend="bold">j g cotta book dealership stuttgart = </tei:hi> [/] Stuttgart [/]
        Telegram [/] Stuttgart Office</tei:ab>
      <tei:note type="editorial" place="inline">top-right panel: left blank</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:note type="editorial" place="inline">middle panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed">Telegram from <tei:hi rend="bold">fr vienna 6663 24 23
          10 m =</tei:hi> W.190 .. the .. at ... hr ... min. ...</tei:ab>
      <tei:note type="editorial" place="inline">bottom panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed"><tei:hi rend="bold">>am sending you by today’s post to
          the account of dr heinrich schenker a check for four thousand three hundred marks 30
          pfennigs = <tei:rs type="person" key="entity-000743">rothschild</tei:rs>
          =“=</tei:hi><tei:ptr corresp="#r0001-fn02" type="footnote"/></tei:ab>
    </tei:div>
  </sdo:record>
