  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs key="entity-005444" type="correspondence" role="correspondence">Mandruck ~ HSchenker</tei:rs>
      <tei:rs key="entity-002744" type="corr-item-type" role="corr-item-type">Letter</tei:rs>

      <dc:title>Typed letter from Mandruck to Schenker, dated May 3, 1927</dc:title>
      <dcterms:created>1927-05-03T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-27</dcterms:dateSubmitted>
      <dc:description>Mandruck return the original sketches of Haydn's "Representation of Chaos" to
        Schenker and inform him that they have prepared the stereotype in accordance with his
        wishes.</dc:description>
      <dc:subject>original; Vorlag; stereotype; Klischee</dc:subject>

    <marcrel:correspondent key="entity-003129">Mandruck</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK AG</tei:addrLine>
          <tei:addrLine>MÜNCHEN</tei:addrLine>
          <tei:addrLine>THERESIENSTRASSE 75</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:byline><tei:handShift new="#TPhr"/><tei:hi rend="italic">H./R.</tei:hi></tei:byline>
        <tei:dateline rend="indent-40"><tei:placeName>München</tei:placeName>, den <tei:date when="1927-05-03">3. Mai 1927</tei:date>.</tei:dateline>
        <tei:address>
          <tei:lb/>
          <tei:addrLine rend="indent-10">Herrn Dr. Heinrich <tei:hi rend="sperr">Schenker</tei:hi>,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
              of this letter is recorded in Schenker's diary at OJ 3/9, p. 3063, May 4, 1927: "Vom
              Verlag (Br.): die dem Archiv gehörige Haydn-Skizze zurück." ("From the publisher
              (letter): the Haydn sketch belonging to the Archive
            returned.").</tei:note></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:hi rend="underline"><tei:hi rend="sperr">Wien</tei:hi>
              III.</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70">Keilgasse 8.</tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/><tei:lb/>

      <tei:p rend="first">Im Auftrag des <tei:rs key="entity-002627" type="organization">Drei Masken
          Verlages</tei:rs>, hier, überreichen wir Ihnen in der Anlage die beiden Vorlagen zu
          <tei:rs key="entity-000344" type="person">Haydn</tei:rs>, <tei:title>Vorstellung des
          Chaos</tei:title> (<tei:rs key="entity-001551" type="composition"><tei:title>Schöpfung</tei:title></tei:rs>) zurück. Das gewünschte Klischee haben wir
        angefertigt, welches im <tei:rs key="entity-001751" type="work">Schenker-Jahrbuch</tei:rs>
        Verwendung findet.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Hochachtungsvoll.</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129" type="organization">MANDRUCK A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:lb/><tei:lb/>
        <tei:p><tei:handShift new="#TPhr"/>2 Beilagen.</tei:p>
      </tei:postscript>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK &amp; Co.</tei:addrLine>
          <tei:addrLine>MUNICH</tei:addrLine>
          <tei:addrLine>THERESIENSTRAßE 75</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:byline><tei:handShift new="#TPhr"/><tei:hi rend="italic">H./R.</tei:hi></tei:byline>
        <tei:dateline rend="indent-40"><tei:placeName>Munich</tei:placeName>, <tei:date when="1927-05-03">May 3, 1927</tei:date>.</tei:dateline>
        <tei:address>
          <tei:lb/>
          <tei:addrLine rend="indent-10">Dr. Heinrich <tei:hi rend="sperr">Schenker</tei:hi>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:hi rend="underline"><tei:hi rend="sperr">Vienna</tei:hi> III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70">Keilgasse 8.</tei:addrLine>
        </tei:address>
      </tei:opener>
      
      <tei:lb/><tei:lb/>

      <tei:p rend="first"> On behalf of <tei:rs key="entity-002627" type="organization">Drei Masken
          Verlag</tei:rs>, we hereby return to you the two originals for <tei:rs key="entity-000344" type="person">Haydn’s</tei:rs> "<tei:title>Representation of Chaos</tei:title>" (from
          <tei:rs key="entity-001551" type="composition"><tei:title level="m">The
            Creation</tei:title></tei:rs>), which are enclosed. We have prepared the stereotype that you wish,
        which will be used in the <tei:rs key="entity-001751" type="work">Schenker
        Yearbook</tei:rs>.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Respectfully,</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129" type="organization">MANDRUCK &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:lb/><tei:lb/>
        <tei:p><tei:handShift new="#TPhr"/>2 enclosures.</tei:p>
      </tei:postscript>

    </tei:div>
  </sdo:record>
