  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002726">DeutschOE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002712">DeutschOE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002727">DeutschOE+H ~
        H+JSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002751">Picture
        postcard</tei:rs>

      <dc:title>Typewritten picture postcard from Deutsch to Schenker, dated March 5,
        1932</dc:title>

      <dcterms:created>1932-03-05T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-03</dcterms:dateSubmitted>
      <dc:description>Deutsch suggests visiting Schenker on Tuesday, March 9.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000164">DeutschOE</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>

    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/><tei:hi rend="bold">Postkarte</tei:hi></tei:fw>
        <tei:lb/>
        <tei:note type="editorial" place="inline">picture: village, church, with mountain peak,
          captioned: “Leermoos – Tirol”</tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied>
            <tei:handShift new="#TPoed"/>Herrn Dr. H. Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40">Keilgasse 8</tei:addrLine>
          <tei:addrLine rend="indent-60">Wien III.</tei:addrLine>

        </tei:address>
        <tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-40"><tei:handShift new="#TPoed"/>Wien, <tei:date
            when="1932-03-05">5. März 32</tei:date></tei:dateline>
        <tei:salute>Lieber <tei:rs type="person" key="entity-000632">Verehrter</tei:rs>!<tei:note
            xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this postcard is recorded
            in Schenker’s diary for March 5, 1932: “Von Deutsch (K.): kommt Dienstag.” (“From
            Deutsch (postcard): he will come on Tuesday.”).</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Wenn es Ihnen also recht ist, komme ich <tei:date when="1932-03-09"
          >Dienstag</tei:date> nachmittags. Bitte um die Stunde.<tei:note xml:id="r0001-fn02"
          type="editorial" place="foot">No meeting with Deutsch is recorded in Schenker’s diary for
          March 9 or the surrounding days.</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Schönste Grüsse!</tei:salute>
        <tei:salute rend="indent-70">Ihr ganz ergebener</tei:salute>
        <tei:signed rend="indent-90"><tei:handShift new="#OED"/><tei:rs type="person"
            key="entity-000164">O E Deutsch</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/><tei:hi rend="bold">Postcard</tei:hi></tei:fw>
        <tei:lb/>
        <tei:note type="editorial" place="inline">picture: village, church, with mountain peak,
          captioned: “Leermoos – Tyrol”</tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied>
            <tei:handShift new="#TPoed"/>Dr. H. Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40">Keilgasse 8</tei:addrLine>
          <tei:addrLine rend="indent-60">Vienna III</tei:addrLine>

        </tei:address>
        <tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-40"><tei:handShift new="#TPoed"/>Vienna, <tei:date
            when="1932-03-05">March 5, 1932</tei:date></tei:dateline>
        <tei:salute>Dear revered <tei:rs type="person" key="entity-000632">friend</tei:rs>,<tei:ptr
            corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">If it is okay with you, then, I shall come on <tei:date when="1932-03-09"
          >Tuesday</tei:date> afternoon. Please let me know what time<tei:ptr corresp="#r0001-fn02"
          type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Best greetings!</tei:salute>
        <tei:salute rend="indent-70">Your wholly devoted</tei:salute>
        <tei:signed rend="indent-90"><tei:handShift new="#OED"/><tei:rs type="person"
            key="entity-000164">O. E. Deutsch</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>

  </sdo:record>
