  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs type="correspondence" role="correspondence" key="entity-005439">Waldheim-Eberle ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003349">Invoice</tei:rs>

      <dc:title>Typewritten Invoice from Waldheim to Schenker, dated July 25, 1932</dc:title>
      <dcterms:created>1932-07-25T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-15</dcterms:dateSubmitted>

      <dc:description>Waldheim-Eberle submits its final invoice for the Fünf
        Urlinie-Tafeln.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002402">Waldheim-Eberle</marcrel:correspondent>
      <marcrel:correspondent key="entity-008404">Rudolf Kindler</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:note type="editorial" place="inline">printer's emblem with
              "WEAG"</tei:note></tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="bold">WALDHEIM-EBERLE
              A.G.</tei:hi></tei:addrLine>
          <tei:addrLine>GROSSDRUCKEREI UND VERLAG</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">WIEN VII. SEIDENGASSE NR. 3–11</tei:hi></tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-70">
          <tei:placeName>Wien</tei:placeName>, <tei:handShift new="#TPwe"/><tei:date
            when="1932-07-25">25. Juli 1932</tei:date>.</tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>FAKTURA für <tei:handShift new="#TPwe"/>Herrn
            Prof. Dr. Heinrich <tei:hi rend="sperr">Schenker</tei:hi>,</tei:addrLine>
          <tei:addrLine rend="indent-65">
            <tei:hi rend="underline"><tei:hi rend="sperr">Igls</tei:hi> b.
            Innsbruck</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70">Iglerhof.</tei:addrLine>
        </tei:address>
        <tei:lb/>
      </tei:opener>

      <tei:p>AUF IHRE RECHNUNG UND GEFAHR – ZAHLBAR UND KLAGBAR IN WIEN</tei:p>

      <tei:p>je 1000 Thementafeln No. 1/11.<tei:note xml:id="r0001-fn01" type="editorial"
          place="foot">Receipt of this invoice is recorded in Schenker’s diary for August 4, 1932:
          “Von Waldheim die Rechnung.” (“From Waldheim, the invoice.”). — The diary for July 29
          records: “ Von Waldheim (K.): haben die Exemplare nach Wunsch versendet.” (“From Waldheim
          (postcard): they have sent off the copies as requested.”), and for August 6: “An Waldheim:
          S. 1140.30 – auf Nimmerwiedersehen!” (“To Waldheim: 1,140.30 shillings – never to be seen
          again!”).</tei:note></tei:p>

      <tei:p><tei:table rows="8" cols="3">

          <tei:row role="label" n="1">
            <tei:cell role="data"/>
            <tei:cell role="data">Preis</tei:cell>
            <tei:cell role="data">Betrag</tei:cell>
          </tei:row>

          <tei:row role="data" n="2">
            <tei:cell role="data">laut Offert </tei:cell>
            <tei:cell role="data">S 800.–</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="3">
            <tei:cell role="data">1000 Umschläge </tei:cell>
            <tei:cell role="data">S 170.–</tei:cell>
            <tei:cell role="data"/>
          </tei:row>
          <tei:row role="data" n="4">
            <tei:cell role="data">Frachtspesen f. 4 Kisten inkl.</tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="5">
            <tei:cell role="data">Emballage, Verpackung &amp; Anfertigung </tei:cell>
            <tei:cell role="data">S 160.–</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="6">
            <tei:cell role="data">Frankatur &amp; Emballg. f. 11 ½ Bd</tei:cell>
            <tei:cell role="data">S --9.82</tei:cell>
            <tei:cell role="data">S 1139.82</tei:cell>
          </tei:row>

          <tei:row role="data" n="7">
            <tei:cell role="data">Stempel</tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data">S --.50</tei:cell>
          </tei:row>

          <tei:row role="data" n="8">
            <tei:cell role="data">Summe</tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data">S 1140.32</tei:cell>
          </tei:row>

        </tei:table></tei:p>

      <tei:closer>
        <tei:salute rend="indent-35">hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-30"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-40"><tei:handShift new="#stamp"/>Musikabteilung</tei:signed>
        <tei:signed rend="indent-50"><tei:handShift new="#RK"/>ppa. <tei:rs type="person"
            key="entity-008404">Kindler</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:note type="editorial" place="inline">printer's emblem with
              "WEAG"</tei:note></tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="bold">WALDHEIM-EBERLE &amp;
              CO.</tei:hi></tei:addrLine>
          <tei:addrLine>MAJOR PRINTING HOUSE AND PUBLISHER</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">VIENNA VII, SEIDENGASSE 3–11</tei:hi></tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-70">
          <tei:placeName>Vienna</tei:placeName>, <tei:handShift new="#TPwe"/><tei:date
            when="1932-07-25">July 25, 1932</tei:date></tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>INVOICE for <tei:handShift new="#TPwe"/>Prof.
            Heinrich <tei:hi rend="sperr">Schenker</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-65">
            <tei:hi rend="underline"><tei:hi rend="sperr">Igls</tei:hi> nr
            Innsbruck</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70">Iglerhof</tei:addrLine>
        </tei:address>
        <tei:lb/>
      </tei:opener>

      <tei:p>AT YOUR ACCOUNT AND OWN RISK – PAYABLE AND ACTIONABLE IN VIENNA</tei:p>

      <tei:p>each 1,000 thematic graphs No. 1/11.<tei:ptr corresp="#r0001-fn01" type="footnote"
        /></tei:p>

      <tei:p><tei:table rows="8" cols="3">

          <tei:row role="label" n="1">
            <tei:cell role="data"/>
            <tei:cell role="data">Price</tei:cell>
            <tei:cell role="data">Amount</tei:cell>
          </tei:row>

          <tei:row role="data" n="2">
            <tei:cell role="data">as per offer</tei:cell>
            <tei:cell role="data">S 800.–</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="3">
            <tei:cell role="data">1,000 wrappers </tei:cell>
            <tei:cell role="data">S 170.–</tei:cell>
            <tei:cell role="data"/>
          </tei:row>
          <tei:row role="data" n="4">
            <tei:cell role="data">Freight costs for 4 crates including</tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="5">
            <tei:cell role="data">wrapping, packing, and production</tei:cell>
            <tei:cell role="data">S 160.–</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="6">
            <tei:cell role="data">Postage and packing. for 11½ vols</tei:cell>
            <tei:cell role="data">S --9.82</tei:cell>
            <tei:cell role="data">S 1,139.82</tei:cell>
          </tei:row>

          <tei:row role="data" n="7">
            <tei:cell role="data">Stamp</tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data">S --.50</tei:cell>
          </tei:row>

          <tei:row role="data" n="8">
            <tei:cell role="data">Total</tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data">S 1,140.32</tei:cell>
          </tei:row>

        </tei:table></tei:p>

      <tei:closer>
        <tei:salute rend="indent-35">with kind regards,</tei:salute>
        <tei:signed rend="indent-30"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-40"><tei:handShift new="#stamp"/>Music Division</tei:signed>
        <tei:signed rend="indent-50"><tei:handShift new="#RK"/>ppa. <tei:rs type="person"
            key="entity-008404">Kindler</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>
