    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:rs type="person" key="entity-000307"
                ><tei:hi rend="bold">Dr. Max Gross</tei:hi></tei:rs></tei:addrLine>
          <tei:addrLine>Hof- und Gerichts Advokat</tei:addrLine>
          <tei:addrLine rend="indent-10">Telephon 7125.</tei:addrLine>
          <tei:addrLine>Postsparkassen-Konto Nr. 82,294.</tei:addrLine>
          <tei:addrLine>Persönliche Sprechstunde:</tei:addrLine>
          <tei:addrLine>Nachmittags ½4–6 Uhr.</tei:addrLine>
        </tei:address>
        <tei:address rend="indent-60">
          <tei:addrLine><tei:placeName>Wien</tei:placeName>, am <tei:date when="1915-01-18"
                ><tei:handShift new="#TPmg"/>18. Jänner 1915</tei:date>.</tei:addrLine>
          <tei:addrLine>1., Singerstraße Nr. 14, Mezzanin.</tei:addrLine>
        </tei:address>
        <tei:address>
          <tei:addrLine rend="indent-20">Herrn Dr. Heinrich <tei:hi rend="underline"
              >Schenker</tei:hi>, WIEN, III.,</tei:addrLine>
          <tei:addrLine rend="indent-70">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">Unten gestatte ich mir Ihnen meine Expensennote zu übersenden<tei:note
          xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this letter is recorded in
          Schenker’s diary for January 19, 1915: “Expensennote von Dr. Groß wegen Mošio.” (“Note of
          expenses from Dr. Groß with regard to Mozio.”). — The troubles over financial support for
            <tei:rs type="person" key="entity-000773">Julia Schenker</tei:rs>, including
          difficulties with <tei:rs type="person" key="entity-000775">Wilhelm Schenker</tei:rs>, and
          involving Schenker’s sister, <tei:rs type="person" key="entity-000319">Sophie
            Guttmann</tei:rs>, can be followed in Schenker’s diary between January 5 and 28, 1915. See in
          particular <tei:ref type="correspondence" cRef="OJ-11-26_5">OJ 11/26, [5]</tei:ref>,
          January 9, 1914.</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">und zeichne</tei:salute>
        <tei:salute rend="indent-70">hochachtungsvoll:</tei:salute>
      </tei:closer>

      <tei:postscript>
        <tei:table rows="11" cols="4">

          <tei:row role="label" n="1">
            <tei:cell role="data"/>
            <tei:cell role="data"><tei:handShift new="#TPmg"/><tei:hi rend="underline"
                >EXPENSENNOTE</tei:hi></tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data"/>
          </tei:row>
          <tei:row role="label" n="2">
            <tei:cell role="label">1914.</tei:cell>
            <tei:cell role="label"><tei:hi rend="underline">In Sachen <tei:rs type="person"
                  key="entity-000774">MORITZ SCHENKER</tei:rs></tei:hi>:</tei:cell>
            <tei:cell role="label"/>
            <tei:cell role="label"/>
          </tei:row>

          <tei:row role="data" n="3">
            <tei:cell role="data">Jänn. 9.</tei:cell>
            <tei:cell role="data">Information aus den Schreiben des Herrn <tei:rs type="person"
                key="entity-000632"><tei:lb/>Dr. Schenker</tei:rs></tei:cell>
            <tei:cell role="data"/>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="4">
            <tei:cell role="data">Jänn. 9.</tei:cell>
            <tei:cell role="data">Schreiben an Herrn <tei:rs type="person" key="entity-000774"
                >Schenker</tei:rs> wegen Bezahlung<tei:lb/><tei:pb n="2"/> samt Porto:</tei:cell>
            <tei:cell role="data">K --,50</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="5">
            <tei:cell role="data">Jänn 9.</tei:cell>
            <tei:cell role="data">Schreiben an Herrn <tei:rs type="person" key="entity-000632">Dr.
                Schenker</tei:rs> mit Einsen-<tei:lb/>dung einer Abschrift samt Port</tei:cell>
            <tei:cell role="data">K6.--</tei:cell>
            <tei:cell role="data">K--.10</tei:cell>
          </tei:row>

          <tei:row role="data" n="6">
            <tei:cell role="data"/>
            <tei:cell role="data">Summe</tei:cell>
            <tei:cell role="data">K6.60</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="label" n="7">
            <tei:cell role="label">1915.</tei:cell>
            <tei:cell role="label"/>
            <tei:cell role="label"/>
            <tei:cell role="label"/>
          </tei:row>


          <tei:row role="data" n="8">
            <tei:cell role="data">Jänn 18.</tei:cell>
            <tei:cell role="data">Porto für Note und Erlagschein</tei:cell>
            <tei:cell role="data">K--.13</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="9">
            <tei:cell role="data"/>
            <tei:cell role="data"/>
            <tei:cell role="data">----</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="10">
            <tei:cell role="data"/>
            <tei:cell role="data"/>
            <tei:cell role="data">K6.73</tei:cell>
            <tei:cell role="data"/>
          </tei:row>

          <tei:row role="data" n="11">
            <tei:cell role="data"/>
            <tei:cell role="data"/>
            <tei:cell role="data">====</tei:cell>
            <tei:cell role="data"/>
          </tei:row>
