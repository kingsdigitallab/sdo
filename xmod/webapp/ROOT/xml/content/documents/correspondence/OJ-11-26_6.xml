      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:rs type="person" key="entity-000307"
                ><tei:hi rend="bold">Dr. Max Gross</tei:hi></tei:rs></tei:addrLine>
          <tei:addrLine>Court and Justice Advocate</tei:addrLine>
          <tei:addrLine rend="indent-10">Telephone 712</tei:addrLine>
          <tei:addrLine>Postal Savings Bank account No. 82,294</tei:addrLine>
          <tei:addrLine>Consultation hours:</tei:addrLine>
          <tei:addrLine>Afternoons 3.30–6 o’clock</tei:addrLine>
        </tei:address>
        <tei:address rend="indent-60">
          <tei:addrLine><tei:placeName>Vienna</tei:placeName>, <tei:date when="1915-01-18"
                ><tei:handShift new="#TPmg"/>January 18, 1915</tei:date>.</tei:addrLine>
          <tei:addrLine>I, Singerstraße 14, mezzanine</tei:addrLine>
        </tei:address>
        <tei:address>
          <tei:addrLine rend="indent-20">Dr. Heinrich <tei:hi rend="underline">Schenker</tei:hi>,
            VIENNA III</tei:addrLine>
          <tei:addrLine rend="indent-70">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">Below, I take the liberty of sending you my note of expenses,<tei:ptr
          corresp="#r0001-fn01" type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">and remain</tei:salute>
        <tei:salute rend="indent-70">respectfully:</tei:salute>
      </tei:closer>

