  <sdo:record ID="r0001">
    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-008496">KuxW ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003332">calling card</tei:rs>


      <dc:title>Handwritten calling card from Kux to Schenker, dated February 13, 1907[?]</dc:title>
      <dcterms:created>1907-02-13T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2025-02-14</dcterms:dateSubmitted>
      <dc:description>Kux asks Schenker to visit him the following day.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000498">Wilhelm Kux</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>

    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:dateline>13/2 190<tei:subst>
            <tei:del rend="overwrittem">5</tei:del>
            <tei:add place="superimposed">7</tei:add>
          </tei:subst><tei:note xml:id="r0001-fn01" type="editorial" place="foot">It is unclear
            whether “5” is overwritten by “7” or <tei:hi rend="italic">vice versa</tei:hi>; thus the
            date of this item could be February 13, 1905. Neither date can be corroborated. —
            Receipt of this calling card is not recorded in Schenker’s
          diary.</tei:note></tei:dateline>
      </tei:opener>

      <tei:p><tei:handShift new="#pr"/><tei:rs type="person" key="entity-000498">Wilhelm
        Kux</tei:rs><tei:lb/> Director der Niederösterreichischen Escompte Gesellschaft<tei:lb/>
        <tei:handShift new="#WK"/>bittet Sie, lieber <tei:rs type="person" key="entity-000632"
          >Doctor</tei:rs>, ihn morgen im Laufe des Tages behufs kurzer Rücksprache zu
        besuchen.</tei:p>

      <tei:closer>
        <tei:salute>Beste Grüße!</tei:salute>
      </tei:closer>
      <tei:lb/>
      <tei:lb/>
      <tei:milestone unit="folio" n="verso"/>
      <tei:note type="editorial" place="inline">blank</tei:note>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:dateline>February 13, 190<tei:subst>
            <tei:del rend="overwrittem">5</tei:del>
            <tei:add place="superimposed">7</tei:add>
          </tei:subst><tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:dateline>
      </tei:opener>

      <tei:p><tei:handShift new="#pr"/><tei:rs type="person" key="entity-000498">Wilhelm
        Kux</tei:rs><tei:lb/> Director of the Lower Austrian Discount Bank<tei:lb/>
        <tei:handShift new="#WK"/>requests you, dear <tei:rs type="person" key="entity-000632"
          >Doctor</tei:rs>, to visit him tomorrow during the day for a brief consultation.</tei:p>

      <tei:closer>
        <tei:salute>Best greetings! </tei:salute>
      </tei:closer>
      <tei:lb/>
      <tei:lb/>
      <tei:milestone unit="folio" n="verso"/>
      <tei:note type="editorial" place="inline">blank</tei:note>

    </tei:div>

