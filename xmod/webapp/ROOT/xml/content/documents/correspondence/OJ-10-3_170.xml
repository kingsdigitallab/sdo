  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002726">DeutschOE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002712">DeutschOE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002727">DeutschOE+H ~
        H+JSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Handwritten postcard from Deutsch to Schenker, postmarked May 23, 1932</dc:title>

      <dcterms:created>1932-05-17T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-04</dcterms:dateSubmitted>
      <dc:description>Deutsch agrees to meet Schenker at the Café Aspang on May 25.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000164">DeutschOE</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>

    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/><tei:hi rend="bold">Postkarte</tei:hi></tei:fw>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>Absender: <tei:note type="editorial"
              place="inline">blank</tei:note></tei:addrLine>
        </tei:address>
        <tei:address>
          <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied>
            <tei:handShift new="#OED"/>Herrn Dr. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40">Keilgasse 8</tei:addrLine>
          <tei:addrLine rend="indent-60">Wien III.</tei:addrLine>
        </tei:address>
        <tei:lb/><tei:fw type="postmark">|| 3-WIEN 40 | 23. V. 32 13 | * 44 * ||</tei:fw><tei:lb/>
        <tei:salute>Lieber <tei:rs type="person" key="entity-000632">Verehrter</tei:rs>!<tei:note
            xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this postcard is recorded
            in Schenker’s diary for May 24, 1932: “Von Deutsch: wird morgen kommen.” (“From Deutsch: he
            will come tomorrow.”).</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Ich komme gerne <tei:date when="1932-05-25">Mittwoch, den 25.</tei:date>,
        nach <tei:rs type="place" key="entity-002259">Aspang</tei:rs>.<tei:note xml:id="r0001-fn02"
          type="editorial" place="foot">Schenker’s diary for May 25, 1932 records: “Von 5–½8<tei:hi
            rend="supralinear">h</tei:hi> mit Deutsch im Café Aspang.” (“From 5 to 7:30, with
          Deutsch at the Café Aspang.”).</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">Schönstens empfohlen</tei:salute>
        <tei:salute rend="indent-60">Ihr</tei:salute>
        <tei:signed rend="indent-70"><tei:rs type="person" key="entity-000164">O E
          Deutsch</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:p><tei:milestone unit="folio" n="verso"/>
          <tei:note type="editorial" place="inline">blank</tei:note></tei:p>
      </tei:postscript>

    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/><tei:hi rend="bold">Postcard</tei:hi></tei:fw>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>From: <tei:note type="editorial"
              place="inline">blank</tei:note></tei:addrLine>
        </tei:address>
        <tei:address>
          <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied>
            <tei:handShift new="#OED"/>Dr. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40">Keilgasse 8</tei:addrLine>
          <tei:addrLine rend="indent-60">Vienna III</tei:addrLine>
        </tei:address>
        <tei:lb/><tei:fw type="postmark">|| 3-VIENNA 40 | 23. V. 32 13 | * 44 * ||</tei:fw><tei:lb/>
        <tei:salute>Dear revered <tei:rs type="person" key="entity-000632">friend</tei:rs>,<tei:ptr
            corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">I shall gladly come to the <tei:rs type="place" key="entity-002259">Café
          Aspang</tei:rs> on <tei:date when="1932-05-25">Wednesday, the 25th</tei:date>.<tei:ptr
          corresp="#r0001-fn02" type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">With best regards,</tei:salute>
        <tei:salute rend="indent-60">Your</tei:salute>
        <tei:signed rend="indent-70"><tei:rs type="person" key="entity-000164">O. E.
            Deutsch</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:p><tei:milestone unit="folio" n="verso"/>
          <tei:note type="editorial" place="inline">blank</tei:note></tei:p>
      </tei:postscript>

    </tei:div>

  </sdo:record>
