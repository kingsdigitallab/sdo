  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002700">Hertzka ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">postcard</tei:rs>



      <dc:title>Typewritten postcard from UE (unidentified signatory) to Schenker, dated September
        20, 1909</dc:title>

      <dcterms:created>1909-09-20T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2016-09-15</dcterms:dateSubmitted>
      <dc:description>Hertzka is currently away.</dc:description>
      <dc:subject>Rückkehr; return</dc:subject>

      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:correspondent key="entity-000356">Emil Hertzka</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:supplied reason="format">all in decorative rule-frame</tei:supplied>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="italic">An</tei:hi>
            <tei:handShift new="#TPue"/>Wohlgeboren Herrn Professor Dr. Heinr.
            Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:handShift new="#printed"/><tei:hi rend="italic"
              >in</tei:hi>
            <tei:handShift new="#TPue"/><tei:hi rend="underline sperr">WIEN</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-40">III. Reisnerstraße 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 1/1 WIEN <tei:gap unit="chars" extent="2" reason="illegible"/> |
          20 <tei:gap unit="chars" extent="6" reason="illegible"/> | <tei:gap unit="chars"
            extent="6" reason="illegible"/> ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:hi rend="underline">Wohlgeboren <tei:persName>Herrn Professor Dr. H.
                Schenker</tei:persName></tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-60">
            <tei:hi rend="underline sperr">Wien.</tei:hi></tei:addrLine>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs key="entity-000632" type="person"
            >Doktor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
            this postcard is not recorded in Schenker's diary. It is notable that the typewriter on
            which this was typed had the character "ß", and that "J" is used in the message in place
            of "I" even though the latter is used on the <tei:hi rend="italic"
            >recto</tei:hi>.</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Jm Besitze Jhrer gesch. Karte vom <tei:date when="1909-09-20">20. d.
          M.</tei:date><tei:note xml:id="r0001-fn02" type="editorial" place="foot">= <tei:ref
            type="correspondence" cRef="WSLB-42">WSLB 42</tei:ref>.</tei:note> teilen wir Jhnen
        höflichst mit, daß sich unser Herr Direktor <tei:rs key="entity-000356" type="person"
          >Hertzka</tei:rs> zur Zeit in <tei:rs key="entity-002286" type="place">München</tei:rs>
        befindet. Sofort nach seiner Rückkehr werden wir uns erlauben, Sie hie von zu
        verständigen.</tei:p>

      <tei:closer>
        <tei:note type="editorial" place="inline">right side:</tei:note>
        <tei:salute><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:signed><tei:rs key="entity-002408" type="organization">„Universal-Edition“
            A-G.</tei:rs><tei:lb/>(Jos. Aibl Verlag, G.m.b.H.)</tei:signed>
        <tei:signed><tei:handShift new="#unk"/><tei:note type="editorial" place="inline">illegible
            initials</tei:note></tei:signed><tei:lb/><tei:lb/>
        <tei:note type="editorial" place="inline">left side:</tei:note>
        <tei:dateline><tei:handShift new="#printed"/>Wien, am <tei:handShift new="#TPue"/>20.
          September <tei:handShift new="#printed"/>190<tei:handShift new="#TPue"/>9</tei:dateline>
        <tei:address>
          <tei:addrLine>I, Wipplingerstraße 32.</tei:addrLine>
        </tei:address>
      </tei:closer>


    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:supplied reason="format">all in decorative rule-frame</tei:supplied>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="italic">To:</tei:hi>
            <tei:handShift new="#TPue"/>Professor Heinr. Schenker, Esq.</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:handShift new="#printed"/><tei:hi rend="italic"
              >in:</tei:hi>
            <tei:handShift new="#TPue"/><tei:hi rend="underline sperr">VIENNA
            III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-40">Reisnerstraße 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 1/1 VIENNA <tei:gap unit="chars" extent="2" reason="illegible"/>
          | 20 <tei:gap unit="chars" extent="6" reason="illegible"/> | <tei:gap unit="chars"
            extent="6" reason="illegible"/> ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:hi rend="underline"><tei:persName>Professor H. Schenker</tei:persName>,
              Esq.</tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-60">
            <tei:hi rend="underline sperr">Vienna</tei:hi></tei:addrLine>
        </tei:address>
        <tei:salute>Dear Dr. <tei:supplied reason="identity"><tei:rs key="entity-000632"
              type="person">Schenker</tei:rs></tei:supplied>,<tei:ptr corresp="#r0001-fn01"
            type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">In receipt of your esteemed postcard of the <tei:date when="1909-09-20"
          >20th of the month</tei:date>,<tei:ptr corresp="#r0001-fn02" type="footnote"/> we would
        respectfully inform you that our Director <tei:rs key="entity-000356" type="person"
          >Hertzka</tei:rs> is currently away in <tei:rs key="entity-002286" type="place"
          >Munich</tei:rs>. We will take the liberty of notifying you the moment he returns.</tei:p>

      <tei:closer>
        <tei:note type="editorial" place="inline">right side:</tei:note>
        <tei:salute><tei:handShift new="#printed"/>With kind regards,</tei:salute>
        <tei:signed><tei:rs key="entity-002408" type="organization">Universal Edition &amp;
            Co.</tei:rs><tei:lb/>(Jos. Aibl Publishing, &amp; Co.)</tei:signed>
        <tei:signed><tei:handShift new="#unk"/><tei:note type="editorial" place="inline">illegible
            initials</tei:note></tei:signed><tei:lb/><tei:lb/>
        <tei:note type="editorial" place="inline">left side:</tei:note>
        <tei:dateline><tei:handShift new="#printed"/>Vienna, <tei:handShift new="#TPue"/>September
          20, <tei:handShift new="#printed"/>190<tei:handShift new="#TPue"/>9</tei:dateline>
        <tei:address>
          <tei:addrLine>I, Wipplingerstraße 32.</tei:addrLine>
        </tei:address>
      </tei:closer>


    </tei:div>
