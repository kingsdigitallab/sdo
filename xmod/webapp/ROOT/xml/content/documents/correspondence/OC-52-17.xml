  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-006164">WeinbergerJ ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>



      <dc:title>Typewritten letter from Weinberger (UE) to Schenker, dated March 11, 1905</dc:title>

      <dcterms:created>1905-03-11T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2016-02-22</dcterms:dateSubmitted>
      <dc:description>List of recipients of complimentary copies of the Handel Organ
        Concertos</dc:description>
      <dc:subject>Adressen; addresses</dc:subject>

      <marcrel:correspondent key="entity-000946">WeinbergerJ</marcrel:correspondent>
      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>Universal-Edition
            Actiengesellschaft</tei:addrLine>
          <tei:addrLine>Maximilianstrasse No.11.</tei:addrLine>
          <tei:addrLine>TELEPHON N<tei:hi rend="supralinear">o.</tei:hi> 3364</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50">
          <tei:placeName>Wien</tei:placeName>, <tei:handShift new="#TPue"/><tei:date
            when="1905-03-01">11. März 1905</tei:date>.</tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine rend="indent-10">
            <tei:persName>Hochwolgeboren Herrn Professor Dr. Heinrich Schenker,</tei:persName>
          </tei:addrLine>
          <tei:addrLine rend="indent-40">
            <tei:hi rend="sperr">WIEN</tei:hi>.</tei:addrLine>
        </tei:address>
        <tei:salute>Euer <tei:rs type="person" key="entity-000632"
          >Hochwolgeboren</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p rend="first">Wir teilen Ihnen höflichst mit, dass wir heute an die uns angegebenen
          Adressen:<tei:list type="litany" rend="simple">
          <tei:item>Herrn Dr. <tei:rs key="entity-000259" type="person">Theodor Frimmel</tei:rs>,
              <tei:hi rend="sperr">Wien</tei:hi>,</tei:item>
          <tei:item>Herrn Dr. <tei:rs key="entity-000378" type="person">Robert Hirschfeld</tei:rs>,
              <tei:hi rend="sperr">Wien</tei:hi>,</tei:item>
          <tei:item>Herrn Prof. <tei:rs key="entity-000228" type="person">Jacob Fischer</tei:rs>,
              <tei:hi rend="sperr">Wien</tei:hi>,</tei:item>
          <tei:item>Herrn Musik Dir. <tei:rs key="entity-000542" type="person">Ferd. Loewe</tei:rs>,
              <tei:hi rend="sperr">Wien</tei:hi>,</tei:item>
          <tei:item>Herrn Prof. <tei:rs key="entity-000545" type="person">Ernst Ludwig</tei:rs>,
              <tei:hi rend="sperr">Wien</tei:hi>,</tei:item>
          <tei:item>Herrn Dr. <tei:rs key="entity-000807" type="person">Detlef Schultz</tei:rs>,
              <tei:hi rend="sperr">Leipzig</tei:hi>,</tei:item>
        </tei:list></tei:p>

      <tei:p>je ein Exemplar Ihrer Bearbeitung von U.E. No. 936 "<tei:rs key="entity-001722"
          type="work"><tei:title>Haendel Orgel-Konzerte</tei:title></tei:rs>" sandten.-</tei:p>

      <tei:p rend="first">Wir zeichnen mit dem Ausdrucke vorzueglichster</tei:p>

      <tei:closer>
        <tei:salute rend="indent-60">Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002408"
            type="organization">„Universal-Edition“ Actiengesellschaft</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:rs key="entity-000946" type="person">Josef
            Weinberger</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>Universal Edition &amp; Co.</tei:addrLine>
          <tei:addrLine>Maximilianstrasse 11</tei:addrLine>
          <tei:addrLine>TELEPHONE N<tei:hi rend="supralinear">o.</tei:hi> 3364</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50">
          <tei:placeName>Vienna</tei:placeName>, <tei:handShift new="#TPue"/><tei:date
            when="1905-03-11">March 11, 1905</tei:date></tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine rend="indent-10">
            <tei:persName>Professor Heinrich Schenker, Esq.</tei:persName>
          </tei:addrLine>
          <tei:addrLine rend="indent-40">
            <tei:hi rend="sperr">VIENNA</tei:hi>
          </tei:addrLine>
        </tei:address>
        <tei:salute>Your <tei:rs type="person" key="entity-000632">Excellency</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p rend="first">We have the pleasure of informing you that we have today sent to the
        addresses given to us:<tei:list type="litany" rend="simple">
          <tei:item>Dr. <tei:rs key="entity-000259" type="person">Theodor Frimmel</tei:rs>, <tei:hi
              rend="sperr">Vienna</tei:hi>,</tei:item>
          <tei:item>Dr. <tei:rs key="entity-000378" type="person">Robert Hirschfeld</tei:rs>,
              <tei:hi rend="sperr">Vienna</tei:hi>,</tei:item>
          <tei:item>Prof. <tei:rs key="entity-000228" type="person">Jacob Fischer</tei:rs>, <tei:hi
              rend="sperr">Vienna</tei:hi>,</tei:item>
          <tei:item>Music Director <tei:rs key="entity-000542" type="person">Ferd. Loewe</tei:rs>,
              <tei:hi rend="sperr">Vienna</tei:hi>,</tei:item>
          <tei:item>Prof. <tei:rs key="entity-000545" type="person">Ernst Ludwig</tei:rs>, <tei:hi
              rend="sperr">Vienna</tei:hi>,</tei:item>
          <tei:item>Dr. <tei:rs key="entity-000807" type="person">Detlef Schultz</tei:rs>, <tei:hi
              rend="sperr">Leipzig</tei:hi>,</tei:item>
        </tei:list></tei:p>
      <tei:p>one copy each of your arrangement of U.E. No. 936, <tei:rs key="entity-001722"
          type="work"><tei:title>Handel Organ Concertos</tei:title></tei:rs> sandten.- </tei:p>

      <tei:p rend="first">We remain</tei:p>

      <tei:closer>
        <tei:salute rend="indent-60">with kindest regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002408"
            type="organization">Universal Edition &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:rs key="entity-000946" type="person">Josef
            Weinberger</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>
