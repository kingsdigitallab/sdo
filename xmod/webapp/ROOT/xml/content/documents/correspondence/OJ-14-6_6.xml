      <dc:title>Handwritten letter with envelope from Wilhelm to Jeanette Schenker, dated November
        15, 1938</dc:title>

      <dcterms:created>1938-11-15T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2024-11-14</dcterms:dateSubmitted>
      <dc:description>Wilhelm asks Jeanette whether she has received some printed
        forms.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000775">Wilhelm Schenker</marcrel:correspondent>
      <marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient>

    </sdo:itemDesc>


