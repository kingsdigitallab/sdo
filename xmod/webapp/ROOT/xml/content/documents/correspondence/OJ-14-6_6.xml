          <tei:milestone unit="folio" n="recto"/><tei:lb/>
          <tei:address>
            <tei:addrLine><tei:handShift new="#WS"/>To Mrs. Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-50">in <tei:hi rend="underline"><tei:del rend="overstrike"
                  >Baden bei Wien</tei:del></tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-50"><tei:del rend="overstrike">Welzergasse
              11a</tei:del></tei:addrLine>
            <tei:addrLine><tei:handShift new="#JB"/>currently at Vienna IV/1</tei:addrLine>
            <tei:addrLine>Schwindgasse No. 4/2</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:add><tei:handShift new="#JS"/>Answered <tei:date when="1938-11-20">November 20,
