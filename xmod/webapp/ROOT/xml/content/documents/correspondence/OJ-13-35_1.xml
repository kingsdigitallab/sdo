  <sdo:record ID="r0001">
    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-008187">RubinrautH ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003332">calling card</tei:rs>


      <dc:title>Handwritten calling card from Hilde and Semion Rubinraut to Schenker, dated January
        1922</dc:title>
      <dcterms:created>1922-01-01T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2022-06-05</dcterms:dateSubmitted>
      <dc:description>Semion and Hilde Rubinraut send best wishes for the New Year.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000719">Hilde Rubinraut</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>

    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:persName><tei:handShift new="#printed"/><tei:rs type="person" key="entity-000719">HERR
            UND FRAU SEMION RUBINRAUT</tei:rs></tei:persName><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
      </tei:opener>

      <tei:p><tei:handShift new="#HRu"/>Erlauben sich Ihnen sehr geehrter Herr D<tei:hi
          rend="supralinear">r.</tei:hi> die herzlichsten Neujahrwünsche zu übermitteln. </tei:p>

      <tei:closer>
        <tei:dateline>Prag im <tei:date when="1922-01">Jänner 1922</tei:date>.</tei:dateline>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:persName><tei:handShift new="#printed"/><tei:rs type="person" key="entity-000719">MR
            AND MRS SEMION RUBINRAUT</tei:rs></tei:persName><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
      </tei:opener>

      <tei:p><tei:handShift new="#HRu"/>We take the liberty, dear Dr. <tei:supplied
          reason="identity">Schenker</tei:supplied>, of sending you most cordial good wishes for the
        New Year.</tei:p>

      <tei:closer>
        <tei:dateline>Prague, <tei:date when="1922-01">January 1922</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>

