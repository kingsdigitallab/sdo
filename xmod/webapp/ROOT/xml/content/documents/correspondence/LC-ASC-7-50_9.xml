      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002748">Telegram</tei:rs>



      <dc:title>Handwritten telegram draft from Schoenberg to Moriz Violin, dated January 23,
        1940</dc:title>

      <dcterms:created>1940-01-23T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2020-08-31</dcterms:dateSubmitted>
      <dc:description>Schoenberg has wrongly addressed LC ASC 7/50, [8] and asks Violin to check
        with the Post Office to recover the letter.</dc:description>
      <dc:subject>mistaken address</dc:subject>



      <marcrel:correspondent key="entity-000798">ASchoenberg</marcrel:correspondent>
      <marcrel:recipient key="entity-000913">ViolinM</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:note type="editorial" place="inline">This draft is written in English.</tei:note>
      <tei:lb/>
      <tei:lb/>

      <tei:p><tei:handShift new="#AS-pencil"/><tei:subst>
          <tei:del rend="overstrike">Air</tei:del>
          <tei:add place="interlinear-above">My</tei:add>
        </tei:subst> airmail letter<tei:note xml:id="r0001-fn01" type="editorial" place="foot">=
            <tei:ref type="correspondence" cRef="LC-ASC-7-50_8">LC ASC 7/50, [8]</tei:ref>, January
          16, 1940.</tei:note> was <tei:subst>
          <tei:del rend="overstrike">wrongly</tei:del>
          <tei:add place="interlinear-above">mistakenly</tei:add>
        </tei:subst> adressed to 115 Divisadero Street stop ask Post office to give you the
        letter</tei:p>

      <tei:closer>
        <tei:signed rend="indent-70"><tei:rs type="person" key="entity-000798"
          >Schoenberg</tei:rs></tei:signed><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:rs type="person" key="entity-000913">Moriz
            Violin</tei:rs></tei:addrLine>
          <tei:addrLine rend="indent-50">1115 Divisadero Street</tei:addrLine>
          <tei:addrLine rend="indent-80">San Francisco</tei:addrLine>
        </tei:address><tei:lb/>
        <tei:dateline><tei:date when="1940-01-23">I/23 1940</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>

