  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs type="correspondence" role="correspondence" key="entity-005439">Waldheim-Eberle ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>

      <dc:title>Handwritten postcard from Waldheim–Eberle to Schenker, dated September 11,
        1906</dc:title>
      <dcterms:created>1906-09-11T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-09-01</dcterms:dateSubmitted>

      <dc:description>Waldheim-Eberle asks Schenker to visit them regarding the
        Instrumentations-Tabelle.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002402">Waldheim-Eberle</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/><tei:hi rend="bold">Korrespondenz-Karte</tei:hi></tei:fw>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>Absender: </tei:addrLine>

        </tei:address>
        <tei:address>
          <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied>
            <tei:handShift new="#unkWE"/>Hochwohlgeboren Herrn D<tei:hi rend="supralinear"
              >r.</tei:hi> Schenker</tei:addrLine>
          <tei:addrLine rend="indent-50">Wien, III.</tei:addrLine>
          <tei:addrLine rend="indent-50">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 7/3 WIEN <tei:unclear>68</tei:unclear> | 12. IX. 06 | * a *
          ||</tei:fw>
        <tei:fw type="postmark">|| 3/3 WIEN 49 | 12. IX. 06 2 | BESTELLT ||</tei:fw><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#unkWE"/>Hochwohlgeboren Herrn D<tei:hi
              rend="supralinear">r.</tei:hi> Heinrich Schenker, hier.</tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">Wir bitten, uns Ihren Besuch zu schenken, da die Sache Ihrer <tei:rs
          type="work" key="entity-001729"><tei:title>Instrumentations-Tabelle</tei:title></tei:rs>
        spruchreif geworden ist.<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
          of this postcard is not recorded in Schenker’s diary, but the following is recorded on
          September 11: “Brief mit Mahnung an Direktor Stritzko (wegen der
            <tei:title>Tabelle</tei:title>).” (“Letter with warning to Director Stritzka (regarding
          the <tei:title>Table</tei:title>)”. Schenker’s visit took place on September 13 (see
            <tei:ref type="correspondence" cRef="OJ-15-8_4">OJ 15/8, [4]</tei:ref>, footnote
          2).</tei:note></tei:p>


      <tei:closer>
        <tei:salute rend="indent-50">Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">R. v. Waldheim-Jos. Eberle &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-80"><tei:note type="editorial" place="inline">illegible
            initials</tei:note></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Erste Wiener
          Zeitungs-Gesellschaft</tei:signed><tei:lb/>
        <tei:dateline><tei:date when="1906-09-11">11./9. 1906</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/><tei:hi rend="bold">Postcard</tei:hi></tei:fw>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>From: </tei:addrLine>

        </tei:address>
        <tei:address>
          <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied>
            <tei:handShift new="#unkWE"/>Dr. Schenker, Esq.</tei:addrLine>
          <tei:addrLine rend="indent-50">Vienna III</tei:addrLine>
          <tei:addrLine rend="indent-50">Reisnerstrasse 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 7/3 VIENNA <tei:unclear>68</tei:unclear> | 12. IX. 06 | * a *
          ||</tei:fw>
        <tei:fw type="postmark">|| 3/3 VIENNA 49 | 12. IX. 06 2 | DELIVERED ||</tei:fw><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#unkWE"/>Dr. Heinrich Schenker, here</tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">Please do us the honor of a visit, for the matter of your <tei:rs
          type="work" key="entity-001729"><tei:title level="m">Instrumentation
          Table</tei:title></tei:rs> is now ripe for discussion.<tei:ptr corresp="#r0001-fn01"
          type="footnote"/></tei:p>


      <tei:closer>
        <tei:salute rend="indent-50">With kind regards,</tei:salute>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">R. v. Waldheim-Jos. Eberle &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-80"><tei:note type="editorial" place="inline">illegible
            initials</tei:note></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>First Viennese Newspaper
          Society</tei:signed><tei:lb/>
        <tei:dateline><tei:date when="1906-09-11">September 11, 1906</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>
