  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002750">Note</tei:rs>



      <dc:title>Handwritten note by Schenker, undated [late October, 1912?]</dc:title>

      <dcterms:created>1912-10-31T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-03-11</dcterms:dateSubmitted>
      <dc:description>Self-reminder that the requested "supplementary honorarium" of 400 Kronen has
        not yet been delivered.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000632">Heinrich Schenker</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:note type="editorial" place="inline">on small notecard with rounded corners</tei:note>
      </tei:opener>

      <tei:p><tei:hi rend="underline">zugesetzt</tei:hi> – 400 Kr. <tei:hi rend="underline"
          >nachträglich</tei:hi>, nie zurückgekommen trotz großem <tei:note type="editorial"
          place="inline">illegible word</tei:note></tei:p>

      <tei:p>s. Bf. <tei:date when="1912-06-03">3.V1 1912</tei:date><tei:note xml:id="r0001-fn01"
          type="editorial" place="foot">= <tei:ref type="correspondence" cRef="OC-52-427">OC
            52/427</tei:ref>, June 3, 1912 from Hertzka. This note must have been written later than
          June 1912, probably around the end of October, since the last time the demand for 400
          Kronen was mentioned in 1912 was in <tei:ref type="correspondence" cRef="WSLB-140">WSLB
            140</tei:ref>, October 14, 1912, but possibly as late as April 1913 (WSLB 151, April 4),
          when demands finally ceased.</tei:note> Mappe „<tei:rs type="work" key="entity-001733"
          >Erl. A</tei:rs>“</tei:p>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:note type="editorial" place="inline">on small notecard with rounded corners</tei:note>
      </tei:opener>

      <tei:p><tei:hi rend="underline">pestered</tei:hi> – 400 Kronen, <tei:hi rend="underline"
          >supplementary</tei:hi>, never got back despite insistent <tei:unclear
          reason="original_illegible">lobbying</tei:unclear></tei:p>

      <tei:p>See letter of <tei:date when="1912-06-03">June 3, 1912</tei:date><tei:ptr
          corresp="#r0001-fn01" type="footnote"/>
        <tei:supplied reason="clarity">in</tei:supplied> folder „<tei:rs type="work"
          key="entity-001733">Elucidatory Edition</tei:rs>“</tei:p>


    </tei:div>
