  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-005575">KochL ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typewritten postcard from Louis Koch to Schenker, dated March 9, 1920</dc:title>
      <dcterms:created>1920-03-09T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2015-02-16</dcterms:dateSubmitted>
      <dc:description>Koch hopes photographs have arrived.</dc:description>
      <dc:subject>Photosendung; dispatch of photographs</dc:subject>



      <marcrel:correspondent key="entity-000454">Louis Koch</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Postkarte</tei:fw><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine rend="indent-10"> Robert Koch</tei:addrLine>
            <tei:addrLine rend="indent-10">Hofjuwelier</tei:addrLine>
            <tei:addrLine>Frankfurt a. M., den ...........</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine>
              <tei:handShift new="#TPlko"/>S.H. Herrn Dr. H. SCHENKER</tei:addrLine>
            <tei:addrLine rend="indent-15"><tei:hi rend="underline"><tei:hi rend="sperr"
                  >Wien.</tei:hi></tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Reisnerstrasse 38.</tei:addrLine>
            <tei:lb/>
          </tei:address>
          <tei:fw type="postmark">|| FRANKFURT (MAIN) | 9. 3. 20 | * 9 rr ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:hi rend="underline">Herrn Dr. <tei:hi rend="sperr">H. Schenker,
                Wien.</tei:hi></tei:hi><tei:note xml:id="r0001-fn01" type="editorial" place="foot"
              >Receipt of this postcard is recorded in Schenker's diary at OJ 3/1, p. 2221, March
              16, 1920: "Von Koch (K.): bestätigt den Empfang meines Briefes, Autograph sei
              inzwischen abgegangen." ("From Koch (postcard): confirms receipt of my letter, the
              autograph has been dispatched in the meantime.")</tei:note></tei:addrLine>
          <tei:lb/>
          <tei:lb/>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">Ich bestätige den Empfang Ihres Briefes vom <tei:date when="1920-03-02">2.
          d. M.</tei:date><tei:note xml:id="r0001-fn02" type="editorial" place="foot">Sending of
          this letter, which is not known to survive, is recorded in Schenker's diary at OJ 3/1, p.
          2218, March 2, 1920: "An Koch (Br.): urgire die Bilder, erbitte Adresse des Photographen."
          ("To Koch (letter): I urgently request the photographs, ask for the address of the
          photographer.")</tei:note> und hoffe die Photosendung inzwischen in Ihren Besitz.<tei:note
          xml:id="r0001-fn03" type="editorial" place="foot">See <tei:ref type="correspondence"
            cRef="OC-B-267">OC B/267</tei:ref>, February 27, 1920.</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">mit vorzüglicher Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#LKo"/><tei:rs key="entity-000454"
            type="person">Louis Koch</tei:rs></tei:signed><tei:lb/>
        <tei:dateline><tei:placeName>Frankfurt/M.</tei:placeName>, den <tei:date when="1920-03-09"
            >9. 3. 20</tei:date>.</tei:dateline>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Postcard</tei:fw><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine rend="indent-10"> Robert Koch</tei:addrLine>
            <tei:addrLine rend="indent-10">Court Jeweller</tei:addrLine>
            <tei:addrLine>Frankfurt am Main, ...........</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine>
              <tei:handShift new="#TPlko"/>Dr. H. SCHENKER, Esq.,</tei:addrLine>
            <tei:addrLine rend="indent-15"><tei:hi rend="underline"><tei:hi rend="sperr"
                  >Vienna</tei:hi></tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Reisnerstrasse 38</tei:addrLine>
            <tei:lb/>
          </tei:address>
          <tei:fw type="postmark">|| FRANKFURT (MAIN) | 9. 3. 20 | * 9 rr ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:hi rend="underline">Dr. <tei:hi rend="sperr">H. Schenker,
                Vienna</tei:hi></tei:hi><tei:ptr corresp="#r0001-fn01" type="footnote"
            /></tei:addrLine>
          <tei:lb/>
          <tei:lb/>
        </tei:address>
      </tei:opener>

      <tei:p rend="first">I acknowledge receipt of your letter of the <tei:date when="1920-03-02"
          >2nd of this month</tei:date><tei:ptr corresp="#r0001-fn02" type="footnote"/> and hope
        that in the meantime you have taken delivery of the dispatch of photographs.<tei:ptr
          corresp="#r0001-fn03" type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">With kind regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#LKo"/><tei:rs key="entity-000454"
            type="person">Louis Koch</tei:rs></tei:signed><tei:lb/>
        <tei:dateline><tei:placeName>Frankfurt am Main</tei:placeName>, <tei:date when="1920-03-09"
            >March 9, 1920</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>
