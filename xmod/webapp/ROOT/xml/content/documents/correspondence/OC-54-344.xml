      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002750">Note</tei:rs>

      <dc:title>Handritten note from Waldheim to Schenker, dated May 31, 1932</dc:title>
      <dcterms:created>1932-05-31T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-08-14</dcterms:dateSubmitted>

      <dc:description>Waldheim-Eberle notes that 23 blueprints have been acquired</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002402">Waldheim-Eberle</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">


      <tei:milestone unit="folio" n="recto"/>
      <tei:p><tei:handShift new="#unkWE1"/>23 Lichtpause Originale übernommen<tei:note
          xml:id="r0001-fn01" type="editorial" place="foot">Schenker’s diary for May 31, 1932
          records: “Bei Waldheim übergeben wir die Blätter: es sind also 24 Seiten, die Kosten also
          doppelt so groß – der Direktor empfiehlt Bosworth als Kommissions-Verlag.” (“At
          Waldheim’s, we hand over the sheets: there are, in fact, 24 pages, so the costs will be
          twice as high – the director recommends Bosworth as a commission publishing house.”). —
          Bosworth &amp; Co., sheet music dealers, antiquarian and strings, Vienna I, Dr.
          Karl-Lueger-Platz 5.</tei:note></tei:p>



      <tei:closer>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
            type="organization">Waldheim-Eberle A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-80"><tei:handShift new="#unkWE2"/><tei:note type="editorial"
            place="inline">illegible initials</tei:note></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>Musikabteilung</tei:signed>
        <tei:dateline><tei:handShift new="#unkWE1"/><tei:date when="1932-05-31">31./5.
          32</tei:date></tei:dateline>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:milestone unit="folio" n="recto"/>

      <tei:p><tei:handShift new="#unkWE1"/>23 blueprint originals received<tei:ptr
          corresp="#r0001-fn01" type="footnote"/></tei:p>

      <tei:closer>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002402"
