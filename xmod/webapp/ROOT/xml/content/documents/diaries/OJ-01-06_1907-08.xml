      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker August 4, 1907</dc:title>
         <dcterms:created>1907-08-04T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2017-03-18</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:date when="1907-08-04">Freitag, 4. 8.</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0002-se01">Nachmittag: Ausflug nach dem
               „Kraxentrager“<tei:note xml:id="r0002-fn01" type="editorial" place="foot"
               >Kraxentrager: mountain in the Ziller Valley Alps, 2,999 meters high, 4 miles (7km)
               east of the Brenner Pass, on the border between North and South Tyrol (now the
               Austria/Italy border). The Landshuter Hütte is 2,693 meters above sea level.
            </tei:note> beim Brenner. <tei:supplied reason="omission"><tei:milestone unit="section"
                  rend="inline em-dash"/></tei:supplied> In der Landshutter Hütte: Gestein- u.
            Pflanzensammlungen. <tei:supplied reason="omission"><tei:milestone unit="section"
                  rend="inline em-dash"/></tei:supplied> In Gesellschaft einer Berliner Familie.
               <tei:supplied reason="omission"><tei:milestone unit="section" rend="inline em-dash"
               /></tei:supplied> 14-jähriger Führer Holzmann reizend aufgeweckt.</tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:date when="1907-08-04">Friday, August 4</tei:date>
         </tei:opener>
         <tei:ab corresp="r0002-se01">In the afternoon: excursion to the Kraxentrager<tei:ptr
               corresp="#r0002-fn01" type="footnote"/> near Brenner. <tei:supplied reason="omission"
                  ><tei:milestone unit="section" rend="inline em-dash"/></tei:supplied> In the
            Landshut hut: collections of rocks and plants. <tei:supplied reason="omission"
                  ><tei:milestone unit="section" rend="inline em-dash"/></tei:supplied> In the
            company of a Berlin family. <tei:supplied reason="omission"><tei:milestone
                  unit="section" rend="inline em-dash"/></tei:supplied> 14-year-old guide Holzmann
            charmingly alert.</tei:ab>
      </tei:div>
   </sdo:record>

