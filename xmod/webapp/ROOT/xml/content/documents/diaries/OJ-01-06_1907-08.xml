         <tei:opener>
            <tei:date when="1907-08-12">Mont. 12. 8.</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0004-se01">Einladung von D<tei:hi rend="supralinear"
               >r</tei:hi>
            <tei:rs key="entity-000259" type="person">Frimmel</tei:rs> zur Mitarbeit am
                  „<tei:title><tei:hi rend="lateinschr"
