         <tei:ab type="sub-entry" xml:id="r0004-se02"><tei:rs key="entity-000199" type="person">Frau
                  <tei:hi rend="lateinschr">Eissler</tei:hi></tei:rs> in <tei:rs key="entity-002304"
               type="place">Innsbruck</tei:rs> getroffen: per Automobil nach Hall.<tei:note
               xml:id="r0004-fn01" type="editorial" place="foot">Hall in Tirol: town 5 miles (8km)
               east-northeast of Innsbruck, on the Inn River and the main road to Schaz and
               Kufstein.</tei:note></tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:date when="1907-08-12">Monday, August 12</tei:date>
         </tei:opener>
         <tei:ab corresp="r0004-se01">Invitation from Dr. <tei:rs key="entity-000259" type="person"
               >Frimmel</tei:rs> to collaborate on the <tei:title level="j"
               >Beethoven-Jahrbuch</tei:title>.</tei:ab>

         <tei:ab corresp="r0004-se02"><tei:rs key="entity-000199" type="person">Mrs.
               Eissler</tei:rs> met in <tei:rs key="entity-002304" type="place">Innsbruck</tei:rs>:
            by automobile to Hall.<tei:ptr corresp="#r0004-fn01" type="footnote"/></tei:ab>
      </tei:div>
   </sdo:record>

