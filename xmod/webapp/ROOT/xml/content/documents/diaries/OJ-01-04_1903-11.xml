      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker November 4, 1903</dc:title>
         <dcterms:created>1903-11-04T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2017-11-29</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:dateline><tei:handShift new="#HS"/><tei:date when="1903-11-04">4.
               11.</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0001-se01"><tei:note type="editorial" place="inline">in
               left margin:</tei:note>
            <tei:handShift new="#HSpencil"/>√ <tei:handShift new="#HS"/>Nach <tei:rs type="place"
               key="entity-002501">Berlin</tei:rs> mit <tei:rs key="entity-000913" type="person"
               >Floriz</tei:rs>, bei Brodes, zu schnelle Tempi <tei:add place="interlinear-above">im
               Vortrage <tei:rs key="entity-000116" type="person">Busoni</tei:rs>s</tei:add>.
            Schlimme „Kritiken“.<tei:note xml:id="r0001-fn01" type="editorial" place="foot">The
               première of Schenker's <tei:rs type="work" key="entity-001664"><tei:title level="m"
                     >Syrische Tänze</tei:title></tei:rs> in Schoenberg's orchestral transcription,
               under the direction of Ferruccio Busoni, took place on November 5, 1903 in the
               Beethovensaal in Berlin. A copy of the printed program for the concert is preserved
               as OJ 35/5. Twenty-one reviews of the concert, dating between November 6 and 19, are
               preserved at OC 2/pp. 5‒7. Although this entry is dated the 4th, it would seem to
               cover the performance itself and the subsequent few days.</tei:note></tei:ab>

         <tei:ab type="sub-entry" xml:id="r0001-se02">Influenza . . </tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:dateline><tei:handShift new="#HS"/><tei:date when="1903-11-04">November
                  4</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab corresp="r0001-se01"><tei:note type="editorial" place="inline">in left
               margin:</tei:note>
            <tei:handShift new="#HSpencil"/>√ <tei:handShift new="#HS"/>To <tei:rs type="place"
               key="entity-002501">Berlin</tei:rs> with <tei:rs key="entity-000913" type="person"
               >Floriz</tei:rs>, at Brodes's place. Tempi <tei:add place="interlinear-above">in
                  <tei:rs key="entity-000116" type="person">Busoni</tei:rs>'s performance</tei:add>
            too fast; dreadful "reviews."<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:ab>

         <tei:ab corresp="r0001-se02">Influenza . . </tei:ab>
      </tei:div>
   </sdo:record>

