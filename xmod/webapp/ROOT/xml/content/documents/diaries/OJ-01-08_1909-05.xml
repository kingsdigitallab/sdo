      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker May 7, 1909</dc:title>
         <dcterms:created>1909-05-07T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2018-04-13</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:hi rend="bold">Ser. A, </tei:hi><tei:pb n="100"/><tei:hi rend="bold">b</tei:hi>
            <tei:fw type="header" rend="center"><tei:handShift new="#HSpencil"/>09</tei:fw><tei:lb/>
            <tei:dateline><tei:handShift new="#HS"/><tei:date when="1909-05-07"
               >7/5.</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0001-se01">Als <tei:hi rend="lateinsch">Ostmark</tei:hi>
            hat <tei:rs type="place" key="entity-002506"
               ><tei:country>Österreich</tei:country></tei:rs> sein politisches Dasein begonnen, u.
            als <tei:hi rend="lateinsch">Ostmark</tei:hi> scheint es dieses auch beschließen zu
            wollen.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0001-se02">Deutsche Bundesfürsten in <tei:hi
               rend="lateinschr">Wien</tei:hi> (in <tei:rs type="place" key="entity-002266"
               >Schönnbrunn</tei:rs>) ‒ ritterlicher Zug, allerletzter Ausklang <tei:del
               rend="overstrike">s</tei:del>einer großen, seltsam verschlungenen Epoche!</tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:hi rend="bold">Ser. A, </tei:hi><tei:pb n="100"/><tei:hi rend="bold">b</tei:hi>
            <tei:fw type="header" rend="center"><tei:handShift new="#HSpencil"/>09</tei:fw><tei:lb/>
            <tei:dateline><tei:handShift new="#HS"/><tei:date when="1909-05-07">May
               7</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab corresp="r0001-se01"><tei:rs type="place" key="entity-002506"
                  ><tei:country>Austria</tei:country></tei:rs> began its political existence as the
            Eastern March, and as the Eastern March is also how it appears to have ended
            up.</tei:ab>

         <tei:ab corresp="r0001-se02">German federal princes in Vienna (in <tei:rs type="place"
               key="entity-002266">Schönnbrunn</tei:rs>) ‒ knightly procession, the very last
            vestige of a great, remarkably long-lasting epoch!</tei:ab>
      </tei:div>
   </sdo:record>

   <sdo:record ID="r0002">
      <!--Use the itemDesc to group information about a single record, e.g. a single diary entry, lessonbook entry or piece of correspondence. 
                         The   contains a series of Dublin Core elements. -->
      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker May 25, 1909</dc:title>
         <dcterms:created>1909-05-25T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2012-10-03</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:hi rend="bold">Ser. A, </tei:hi><tei:pb n="100"/><tei:hi rend="bold">a</tei:hi>
            <tei:fw type="header" rend="center"><tei:handShift new="#HSpencil"/><tei:date
                  when="1909-05">5/ <tei:handShift new="#HS"/>09</tei:date>.</tei:fw><tei:lb/><tei:lb/>
            <tei:date when="1909-05-25">25. 5.</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0002-se01"><tei:handShift new="#HS"/>Brief an <tei:rs
               type="person" key="entity-000707">Prof. Rettich</tei:rs> mit Beilagen.<tei:note
               xml:id="r0002-fn01" type="editorial" place="foot">No correspondence with Rettich is
               known to survive. Schenker was at this time trying urgently to obtain assurance from
                  <tei:rs type="person" key="entity-000743">Baron Alphons Rothschild</tei:rs> (whose
               private secretary Rettich was) of financial support in the form of the printing costs
               of <tei:rs type="work" key="entity-001726"><tei:title level="m"
                     >Kontrapunkt</tei:title> I</tei:rs>.</tei:note></tei:ab>

         <tei:ab type="sub-entry" xml:id="r0002-se02">Abschluß der Correkturen des ersten
            Abschnittes: <tei:hi rend="lateinschr">C. f.</tei:hi><tei:note xml:id="r0002-fn02"
               type="editorial" place="foot">The first section of Schenker's <tei:rs type="work"
                  key="entity-001726"><tei:title level="m">Kontrapunkt</tei:title> I</tei:rs>
               (1910), is entitled <tei:title level="a">Cantus firmus als Grundlage der
                  Kontrapunktstudien</tei:title> (<tei:title level="a">Cantus firmus as the
                  Foundation of Contrapuntal Studies</tei:title>).</tei:note></tei:ab>

         <tei:ab type="sub-entry" xml:id="r0002-se03"><tei:rs type="person" key="entity-000344"
                  ><tei:hi rend="lateinschr">Haydn</tei:hi></tei:rs>feier u. <tei:handShift
               new="#HSpencil"/>‒<tei:handShift new="#HS"/>Kongreß: <tei:rs type="person"
               key="entity-000344"><tei:hi rend="lateinschr">Haydn</tei:hi></tei:rs> wird mit so
            viel Vorbehalten gefeiert, daß man sich fragen muß, wozu dann noch die Feier
            überhaupt?</tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:hi rend="bold">Ser. A, </tei:hi><tei:pb n="100"/><tei:hi rend="bold">a</tei:hi>
            <tei:fw type="header" rend="center"><tei:handShift new="#HSpencil"/><tei:date
                  when="1909-05">May <tei:handShift new="#HS"/>1909</tei:date></tei:fw><tei:lb/><tei:lb/>
            <tei:date when="1909-05-25">May 25</tei:date>
         </tei:opener>
         <tei:ab corresp="r0002-se01"><tei:handShift new="#HS"/>Letter to <tei:rs type="person"
               key="entity-000707">Professor Rettich</tei:rs> with enclosures.<tei:ptr
               corresp="#r0002-fn01" type="footnote"/></tei:ab>

         <tei:ab corresp="r0002-se02">Completion of the proofs of the first section: <tei:hi
               rend="italic">cantus firmus</tei:hi>.<tei:ptr corresp="#r0002-fn02" type="footnote"
            /></tei:ab>

         <tei:ab corresp="r0002-se03"><tei:rs type="person" key="entity-000344">Haydn</tei:rs>
            festival and conference: <tei:rs type="person" key="entity-000344">Haydn</tei:rs> is
            being commemorated with so many reservations that one has to ask why we are holding a
            festival at all?</tei:ab>
      </tei:div>
   </sdo:record>

