                <elementSpec ident="hi" mode="change" module="core">
                    <attList>
                        <attDef ident="rend" mode="replace">
                            <desc>specify a list of values</desc>
                            <datatype maxOccurs="1">
                                <rng:ref name="data.word"/>
                            </datatype>
                            <valList type="closed">
                                <valItem ident="bold">
                                    <desc>Bold</desc>
                                </valItem>
                                <valItem ident="italic">
                                    <desc>Italic</desc>
                                </valItem>
                                <valItem ident="bolditalic">
                                    <desc>Bold and italic</desc>
                                </valItem>
                                <valItem ident="sup">
                                    <desc>Superscript</desc>
                                </valItem>
                                <valItem ident="sub">
                                    <desc>Subscript</desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="code" module="tagdocs" mode="change">
                    <classes mode="replace">
                        <memberOf key="model.pLike" mode="add"/>
                    </classes>
                </elementSpec>
                <elementSpec ident="ref" module="core" mode="change">
                    <attList>
                        <attDef ident="cRef" mode="change">
                            <desc>specifies the destination of the reference by supplying a
                                canonical reference from a scheme defined in a refsDecl element in
                                the TEI header or in an appropriate documentation.</desc>
                            <exemplum>
                                <p>The usage of <att>cRef</att> within <gi>ref</gi> assumes the
                                    following declaration to be supplied in the <gi>teiHeader</gi></p>
                                <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                    <refsDecl xml:id="internal">
                                        <cRefPattern matchPattern="(.+)"
                                            replacementPattern="/$context-path/($1).html">
                                            <p>Internal links works with files xml:id which are also
                                                file names</p>
                                        </cRefPattern>
                                        <cRefPattern matchPattern="(.+)#(.+)"
                                            replacementPattern="/$context-path/($1).html#($2)">
                                            <p>When linking to a section of a page, the file xml:id
                                                is followed by '#' and the xml:id of the
                                            section.</p>
                                        </cRefPattern>
                                        <cRefPattern matchPattern="#(.+)" replacementPattern="#($1)">
                                            <p>Links internal to the same page are prefixed by a
                                            '#'</p>
                                        </cRefPattern>
                                    </refsDecl>
                                </egXML>
                            </exemplum>
                            <exemplum>
                                <egXML xmlns="http://www.tei-c.org/ns/Examples"> Link to another page within <ref
                                        cRef="p0">the same site</ref>.</egXML>
                            </exemplum>
                            <exemplum>
                                <egXML xmlns="http://www.tei-c.org/ns/Examples">Link to a <ref cRef="g4#ng003-01"
                                >section</ref> of anothe page within the same site.</egXML>
                            </exemplum>
                            <exemplum>
                                <egXML xmlns="http://www.tei-c.org/ns/Examples">Link <ref
                                        cRef="#anchor1">internal to this page</ref></egXML>
                            </exemplum>
                        </attDef>
                    </attList>
                </elementSpec>
            </schemaSpec>
            <p>TEI P5 customisation for usage within xMod web delivery in CCH projects</p>
        </body>
    </text>
