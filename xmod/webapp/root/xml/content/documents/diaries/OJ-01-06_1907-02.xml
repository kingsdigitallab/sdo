      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker February 6, 1907</dc:title>
         <dcterms:created>1907-02-06T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2012-05-04</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>
      <tei:div type="transcription">
         <tei:opener>
            <tei:date>Mtw. 6.</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0004-se01">An Frau <tei:hi rend="lateinschr"
               >Seitz</tei:hi> wegen pünktlicher Monatszahlungen nach <tei:rs key="entity-003224"
               type="place"><tei:hi rend="lateinschr">Alexandria</tei:hi></tei:rs> geschrieben. </tei:ab>

         <tei:ab type="sub-entry" xml:id="r0004-se02"><tei:rs key="entity-002430"
               type="organization">Gesellschaft</tei:rs>skonz. III:<tei:note xml:id="r0004-fn01"
               type="editorial" place="foot">An a capella concert by the Singverein conducted by
               Hans Wagner, the program included choral works by Palestrina, Cherubini, Michael
               Haydn, Löwe, Cornelius, Herbeck, Brüll, and Fuchs. Julia Culp sang arias by J. S.
               Bach and "Scottish Songs" by Beethoven: <tei:title level="j">Neue Freie
                  Presse</tei:title> Feb 2, p. 15. </tei:note>
            <tei:rs key="entity-002644" type="person"><tei:hi rend="lateinschr">Pal<tei:subst>
                     <tei:del rend="overwritten">ä</tei:del>
                     <tei:add place="superimposed">e</tei:add>
                  </tei:subst>strina</tei:hi></tei:rs>'s, „<tei:rs key="entity-003226"
               type="composition"><tei:title><tei:hi rend="lateinschr">Stabat
                  mater</tei:hi></tei:title></tei:rs>“ Langweilig, oder mindestens nicht <tei:add
               place="interlinear-above">gerade</tei:add> so <tei:del rend="overstrike">zu
               bringen</tei:del> wiederzugeben.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0004-se03"><tei:rs key="entity-000027" type="person"
                  ><tei:hi rend="lateinschr">Bach</tei:hi></tei:rs>'s <tei:term><tei:hi
                  rend="lateinschr">Kantate</tei:hi></tei:term>: „<tei:title>Schlage doch,
               gewünschte Stunde</tei:title>“ ohne Verständnis <tei:add place="interlinear-above"
               >von (?) bearbeitet <tei:del rend="overstrike">probt</tei:del></tei:add>
            <tei:del rend="overstrike">durchgegarbeitet u. ausgeführt</tei:del> u. ebenso ohne
            Verständnis zur Aufführung gebracht.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0004-se04">Frau Culp sang <tei:add
               place="interlinear-above">einige</tei:add> Schottische <tei:term>Lieder</tei:term>
            von <tei:rs key="entity-000055" type="person"><tei:hi rend="lateinschr"
               >Beeth</tei:hi></tei:rs> (mit Kl., Vl., Cl.) sehr gut.</tei:ab>
      </tei:div>
      <tei:div type="translation">
         <tei:opener>
            <tei:date>Wed. 6.</tei:date>
         </tei:opener>
         <tei:ab corresp="r0004-se01">The Mrs. Seitz regarding punctual monthly payments, written to
               <tei:rs key="entity-003224" type="place">Alexandria</tei:rs>. </tei:ab>

         <tei:ab corresp="r0004-se02"><tei:rs key="entity-002430" type="organization"
               >Gesellschaft</tei:rs> Concert III:<tei:ptr corresp="#r0001-fn01" type="footnote"/>
            <tei:rs key="entity-002644" type="person">Palestrina</tei:rs>'s <tei:rs
               key="entity-003226" type="composition"><tei:title level="m">Stabat
               mater</tei:title></tei:rs>: tedious, or at least <tei:supplied rend="clarity">it
               should</tei:supplied> not <tei:supplied rend="clarity">be</tei:supplied> performed in
               <tei:add place="interlinear-above">exactly</tei:add> that way.</tei:ab>

         <tei:ab corresp="r0004-se03"><tei:rs key="entity-000027" type="person">Bach</tei:rs>'s
               <tei:term>Cantata</tei:term>: "<tei:title>Schlage doch, gewünschte
            Stunde</tei:title>" <tei:del rend="overstrike">prepared and performed</tei:del>
            <tei:add place="interlinear-above">edited <tei:del rend="overstrike">rehearsed</tei:del>
               by (?)</tei:add> without understanding and brought to performance with equally little
            understanding.</tei:ab>

         <tei:ab corresp="r0004-se04">Mrs. Culp sang <tei:add place="interlinear-above"
               >some</tei:add> Scottish <tei:term>song</tei:term>s by <tei:rs key="entity-000055"
               type="person">Beethoven</tei:rs> (with piano, violin, and clarinet) very well.
         </tei:ab>
      </tei:div>
   </sdo:record>
