      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker March 1, 1907</dc:title>
         <dcterms:created>1907-03-01T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2012-08-16</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>
      <tei:div type="transcription">
         <tei:opener>
            <tei:fw type="header" rend="center"><tei:handShift new="#HS"/><tei:hi rend="underline"
                  >März</tei:hi>.</tei:fw>
            <tei:date when="1907-03-01">Freit. 1.</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0001-se01"><tei:handShift new="#HSblue"/>√ <tei:handShift
               new="#HS"/>An <tei:hi rend="lateinschr">Steinauer</tei:hi> (<tei:rs
               key="entity-000727" type="person"><tei:hi rend="lateinschr">Rosé</tei:hi></tei:rs>)
            Schuld endlich ganz beglichen.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0001-se02"><tei:rs key="entity-000542" type="person"
               >Löwe</tei:rs> macht mir den Antrag, <tei:rs key="entity-000094" type="person"
                  ><tei:hi rend="lateinschr">Brahms</tei:hi></tei:rs>' <tei:rs key="entity-001293"
               type="composition"><tei:title>D. Rq.</tei:title></tei:rs> sowie die <tei:rs
               key="entity-003271" type="organization">Singakad.</tei:rs> überhaupt zu dirigieren.
            Ablehnung, da es augenblicklich unangebracht wäre, vergebens das Opfer künstlerischer
            Mühen zu bringen, u. <tei:add place="interlinear-above">dadurch nur</tei:add> die
            übrigen Arbeiten (Buch, wie Komposit.) zu gefährden.</tei:ab>
      </tei:div>
      <tei:div type="translation">
         <tei:opener>
            <tei:fw type="header" rend="center"><tei:handShift new="#HS"/><tei:hi rend="underline"
                  >March</tei:hi></tei:fw>
            <tei:date when="1907-03-01">Fri. 1</tei:date>
         </tei:opener>
         <tei:ab corresp="r0001-se01"><tei:handShift new="#HSblue"/>√ <tei:handShift new="#HS"/>To
            Steinauer (<tei:rs key="entity-000727" type="person">Rosé</tei:rs>) debt finally paid
            off.</tei:ab>

         <tei:ab corresp="r0001-se02"><tei:rs key="entity-000542" type="person">Löwe</tei:rs> puts
            to me the proposal of conducting the <tei:rs key="entity-000094" type="person"
               >Brahms</tei:rs>
            <tei:rs key="entity-001293" type="composition"><tei:title level="m">German
                  Requiem</tei:title></tei:rs> together with the <tei:rs key="entity-003271"
               type="organization">Singakadakademie</tei:rs>. I decline since it would be
            ill-advised at the moment to incur the sacrifice of that artistic effort to no avail,
               <tei:add place="interlinear-above">merely thereby</tei:add> endangering my other
            projects (book, also composition). </tei:ab>
      </tei:div>
   </sdo:record>
