      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker March 3, 1907</dc:title>
         <dcterms:created>1907-03-03T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2012-08-16</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>
      <tei:div type="transcription">
         <tei:opener>
            <tei:date when="1907-03-03">Sonnt. 3.</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0003-se01"><tei:rs key="entity-000871" type="person"
                  ><tei:hi rend="lateinschr">R. Strauss</tei:hi></tei:rs> dirigiert <tei:hi
               rend="lateinschr"><tei:rs key="entity-003235" type="organization">Philh.</tei:rs>
               Kz</tei:hi>: <tei:rs key="entity-000094" type="person"><tei:hi rend="lateinschr"
                  >Brahms</tei:hi></tei:rs>' <tei:rs key="entity-001353" type="composition"
                  ><tei:title>IV</tei:title></tei:rs> (ohne Wärme, Verständniss), u. <tei:rs
               key="entity-003272" type="composition"><tei:title><tei:hi rend="lateinschr"
                     >Zarathustra</tei:hi></tei:title></tei:rs>.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0003-se02">Frau <tei:rs key="entity-000582" type="person"
               >I. M.</tei:rs>
            <tei:add place="interlinear-above">er</tei:add>krankt an <tei:hi rend="lateinschr"
               >Influenza</tei:hi>.</tei:ab>
      </tei:div>
      <tei:div type="translation">
         <tei:opener>
            <tei:date when="1907-03-03">Sun. 3</tei:date>
         </tei:opener>
         <tei:ab corresp="r0003-se01"><tei:rs key="entity-000871" type="person">R. Strauss</tei:rs>
            conducts the <tei:rs key="entity-003235" type="organization">Philharmonic</tei:rs>
            concert: <tei:rs key="entity-000094" type="person">Brahms</tei:rs>
            <tei:rs key="entity-001353" type="composition">Fourth</tei:rs> (devoid of warmth,
            understanding), and <tei:rs key="entity-003272" type="composition"><tei:title level="m"
                  >Zarathustra</tei:title></tei:rs>.</tei:ab>

         <tei:ab corresp="r0003-se02">Mrs <tei:rs key="entity-000582" type="person">I. M.</tei:rs>
            unwell with influenza.</tei:ab>
      </tei:div>
   </sdo:record>
   <sdo:record ID="r0004">
