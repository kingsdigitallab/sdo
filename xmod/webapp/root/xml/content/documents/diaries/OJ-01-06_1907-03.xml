      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker March 17, 1907</dc:title>
         <dcterms:created>1907-03-17T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2012-08-17</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>
      <tei:div type="transcription">
         <tei:opener>
            <tei:date when="1907-03-17">Sonntg. 17</tei:date>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0012-se01">Letztes <tei:hi rend="lateinschr"><tei:rs
                  key="entity-003235" type="organization">Philh.</tei:rs>
               <tei:term>Konz</tei:term></tei:hi>: <tei:rs key="entity-000610" type="person"><tei:hi
                  rend="lateinschr">Mottl</tei:hi></tei:rs> dirigiert eine delikate Symph. von
               <tei:rs key="entity-000612" type="person"><tei:hi rend="underline"
               >Mozart</tei:hi></tei:rs>, B<tei:hi rend="supralinear">dur.</tei:hi> (<tei:rs
               key="entity-003276" type="composition"><tei:title>K.319</tei:title></tei:rs>), die
            voll Originalität u. echter Meisterschaft; <tei:rs key="entity-000094" type="person"
                  ><tei:hi rend="lateinschr">Brahms</tei:hi></tei:rs>
            <tei:rs key="entity-001355" type="composition"><tei:title><tei:hi rend="lateinschr"
                     >Haydn</tei:hi>-Variat.</tei:title></tei:rs> für Orch. erregen im <tei:hi
               rend="lateinschr">Publ.</tei:hi>
            <tei:hi rend="lateinschr">Entusiasmus</tei:hi>: das Steigen <tei:rs key="entity-000094"
               type="person"><tei:hi rend="lateinschr">Brahms</tei:hi></tei:rs>
            <tei:add place="interlinear-above">auch in der Empfindung des Publikums</tei:add>
            deutlich fühlbar.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0012-se02">Spaziergang bei <tei:rs key="entity-003275"
               type="place">Hütteldorf</tei:rs>. Herrlicher Frühlingstag bis +25°!</tei:ab>
      </tei:div>
      <tei:div type="translation">
         <tei:opener>
            <tei:date when="1907-03-17">Sun. 17</tei:date>
         </tei:opener>
         <tei:ab corresp="r0012-se01">Last <tei:rs key="entity-003235" type="organization"
               >Philharmonic</tei:rs>
            <tei:term>concert</tei:term>: <tei:rs key="entity-000610" type="person">Mottl</tei:rs>
            conducts a delicate symphony by <tei:rs key="entity-000612" type="person"><tei:hi
                  rend="underline">Mozart</tei:hi></tei:rs> in B<tei:g type="flat"/> major (<tei:rs
               key="entity-003276" type="composition"><tei:title>K 319</tei:title></tei:rs>), which
            is full of originality and true mastery; <tei:rs key="entity-000094" type="person"
               >Brahms</tei:rs>
            <tei:rs key="entity-001355" type="composition"><tei:title>Haydn
               Variations</tei:title></tei:rs> for orchestra excite enthusiasm in the audience:
               <tei:rs key="entity-000094" type="person">Brahms</tei:rs>'s rise <tei:add
               place="interlinear-above">even in public feeling</tei:add> is clearly
            palpable.</tei:ab>

         <tei:ab corresp="r0012-se02">Walk in the <tei:rs key="entity-003275" type="place"
               >Hütteldorf</tei:rs> area. Magnificent spring day, up to +25°!</tei:ab>
      </tei:div>
   </sdo:record>
