      <tei:div type="transcription">
         <tei:opener>
            <tei:dateline><tei:handShift new="#HS"/><tei:date when="1903-10-09">7‒9/.
               10</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0002-se01">Orchesterstimmen der „<tei:rs type="work"
               key="entity-001664"><tei:title>Syrischen Tänze</tei:title></tei:rs>“ für <tei:rs
               key="entity-000116" type="person">Busoni</tei:rs>’s Abend <tei:add
               place="interlinear-above">in <tei:rs type="place" key="entity-002501"
               >Berlin</tei:rs></tei:add><tei:note xml:id="r0002-fn01" type="editorial" place="foot"
               >For their performance by the Berlin Philharmonic under the baton of José Vianna da
               Motta, in an orchestral arrangement by Arnold Schoenberg, on November 5, 1903 in the
               Beethoven-Saal in Berlin, on the program of one of Busoni's Orchestral Evenings.
               Schoenberg's orchestration was never published, and the parts seem to have been
               lost.</tei:note> corrigiert.</tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:dateline><tei:handShift new="#HS"/><tei:date when="1903-10-09">October
                  7‒9</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab>Orchestral parts for my <tei:rs type="work" key="entity-001664"><tei:title
                  level="m">Syrian Dances</tei:title></tei:rs> for <tei:rs key="entity-000116"
               type="person">Busoni</tei:rs>'s evening <tei:add place="interlinear-above">in <tei:rs
                  type="place" key="entity-002501">Berlin</tei:rs></tei:add><tei:ptr
               corresp="#r0002-fn01" type="footnote"/> corrected.</tei:ab>
      </tei:div>
   </sdo:record>

