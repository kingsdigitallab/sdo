         </tei:opener>
         <tei:ab corresp="r0003-se01">Letter to <tei:rs type="person" key="entity-000750"
               >Rudorff</tei:rs>, see annex.<tei:ptr corresp="#r0003-fn01" type="footnote"
            /></tei:ab>
      </tei:div>
   </sdo:record>

