      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker July 22, 1908</dc:title>
         <dcterms:created>1908-07-22T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2017-11-26</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:pb n="90"/><tei:lb/>
            <tei:note type="editorial" place="inline">written on the <tei:hi rend="italice"
                  >verso</tei:hi> of a playbill for Wagner's <tei:title level="m">Das
                  Rheingold</tei:title></tei:note><tei:lb/>
            <tei:fw type="header" rend="center"><tei:handShift new="#HSblue"/>08</tei:fw>
            <tei:dateline><tei:handShift new="#HS"/>Mittw. <tei:date when="1908-07-22">22.
                  7.</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0001-se01">Nach schwerem Wettersturz wieder erster,
            herrlichster Tag. D<tei:supplied reason="abbreviation">ie</tei:supplied> Natur liegt wie
            neugeboren da. Kühle des vorausgegangenen Chaos beherrscht noch den Vormittag.</tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:pb n="90"/><tei:lb/>
            <tei:note type="editorial" place="inline">written on the <tei:hi rend="italice"
                  >verso</tei:hi> of a playbill for Wagner's <tei:title level="m">Das
                  Rheingold</tei:title></tei:note><tei:lb/>
            <tei:fw type="header" rend="center"><tei:handShift new="#HSblue"/>08</tei:fw>
            <tei:dateline><tei:handShift new="#HS"/>Wednesday, <tei:date when="1908-07-22">July
                  22</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab corresp="r0001-se01">After a sharp downturn in the weather, again a first
            magnificent day. Nature emerges like a new-born. The chill of the preceding chaos still
            prevails this morning.</tei:ab>
      </tei:div>
   </sdo:record>

   <sdo:record ID="r0002">
      <!--Use the itemDesc to group information about a single record, e.g. a single diary entry, lessonbook entry or piece of correspondence. 
