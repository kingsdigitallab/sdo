         <dc:title>Diary entry by Schenker April 29, 1909</dc:title>
         <dcterms:created>1909-04-29T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2018-04-13</dcterms:dateSubmitted>
