      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker April 3, 1909</dc:title>
         <dcterms:created>1909-04-03T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>The Date of Electronic Creation Goes Here:
            2018-04-13</dcterms:dateSubmitted>
         <dc:description>A summary of the contents of the record (currently unused)</dc:description>
         <dc:subject>A list of subjects (currently unused)</dc:subject>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:hi rend="bold">Ser. A,</tei:hi>
            <tei:pb n="98"/>
            <tei:fw type="header" rend="center"><tei:handShift new="#HS"/><tei:date when="1909-04"
                  >4./09</tei:date></tei:fw><tei:lb/>
            <tei:dateline><tei:date when="1909-04-03">3. 4.</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab type="sub-entry" xml:id="r0001-se01">Sven Hedin’s Vortrag im gr. Musikvereinssaal:
            Mir selbst solche Sucht nach unentdeckten Punkten der Erde <tei:add
               place="interlinear-above"><tei:handShift new="#HSpencil"/>völlig</tei:add>
            <tei:handShift new="#HS"/>unbegreiflich.<tei:note xml:id="r0001-fn01" type="editorial"
               place="foot">The Swedish geographer, explorer, and photographer Dr. Sven von Hedin's
               lecture, at 7.30 on April 3, which was sold out in advance, was presented with
               lantern slides. Whilst Hedin's topic is not stated in the newspapers, Schenker's
               comment suggests that it concerned his recent expedition to Chinese Turkistan
               (Xinjiang) and Tibet.</tei:note></tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:hi rend="bold">Ser. A,</tei:hi>
            <tei:pb n="98"/>
            <tei:fw type="header" rend="center"><tei:handShift new="#HS"/><tei:date when="1909-04"
                  >April 1909</tei:date></tei:fw><tei:lb/>
            <tei:dateline><tei:date when="1909-04-03">April 3</tei:date></tei:dateline>
         </tei:opener>
         <tei:ab corresp="r0001-se01">Sven Hedin's lecture in the large hall of the Musikverein
            Building: To me, such mania for undiscovered parts of the planet <tei:supplied
               reason="clarity">is</tei:supplied>
            <tei:add place="interlinear-above"><tei:handShift new="#HSpencil"/>completely</tei:add>
            <tei:handShift new="#HS"/>inexplicable.<tei:ptr corresp="#r0001-fn01" type="footnote"
            /></tei:ab>
      </tei:div>
   </sdo:record>

