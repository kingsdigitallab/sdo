      <tei:div type="transcription">
         <tei:opener>
            <tei:date when="1906-09-06">Donn. 6/9</tei:date> Zu wenig geschlafen. 7<tei:hi
               rend="supralinear">h.</tei:hi> früh <tei:measure type="temperature" unit="reaumur"
               >16°</tei:measure>
            <tei:hi rend="lateinschr">R.</tei:hi>
            <tei:note type="editorial" place="inline">= 20° C.</tei:note> im Schatten. Erster Wind,
            erste Wolken seit Tagen.</tei:opener>

         <tei:ab type="sub-entry" xml:id="r0006-se01"> </tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:date when="1906-09-06">Thursday, September 6.</tei:date> Had too little sleep. 7
            a.m. <tei:measure type="temperature" unit="reaumur">16°</tei:measure> Réaumur <tei:note
               type="editorial" place="inline">= 20° C.</tei:note> in the shade. The first wind and
            first clouds for several days.</tei:opener>

         <tei:ab corresp="r0006-se01"> </tei:ab>
      </tei:div>
   </sdo:record>

