      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker August 31 ‒ September 1, 1906</dc:title>
         <dcterms:created>1906-09-01T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>2017-05-26</dcterms:dateSubmitted>
         <dc:description/>
         <dc:subject/>
      </sdo:itemDesc>

      <tei:div type="transcription">
         <tei:opener>
            <tei:date when="1906-09-01"><tei:handShift rend="#HS-red"/>Frtg, Samstag, 31/8
               1/9,</tei:date>
         </tei:opener>

         <tei:ab type="sub-entry" xml:id="r0001-se01">An den Korrekturen. <tei:rs
               key="entity-002405" type="organization">Cotta</tei:rs> drängt.<tei:note
               type="editorial" place="inline">= <tei:ref type="correspondence" cRef="OJ-9-31_11">OJ
                  9/31, [11]</tei:ref></tei:note>
            <tei:supplied reason="omission"><tei:milestone unit="section" rend="inline em-dash"
               /></tei:supplied> Herrlichste Tage.</tei:ab>

         <tei:ab type="sub-entry" xml:id="r0001-se02"><tei:rs key="entity-000599" type="person"
                  ><tei:hi rend="lateinschr">Mittelmann</tei:hi></tei:rs> erklärt astronomische
            Grundbegriffe. <tei:milestone unit="section" rend="inline em-dash"/> Wohnung endlich
            ganz in Ordnung gebracht.</tei:ab>
      </tei:div>

      <tei:div type="translation">
         <tei:opener>
            <tei:date when="1906-09-01"><tei:handShift rend="#HS-red"/>Friday and Saturday,
               September 31 and September 1</tei:date>
         </tei:opener>

         <tei:ab corresp="r0001-se01">I work on proofs. <tei:rs key="entity-002405"
               type="organization">Cotta</tei:rs> is hounding me.<tei:note type="editorial"
               place="inline">= <tei:ref type="correspondence" cRef="OJ-9-31_11">OJ 9/31,
                  [11]</tei:ref></tei:note>
            <tei:supplied reason="omission"><tei:milestone unit="section" rend="inline em-dash"
               /></tei:supplied> Days of glorious weather.</tei:ab>

         <tei:ab corresp="r0001-se02"><tei:rs key="entity-000599" type="person">Mittelmann</tei:rs>
            explains the basic concepts of astronomy. <tei:milestone unit="section"
               rend="inline em-dash"/> The apartment finally in order.</tei:ab>
      </tei:div>
   </sdo:record>

