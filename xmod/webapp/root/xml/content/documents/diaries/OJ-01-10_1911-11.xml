   <sdo:record ID="r0004">

      <sdo:itemDesc>
         <dc:title>Diary entry by Schenker November [15], 1911</dc:title>
         <dcterms:created>1911-11-15T12:00:00Z</dcterms:created>
         <dcterms:dateSubmitted>2020-02-05</dcterms:dateSubmitted>
         <dc:description/>
