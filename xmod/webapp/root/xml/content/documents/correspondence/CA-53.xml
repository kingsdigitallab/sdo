      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002748">Telegram</tei:rs>

      <dc:title>Telegram from Schenker to Cotta, dated November 4, 1906</dc:title>

      <dcterms:created>1906-11-04T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2017-05-18</dcterms:dateSubmitted>
      <dc:description>Schenker has mailed a duplicate of gathering 28.</dc:description>
      <dc:subject>Duplikat; duplicate </dc:subject>

      <marcrel:correspondent key="entity-000632">HSchenker</marcrel:correspondent>
      <marcrel:recipient key="entity-002405">Cotta</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:note type="editorial" place="inline"><tei:hi rend="bold">Note:</tei:hi> Roman = printed;
          <tei:hi rend="italic">italic</tei:hi> = handwritten; <tei:hi rend="bold">bold
          Roman</tei:hi> = teletype-printed; [/] = new line</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:note type="editorial">top-left panel:</tei:note>
      <tei:note xml:id="r0001-fn01" type="editorial" place="foot"> Sending of this telegram is not
        recorded in Schenker's diary. See <tei:ref type="correspondence" cRef="CA-52">CA
          52</tei:ref> for circumstances surrounding it.</tei:note>
      <tei:ab type="formpart" rend="printed">(15) [/] Leitung Nr. 4 [/] Telegramm Nr. ....
        <tei:lb/>Aufgenommen von Innsbruck [/] den <tei:hi rend="italic">4</tei:hi> | <tei:hi
          rend="italic">11</tei:hi> um <tei:hi rend="italic">9</tei:hi> Uhr <tei:hi rend="italic"
          >40</tei:hi> M. [/] durch <tei:unclear reason="reading"><tei:hi rend="italic"
            >Wien</tei:hi></tei:unclear>
      </tei:ab>
      <tei:note type="editorial" place="inline">top-center panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed"><tei:hi rend="bold">cotta stgt </tei:hi> [/] Stuttgart
        [/] Telegramm [/] Amt Stuttgart</tei:ab>
      <tei:note type="editorial" place="inline">top-right panel: left blank</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:note type="editorial" place="inline">middle panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed">Telegramm aus <tei:hi rend="bold">wien 49+251 6 4 8 50
          m =</tei:hi> .. um ...Uhr ... Min. ...</tei:ab>
      <tei:note type="editorial" place="inline">bottom panel:</tei:note>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed"><tei:hi rend="bold">duplikat</tei:hi><tei:note
          xml:id="r0001-fn02" type="editorial" place="foot">i.e., a duplicate of gathering 28, which
          Schenker had sent but Cotta not received.: see <tei:ref type="correspondence" cRef="CA-52"
            >CA 52</tei:ref>.</tei:note>
        <tei:hi rend="bold"> express abgegangen schenker ="</tei:hi></tei:ab>
    </tei:div>


    <tei:div type="translation">
      <tei:note type="editorial" place="inline"><tei:hi rend="bold">Note:</tei:hi> Roman = printed;
          <tei:hi rend="italic">italic</tei:hi> = handwritten; <tei:hi rend="bold">bold
          Roman</tei:hi> = teletype-printed; [/] = new line</tei:note>
      <tei:lb/>
      <tei:lb/>
      <tei:ab type="formpart" rend="printed">
        <tei:note type="editorial">top-left panel:</tei:note>
        <tei:ptr corresp="#r0001-fn01" type="footnote"/><tei:lb/> (15) [/] Line No. 4 [/] Telegram
        No. .... <tei:lb/>Received from Innsbruck [/] <tei:hi rend="italic">4</tei:hi> | <tei:hi
          rend="italic">11</tei:hi> at <tei:hi rend="italic">9.40</tei:hi> a.m. [/] via <tei:unclear
          reason="original_unclear"><tei:hi rend="italic">Vienna</tei:hi></tei:unclear>
      </tei:ab>

      <tei:ab type="formpart" rend="printed">
        <tei:note type="editorial" place="inline">top-center panel:</tei:note>
        <tei:lb/>
        <tei:hi rend="bold">cotta stuttgart </tei:hi> [/] Stuttgart [/] Telegram [/] Office
