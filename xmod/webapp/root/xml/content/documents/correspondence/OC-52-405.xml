  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-008373">Prov.Bank ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002746">Bank document</tei:rs>

      <dc:title>Notice of bank transaction, UE to Schenker, dated June 5, 1912</dc:title>

      <dcterms:created>1912-06-05T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-01-23</dcterms:dateSubmitted>
      <dc:description>The Austrian Provincial Bank notifies Schenker of 100 Kronen sent to him on
        behalf of Universal Edition.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002467">Austrian Provincial Bank</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:note type="editorial" place="pre-text">Roman type = printed; Italic type = handwritten or
        stamp</tei:note>

      <tei:opener>
        <tei:address>
          <tei:addrLine>KAIS. KÖN. PRIVILEGIRTE.</tei:addrLine>
          <tei:addrLine>ÖSTERREICHISCHE LÄNDERBANK.</tei:addrLine>
          <tei:addrLine>Telegramm-Addresse. </tei:addrLine>
          <tei:addrLine>LAENDERBANK.</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:placeName>Wien</tei:placeName>, am <tei:date
            when="1912-06-05"><tei:hi rend="italic">5. Juni</tei:hi> 19<tei:hi rend="italic"
              >12</tei:hi></tei:date></tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:hi rend="italic">Wohlg. Herrn D<tei:hi rend="supralinear">r.</tei:hi>
              <tei:persName>Heinrich Schenker</tei:persName></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70">
            <tei:hi rend="underline">Wien <tei:hi rend="italic">III</tei:hi></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:hi rend="italic">Reisnerstr.
            38</tei:hi></tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:note type="editorial" place="inline">Panel down left side with addresses, etc.</tei:note>
      <tei:lb/>
      <tei:lb/>

      <tei:p rend="first">Im Auftrage <tei:hi rend="italic">d. Universal Edition AG.</tei:hi> und
        für Rechnung <tei:note type="editorial" place="inline">blank</tei:note>
        <tei:hi rend="italic">Wien</tei:hi> senden wir Ihnen <tei:hi rend="italic">per <tei:rs
            type="organization" key="entity-003190">Postsparkasse</tei:rs></tei:hi>
        <tei:cit>
          <tei:q><tei:hi rend="italic">K 100–</tei:hi></tei:q>
        </tei:cit><tei:note xml:id="r0001-fn01" type="editorial" place="foot">In payment for the
          "Vorbemerkungen" ("Preliminary Remarks") to <tei:rs type="work" key="entity-001729"
            >Niloff, <tei:title level="m">Instrumentations-Tabelle</tei:title></tei:rs>; see
            <tei:ref type="corespondence" cRef="OC-52-404">OC 52/404</tei:ref>, June 4, 1912, for
          Hertzka's notification.</tei:note>
      </tei:p>

      <tei:p rend="first">worüber wir uns Ihre gefl. Empfangsanzeige erbitten.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Achtungsvoll</tei:salute>
        <tei:salute rend="indent-60">Für die Bank-Abteilung der</tei:salute>
        <tei:signed rend="indent-50">
          <tei:rs type="organization" key="entity-002467">KAIS. KÖN. PRIVILEGIRTEN ÖSTERREICHISCHEN
            LÄNDERBANK</tei:rs></tei:signed>
        <tei:note type="editorial" place="inline">initialled – illegible</tei:note>
      </tei:closer>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine>IMPERIAL-ROYAL PRIVILEGED</tei:addrLine>
          <tei:addrLine>AUSTRIAN PROVINCIAL BANK</tei:addrLine>
          <tei:addrLine>Telegram address:</tei:addrLine>
          <tei:addrLine>LAENDERBANK.</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:placeName>Vienna</tei:placeName>, <tei:date
            when="1912-06-05"><tei:hi rend="italic">June 5,</tei:hi> 19<tei:hi rend="italic"
              >12</tei:hi></tei:date></tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:hi rend="italic">Dr. <tei:persName>Heinrich
              Schenker</tei:persName></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70">
            <tei:hi rend="underline">Vienna <tei:hi rend="italic"
            >III</tei:hi></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-70"><tei:hi rend="italic">Reisnerstrasse
            38</tei:hi></tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:note type="editorial" place="inline">Panel down left side with addresses, etc.</tei:note>
      <tei:lb/>
      <tei:lb/>

      <tei:p rend="first">On instructions from <tei:hi rend="italic">Universal Edition &amp;
          Co.</tei:hi>, and to the account of <tei:note type="editorial" place="inline"
          >blank</tei:note>
        <tei:hi rend="italic">Vienna</tei:hi>, we are sending to you <tei:hi rend="italic">by the
            <tei:rs type="organization" key="entity-003190">Post Office Savings
          Bank</tei:rs></tei:hi>
        <tei:cit>
          <tei:q><tei:hi rend="italic">100– Kronen</tei:hi></tei:q>
        </tei:cit><tei:ptr corresp="#r0001-fn01" type="footnote"/>
      </tei:p>

      <tei:p rend="first">for which we request your written acknowledgement of receipt.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Respectfully,</tei:salute>
        <tei:salute rend="indent-60">For the bank division of the </tei:salute>
        <tei:signed rend="indent-50">
          <tei:rs type="organization" key="entity-002467">IMPERIAL-ROYAL PRIVILEGED AUSTRIAN
            PROVINCIAL BANK</tei:rs></tei:signed>
        <tei:note type="editorial" place="inline">initialled – illegible</tei:note>
      </tei:closer>

    </tei:div>

