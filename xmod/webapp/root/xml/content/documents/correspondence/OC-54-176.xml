  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs key="entity-005444" type="correspondence">Mandruck ~ HSchenker</tei:rs>
      <tei:rs key="entity-002744" type="corr-item-type">Letter</tei:rs>

      <dc:title>Typed letter from Mandruck to Schenker, dated June 17, 1927</dc:title>
      <dcterms:created>1927-06-17T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-04-03</dcterms:dateSubmitted>
      <dc:description>Mandruck enclose further page proofs for the second Meisterwerk
        volume, along with the corresponding galley proofs.</dc:description>
      <dc:subject>page proof; Bogen; galley proof; Fahnen</dc:subject>

    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK AG</tei:addrLine>
          <tei:addrLine>MÜNCHEN</tei:addrLine>
          <tei:addrLine>ABTEILUNG: <tei:hi rend="underline">BUCHDRUCK</tei:hi></tei:addrLine>
        </tei:address>
        <tei:dateline><tei:placeName>MÜNCHEN</tei:placeName>, THERESIENSTR. 75, <tei:handShift
            new="#TPhd"/><tei:date when="1927-06-17">17. Juni 27</tei:date>.</tei:dateline>
        <tei:lb/>
        <tei:byline><tei:hi rend="italic">H./D.</tei:hi></tei:byline>
        <tei:lb/>
        <tei:address>
          <tei:addrLine rend="indent-30">Herrn Dr.
              Heinrich <tei:hi rend="sperr">Schenker</tei:hi><tei:note xml:id="r0001-fn01"
              type="editorial" place="foot">Receipt of this letter is recorded in Schenker's diary
              at OJ 3/9, p. 3078, June 20, 1927: "Vom Mandruck Bogen 7–8." ("From Mandruck,
              gatherings 7–8.").</tei:note></tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="underline"><tei:hi rend="sperr">Wien</tei:hi>
              III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse <tei:sic>3</tei:sic></tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/>
      <tei:lb/>
      <tei:lb/>

      <tei:p rend="first">In der Anlage erhalten Sie weitere umbrochene Bogen Nr. 7 &amp; 8 des
          <tei:rs key="entity-001751" type="work">Musikjahrbuches</tei:rs> in dreifacher Ausführung,
        nebst den dazugehörigen Fahnen – Abzügen zur gefl. Bedienung.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129"
            type="organization">MANDRUCK A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK &amp; Co.</tei:addrLine>
          <tei:addrLine>MUNICH</tei:addrLine>
          <tei:addrLine>DEPARTMENT: <tei:hi rend="underline">PRINTING</tei:hi></tei:addrLine>
        </tei:address>
        <tei:dateline><tei:placeName>MUNICH</tei:placeName>, THERESIENSTR. 75, <tei:handShift
            new="#TPhd"/><tei:date when="1927-06-17">June 17, 1927</tei:date>.</tei:dateline>
        <tei:lb/>
        <tei:byline><tei:hi rend="italic">H./D.</tei:hi></tei:byline>
        <tei:lb/>
        <tei:address>
          <tei:addrLine rend="indent-30">Dr. Heinrich
                <tei:hi rend="sperr">Schenker</tei:hi><tei:ptr corresp="#r0001-fn01"
              type="footnote"/></tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="underline"><tei:hi rend="sperr"
                >Vienna</tei:hi> III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse <tei:sic>3</tei:sic></tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/>
      <tei:lb/>
      <tei:lb/>

      <tei:p rend="first">In the enclosures you will receive further page proofs of your <tei:rs
          key="entity-001751" type="work">music yearbook</tei:rs>, gatherings 7 and 8, in
        triplicate, together with the a printout of the galley proofs that belong to them, for your
        kind consideration.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">Yours respectfully,</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129"
            type="organization">MANDRUCK &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

    </tei:div>
  </sdo:record>
