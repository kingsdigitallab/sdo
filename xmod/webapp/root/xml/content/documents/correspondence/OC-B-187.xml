  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-005596">AltmannW ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-005534">Preussische
        Staatsbibliothek ~ HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Handwritten postcard from Wilhelm Altmann to Schenker, dated November 14,
        1924</dc:title>
      <dcterms:created>1924-11-14T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2015-06-09</dcterms:dateSubmitted>
      <dc:description>Schenker may hold on to the parts of the St Matthew Passion for some
        time.</dc:description>
      <dc:subject>Photographien; photographs; Einsendung; return; Porto; postage costs</dc:subject>



      <marcrel:correspondent key="entity-000013">Wilhelm Altmann</marcrel:correspondent>
      <marcrel:correspondent key="entity-003292">Preussische
        Staatsbibliothek</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
            >Postkarte</tei:hi></tei:fw><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An: </tei:supplied>
              <tei:handShift new="#unk"/>Herrn Prof. Dr. H. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:hi rend="underline">Wien III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="stamp"><tei:note type="editorial" place="inline">circular stamp, around
              eagle crest:</tei:note>PREUSSISCHE STAATSBIBLIOTHEK * BERLIN *</tei:fw><tei:lb/>
          <tei:fw type="postmark"><tei:note type="editorial" place="inline">x 3</tei:note>|| BERLIN
            NW | 15. 11. 24 4–5N | * 7 x ||</tei:fw><tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/>
          <tei:address>
            <tei:addrLine rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
                >Preussische Staatsbibliothek : Musikabteilung</tei:hi></tei:addrLine>
            <tei:addrLine>TAGEB. NR <tei:handShift new="#WA"/>1863/24</tei:addrLine>
            <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>BERLIN NW7, den <tei:date
                when="1924-11-14"><tei:handShift new="#WA"/>14. 11. <tei:handShift new="#printed"
                  />192<tei:handShift new="#WA"/>4</tei:date></tei:addrLine>
          </tei:address>
        </tei:fw>
        <tei:salute rend="indent-20">Hochverehrter Herr <tei:rs key="entity-000632" type="person"
            >Professor</tei:rs>.<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this postcard is recorded in Schenker's diary at OJ 3/7, p. 2755, November 17, 1924:
            "Von Altmann (K.): die autographen Blätter kann ich längere Zeit behalten, da sie
            hauptsächlich für mich gemacht worden sind, nur um die Vergütung des Portos bitte er."
            ("From Altmann (postcard): I can keep the autograph leaves longer, since they were made
            principally for me, he asks only for the postage to be repaid.") See also <tei:ref
              type="correspondence" cRef="OC-B-185">OC B/185</tei:ref>, October 30,
          1924.</tei:note></tei:salute>
      </tei:opener>

      <tei:p>Die Photographien der <tei:rs key="entity-000027" type="person"
          >Bach</tei:rs>-<tei:term>Stimme</tei:term>n können Sie ruhig eine ganze Weile behalten, da
        sie ja in erster Linie Jhretwegen angefertigt sind. Dagegen bitte ich um umgehende
        Einsendung des ausgelegten Portos an die angegebene Adresse.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">Mit ausgezeichneter Hochachtung und verbindlichstem
          Gruß</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-000013" type="person"
          >Altmann</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
            >Postcard</tei:hi></tei:fw><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">To: </tei:supplied>
              <tei:handShift new="#unk"/>Prof. H. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:hi rend="underline">Vienna
              III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="stamp"><tei:note type="editorial" place="inline">circular stamp, around
              eagle crest:</tei:note>PRUSSIAN STATE LIBRARY * BERLIN *</tei:fw><tei:lb/>
          <tei:fw type="postmark"><tei:note type="editorial" place="inline">x 3</tei:note>|| BERLIN
            NW | 15. 11. 24 4–5 PM | * 7 x ||</tei:fw><tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/>
          <tei:address>
            <tei:addrLine rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold">Prussian
                State Library : Music Department</tei:hi></tei:addrLine>
            <tei:addrLine>DAYBOOK NO. <tei:handShift new="#WA"/>1863/24</tei:addrLine>
            <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>BERLIN NW7, <tei:date
                when="1924-11-14"><tei:handShift new="#WA"/>November 14, <tei:handShift
                  new="#printed"/>192<tei:handShift new="#WA"/>4</tei:date></tei:addrLine>
          </tei:address>
        </tei:fw>
        <tei:salute rend="indent-20">Highly revered <tei:rs key="entity-000632" type="person"
            >Professor</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p>You may hold on to the photographs of the <tei:rs key="entity-000027" type="person"
          >Bach</tei:rs>
        <tei:term>part</tei:term>s in peace for a good while, since they were prepared first and
        foremost for your sake. By contrast I request you to send by return the postage paid, to the
        address provided.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">With kindest regards and warmest greetings</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-000013" type="person"
          >Altmann</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>
