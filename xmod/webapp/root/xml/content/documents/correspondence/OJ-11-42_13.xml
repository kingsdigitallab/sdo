      <dc:title>Handwritten postcard from Maximilian Harden to Schenker, dated May 1,
        1894</dc:title>
      <dcterms:created>1894-05-01T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-12-08</dcterms:dateSubmitted>
      <dc:description>"The road is clear."</dc:description>
      <dc:subject/>



      <marcrel:correspondent key="entity-000333">Maximilian Harden</marcrel:correspondent>
      <marcrel:correspondent key="entity-002566">Die Zukunft</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Deutsche
          Reichspost</tei:fw>
        <tei:fw type="header" rend="center">Postkarte</tei:fw>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>An: <tei:handShift new="#MH"/>Herrn Dr. <tei:hi rend="lateinschr">H.
                Schenker</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>in <tei:handShift
                new="#MH"/><tei:hi rend="lateinschr">Wien</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>Wohnung <tei:handShift
                new="#MH"/>III Traungasse 1</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| BERLIN W. c | 9 | 1/5 | 94 | 5½-6N. ||</tei:fw>
          <tei:fw type="postmark">|| WIEN 3/3 | 49 | BESTELLT | 2.-5.94 | 6N. ||</tei:fw>
          <tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/><tei:lb/>
          <tei:note type="editorial" place="inline">in vertical format</tei:note>
        </tei:fw>
      </tei:opener>

      <tei:p>Die Bahn ist frei,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">It is
          unclear to what this refers.</tei:note> lieber Herr <tei:rs key="entity-000632"
          type="person">Doktor</tei:rs>, </tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">und es grüßt Sie</tei:salute>
        <tei:salute rend="indent-50">Ihr müder</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-000333" type="person">
          Harden</tei:rs></tei:signed><tei:lb/><tei:lb/>
        <tei:date><tei:date when="1894-05-01">1. 5. 94</tei:date></tei:date>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>German Imperial
          Post</tei:fw>
        <tei:fw type="header" rend="center">Postcard</tei:fw>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>To: <tei:handShift new="#MH"/>Dr. H. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>in <tei:handShift
                new="#MH"/>Vienna III</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:handShift new="#printed"/>Address: <tei:handShift
                new="#MH"/>Traungasse 1</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| BERLIN W. c | 9 | 1/5 | 94 | 5½-6 P.M. ||</tei:fw>
          <tei:fw type="postmark">|| VIENNA 3/3 | 49 | DELIVERED | 2.-5.94 | 6 P.M. ||</tei:fw>
          <tei:lb/><tei:lb/>
          <tei:milestone unit="folio" n="verso"/><tei:lb/>
          <tei:note type="editorial" place="inline">in vertical format</tei:note>
        </tei:fw>
      </tei:opener>

      <tei:p>The road is clear,<tei:ptr corresp="#r0001-fn01" type="footnote"/> my dear Dr.
          <tei:supplied reason="identity"><tei:rs key="entity-000632" type="person"
            >Schenker</tei:rs></tei:supplied>,</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">and greetings from</tei:salute>
        <tei:salute rend="indent-50">Your tired</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-000333" type="person">
          Harden</tei:rs></tei:signed><tei:lb/><tei:lb/>
        <tei:date><tei:date when="1894-05-01">May 1, 1894</tei:date></tei:date>
      </tei:closer>

    </tei:div>
