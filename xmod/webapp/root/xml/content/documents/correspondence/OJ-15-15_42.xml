      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Handwritten postcard from Weisse to Schenker, dated June 5, 1929</dc:title>

      <dcterms:created>1929-06-05T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2013-09-12</dcterms:dateSubmitted>
      <dc:description>Weisse confirms arrangements to meet Schenker on June 11.</dc:description>
      <dc:subject/>

    <marcrel:correspondent key="entity-000951">Weisse</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient><marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline">printed:</tei:note>
          <tei:handShift new="#printed"/>Postkarte</tei:fw>
        <tei:lb/><tei:note type="editorial" place="inline">top-left, picture captioned: <tei:rs key="entity-002304" type="place">Innsbruck</tei:rs>, <tei:rs key="entity-002334" type="place">Tirol</tei:rs></tei:note><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">Absender:</tei:supplied>
              <tei:handShift new="#HW"/><tei:hi rend="supralinear">Dr</tei:hi> H Weisse,
              XIII.</tei:addrLine>
            <tei:addrLine rend="indent-30">Wattmanngasse 5.</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied> Herrn Dr.</tei:addrLine>
            <tei:addrLine rend="indent-10">Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-30">Keilgasse 8</tei:addrLine>
            <tei:addrLine rend="indent-50">Wien III.</tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| <tei:gap reason="illegible" extent="1" unit="lines"/> | 5. VI.
            29. 18 | * 1c * ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-75"><tei:date when="1929-06-05">5. Juni
          29</tei:date>.</tei:dateline>
        <tei:salute>Lieber verehrter <tei:rs key="entity-000632" type="person">Meister</tei:rs>,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
            this postcard is recorded in Schenker's diary at OJ 4/2, p. 3344 (June 7, 1929): "Von
            Weisse (K.): wird Mittwoch abends erscheinen." ("From Weisse (postcard): he will appear
            on Wednesday evening.")</tei:note>
        </tei:salute>
      </tei:opener>

      <tei:p rend="first">besten Dank – werde also <tei:date when="1929-06-12">Mittwoch</tei:date>
        pünktlich zwischen ¼ und ½8 bei Ihnen sein; freue mich schon sehr<tei:supplied reason="punctuation">,</tei:supplied> Sie zu sehen.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">Allerherzlichste Grüße von uns beiden.</tei:salute>
        <tei:salute rend="indent-50">Ihr</tei:salute>
        <tei:signed rend="indent-75"><tei:rs key="entity-000951" type="person">H</tei:rs>.</tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline">printed:</tei:note>
          <tei:handShift new="#printed"/>Postcard</tei:fw>
        <tei:lb/><tei:note type="editorial" place="inline">top-left, picture captioned: <tei:rs key="entity-002304" type="place">Innsbruck</tei:rs>, <tei:rs key="entity-002334" type="place">Tyrol</tei:rs></tei:note><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">from:</tei:supplied>
              <tei:handShift new="#HW"/>Dr. H. Weisse, <tei:supplied reason="clarity">Vienna</tei:supplied> XIII</tei:addrLine>
            <tei:addrLine rend="indent-30">Wattmanngasse 5</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">to:</tei:supplied> Dr.</tei:addrLine>
            <tei:addrLine rend="indent-10">Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-30">Keilgasse 8</tei:addrLine>
            <tei:addrLine rend="indent-50">Vienna III</tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| <tei:gap reason="illegible" extent="1" unit="lines"/> | 5. VI.
            29. 18 | * 1c * ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-75"><tei:date when="1929-06-05">June 5,
          1929</tei:date></tei:dateline>
        <tei:salute>Dear revered <tei:rs key="entity-000632" type="person">Master</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/>
        </tei:salute>
      </tei:opener>

      <tei:p rend="first">Many thanks: I shall then be at your place promptly between a quarter and
        half past seven on <tei:date when="1929-06-12">Wednesday</tei:date>. I am already looking
        forward to seeing you very much.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">Most cordial greetings from the two of us.</tei:salute>
        <tei:salute rend="indent-50">Yours,</tei:salute>
        <tei:signed rend="indent-75"><tei:rs key="entity-000951" type="person">Hans</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


