  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>



      <dc:title>Handwritten postcard from Stedler(?) (UE) to Schenker, dated July 4, 1902</dc:title>

      <dcterms:created>1902-07-04T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2016-02-04</dcterms:dateSubmitted>
      <dc:description>Asks for the manuscript copy of C. P. E. Bach Klavierwerke to be
        returned.</dc:description>
      <dc:subject>Zuschrift; letter; Correctur-Abzüge; proofs; Eintragungs-Manuscript; manuscript;
        Correcturen; corrections; Druckerei; printing works; printers</dc:subject>


      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">All in a decorated rule-frame</tei:note><tei:lb/>
        <tei:fw type="envelope">
          <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
              >Correspondenz-Karte</tei:hi></tei:fw><tei:lb/>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied>
              <tei:handShift new="#UEsten1"/>Sr Wolgeboren Herrn Professor Heinrich
              Schenker</tei:addrLine>
            <tei:addrLine><tei:handShift new="#printed"/>in <tei:handShift new="#UEsten1"/>Baden
              b/Wien</tei:addrLine>
            <tei:addrLine>Johannesgasse 5.</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| WIEN <tei:gap reason="illegible"/> | <tei:gap
              reason="illegible"/> | 6–7N ||</tei:fw>
          <tei:fw type="postmark">|| BADEN 1 | N.Ö. | BESTELLT | 5. 7. 02 | 7—10V ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:note type="editorial" place="inline">handwritten on printed lines</tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#UEsten1"/>Sr. Wolgeboren Herrn Professor Heinrich
            Schenker</tei:addrLine>
        </tei:address>
        <tei:salute><tei:handShift new="#UEsten2"/>Hochverehrter Herr <tei:rs type="person"
            key="entity-000632">Professor</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p rend="first"><tei:handShift new="#UEsten1"/>Wir erhalten soeben Ihre geschätzte
          Zuschrift,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">This letter is not
          known to survive.</tei:note> sowie die <tei:hi rend="lateinschr">Correctur-Abzüge zu
            <tei:rs key="entity-001720" type="work">Ph. Em. Bach</tei:rs></tei:hi>. Leider wurde uns
        jedoch das Eintragungs-Manuscript nicht retournirt, ohne welche eine Durchführung der
        Correcturen seitens unserer Druckerei nicht möglich ist. Wir erbitten deshalb freundliche
          <tei:hi rend="underline">postwendende</tei:hi> Einsendung des Manuscriptes und zeichnen
        inzwischen</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#printed"/>„<tei:rs key="entity-002408"
            type="organization">Universal-Edition</tei:rs>“ Actiengesellschaft</tei:signed>
        <tei:signed><tei:handShift new="#St"/><tei:unclear reason="reading"
          >Stedler</tei:unclear></tei:signed><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>Wien, am <tei:handShift new="#UEsten1"/>4./7.
              <tei:handShift new="#printed"/>190<tei:handShift new="#UEsten1"/>2.</tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/>I., Maximilianstrasse Nr. 11.</tei:addrLine>
        </tei:address>
      </tei:closer>


    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">All in a decorated rule-frame</tei:note><tei:lb/>
        <tei:fw type="envelope">
          <tei:fw type="header" rend="center"><tei:handShift new="#printed"/><tei:hi rend="bold"
              >Postcard</tei:hi></tei:fw><tei:lb/>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied>
              <tei:handShift new="#UEsten1"/>Professor Heinrich Schenker, Esq.</tei:addrLine>
            <tei:addrLine><tei:handShift new="#printed"/>in <tei:handShift new="#UEsten1"/>Baden
              near Vienna</tei:addrLine>
            <tei:addrLine>Johannesgasse 5</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| VIENNA <tei:gap reason="illegible"/> | <tei:gap
              reason="illegible"/> | 6–7N ||</tei:fw>
          <tei:fw type="postmark">|| BADEN 1 | Lower Austria | DELIVERED | 5. 7. 02 | 7—10 A.M.
            ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:note type="editorial" place="inline">handwritten on printed lines</tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#UEsten1"/>Professor Heinrich Schenker,
            Esq.</tei:addrLine>
        </tei:address>
        <tei:salute><tei:handShift new="#UEsten2"/>Highly revered <tei:rs type="person"
            key="entity-000632">Professor</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p rend="first"><tei:handShift new="#UEsten1"/>We are in receipt of you esteemed
          letter,<tei:ptr corresp="#r0001-fn01" type="footnote"/> and also of the corrected proofs
        for <tei:rs key="entity-001720" type="work">C. P. E. Bach</tei:rs>. Unfortunately, however,
        the manuscript was not returned to us, and without this it is impossible for the printers to
        carry out the corrections. Please, therefore, be so kind as to send the manuscript to us
          <tei:hi rend="underline">by return of post</tei:hi>. We remain, meanwhile</tei:p>

      <tei:closer>
        <tei:salute rend="indent-70">with kind regards</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#printed"/><tei:rs key="entity-002408"
            type="organization">Universal Edition</tei:rs> &amp; Co.</tei:signed>
        <tei:signed><tei:handShift new="#St"/><tei:unclear reason="reading"
          >Stedler</tei:unclear></tei:signed><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>Vienna, <tei:handShift new="#UEsten1"/>July
            4, <tei:handShift new="#printed"/>190<tei:handShift new="#UEsten1"/>2</tei:addrLine>
          <tei:addrLine><tei:handShift new="#printed"/>I, Maximilianstrasse 11</tei:addrLine>
        </tei:address>
      </tei:closer>

    </tei:div>
