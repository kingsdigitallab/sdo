      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>

      <dc:title>Handwritten letter from Walter and Margarete Dahms to Schenker, dated June 17,
        1926</dc:title>

      <dcterms:created>1926-06-17T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2012-03-23</dcterms:dateSubmitted>
      <dc:description>Birthday greetings. </dc:description>
      <dc:subject>Geburtstag; birthday</dc:subject>

      
      
      
    <marcrel:correspondent key="entity-000149">Dahms</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient><marcrel:correspondent key="entity-000149">DahmsW</marcrel:correspondent><marcrel:correspondent key="entity-002817">DahmsM</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient><marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:salute rend="indent-10">Lieber und verehrter <tei:rs type="person" key="entity-000632">Meister</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
            this letter is recorded in Schenker's diary at OJ 3/8, p. 2950, June 21, 1926: "Von
            Dahms (Br.): gratuliert." ("From Dahms (letter): offers his congratulations.").
          </tei:note>
        </tei:salute>
      </tei:opener>

      <tei:p rend="first">Zu Ihrem Geburtstag senden wir Ihnen unsere allerherzlichsten Grüsse und
        Wünsche! </tei:p>


      <tei:closer>
        <tei:signed rend="indent-30"><tei:rs type="person" key="entity-000149">Walter </tei:rs>und
            <tei:rs key="entity-002817" type="person">Margarete</tei:rs> Dahms</tei:signed>
        <tei:dateline><tei:date when="1926-06-17">17. VI. 1926</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-10">Milano (2)</tei:addrLine>
          <tei:addrLine rend="indent-10">Stenopost 67</tei:addrLine>
        </tei:address>
      </tei:closer>
      <tei:lb/>

      <tei:postscript>
        <tei:p>Brief folgt.<tei:note xml:id="r0001-fn02" type="editorial" place="foot">= OJ 10/1,
            [94]]. </tei:note></tei:p>
      </tei:postscript>


    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:salute rend="indent-10">Dear and revered <tei:rs type="person" key="entity-000632">Master</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/>
        </tei:salute>
      </tei:opener>

      <tei:p rend="first">On your birthday we send you our most cordial greetings and good wishes! </tei:p>


      <tei:closer>
        <tei:signed rend="indent-30"><tei:rs type="person" key="entity-000149">Walter </tei:rs>and
            <tei:rs key="entity-002817" type="person">Margarete</tei:rs> Dahms</tei:signed>
        <tei:dateline><tei:date when="1926-06-17">June 17, 1926</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-10">Milan (2)</tei:addrLine>
          <tei:addrLine rend="indent-10">Stenopost 67</tei:addrLine>
        </tei:address>
      </tei:closer>
      <tei:lb/>

      <tei:postscript>
        <tei:p>Letter to follow.<tei:ptr corresp="#r0001-fn02" type="footnote"/></tei:p>
      </tei:postscript>


    </tei:div>
