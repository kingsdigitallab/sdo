   <tei:note type="editorial">lower middle panel:</tei:note>

   <tei:ab type="formpart" rend="printed">Aus <tei:seg type="entry" rend="ms">Wien 64</tei:seg> Nr.
     <tei:seg type="entry" rend="ms">1154</tei:seg> Worte <tei:seg type="entry" rend="ms"
