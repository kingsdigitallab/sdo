  <sdo:record ID="r0001">
    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-008072">d'AlbertE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003332">calling card</tei:rs>


      <dc:title>Handwritten calling card from Eugen d’Albert to Schenker, dated April 19,
        1914</dc:title>
      <dcterms:created>1914-04-19T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2021-12-11</dcterms:dateSubmitted>
      <dc:description>Eugen d'Albert thanks Schenker for his [50th birthday]
        congratulations.</dc:description>
      <dc:subject>Glückwünsche; congratulations</dc:subject>

      <marcrel:correspondent key="entity-000150">d'AlbertE</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>

    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:salute><tei:handShift new="#d-A"/>Lieber Herr <tei:rs type="person" key="entity-000632"
            >Doktor</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p>Vielen Dank für Ihre freundliche Glückwünsche!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">FFFF</tei:note></tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">Mit besten Grüßen</tei:salute>
        <tei:salute rend="indent-60">Ihr</tei:salute>
        <tei:signed rend="indent-50"><tei:rs type="person" key="entity-000150">Eugen
            d’Albert</tei:rs>.</tei:signed><tei:lb/>
        <tei:dateline><tei:date when="1914-04-19">19. 4. 14</tei:date>.</tei:dateline>
      </tei:closer>

      <tei:milestone unit="folio" n="verso"/>
      <tei:note type="editorial" place="inline"><tei:hi rend="italic">blank</tei:hi></tei:note>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:salute><tei:handShift new="#d-A"/>Dear Dr. <tei:supplied><tei:rs type="person"
              key="entity-000632">Schenker</tei:rs></tei:supplied>,</tei:salute>
      </tei:opener>

      <tei:p>Many thanks for your kind congratulations.<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">With best greetings,</tei:salute>
        <tei:salute rend="indent-60">Your</tei:salute>
        <tei:signed rend="indent-50"><tei:rs type="person" key="entity-000150">Eugen
            d’Albert</tei:rs></tei:signed><tei:lb/>
        <tei:dateline><tei:date when="1914-04-19">April 19, 14</tei:date></tei:dateline>
      </tei:closer>

      <tei:milestone unit="folio" n="verso"/>
      <tei:note type="editorial" place="inline"><tei:hi rend="italic">blank</tei:hi></tei:note>

    </tei:div>

