  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002728">Furtwängler ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>

      <dc:title>Handwritten letter from Furtwängler to Schenker, undated</dc:title>

      <dcterms:created>1919-11-01T11:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2015-02-02</dcterms:dateSubmitted>
      <dc:description>Furtwängler asks if he can call on Schenker the coming
        Friday.</dc:description>
      <dc:subject>besuchen; to visit</dc:subject>



      <marcrel:correspondent key="entity-000268">Furtwängler</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>HOTEL <tei:note type="editorial"
                place="inline">monogram "HI"</tei:note> IMPERIAL</tei:addrLine>
            <tei:addrLine>WIEN</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#WF"/>Herrn Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-20"><tei:hi rend="underline">Wien III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-20">Keilgasse 8</tei:addrLine>
            <tei:lb/>
            <tei:note type="editorial" place="inline">no postmark</tei:note>
            <tei:lb/>
          </tei:address>
        </tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:salute><tei:handShift new="#WF"/>Lieber verehrter Herr <tei:rs type="person"
            key="entity-000632">Schenker</tei:rs>,<tei:note xml:id="r0001-fn01" type="editorial"
            place="foot">This letter is undated and the envelope lacks a postmark. The first dated
            letter from Furtwängler to Schenker from the <tei:rs key="entity-004026" type="place"
              >Hotel Imperial</tei:rs> is from 1929, so it is likely that this item dates from the
            late 1920s or early 1930s.</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Würden Sie mir hierher, <tei:rs key="entity-004026" type="place">Hotel
          Imperial</tei:rs>, in ein paar Worten sagen, ob es Ihnen paßt, wenn ich am
            <tei:date><tei:hi rend="underline">nächsten Freitag</tei:hi></tei:date>abend zu Ihnen
        komme? Ich muß morgen nach <tei:rs key="entity-004409" type="place">Budapest</tei:rs>
        fahren, hoffe dann am <tei:date>Donnerstag</tei:date> bei meiner Rückkehr ein paar Worte von
        Ihnen vorzufinden, ob es geht.</tei:p>

      <tei:p rend="first">Mit den besten Grüßen, auch an Ihre <tei:rs key="entity-000771"
          type="person">Frau Gemahlin</tei:rs>, immer</tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">Ihr</tei:salute>
        <tei:signed rend="indent-40"><tei:rs key="entity-000268" type="person">Wilhelm
            Furtwängler</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>HOTEL <tei:note type="editorial"
                place="inline">monogram "HI"</tei:note> IMPERIAL</tei:addrLine>
            <tei:addrLine>VIENNA</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#WF"/>Mr. Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-20"><tei:hi rend="underline">Vienna
              III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-20">Keilgasse 8</tei:addrLine>
            <tei:lb/>
            <tei:note type="editorial" place="inline">no postmark</tei:note>
            <tei:lb/>
          </tei:address>
        </tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:salute><tei:handShift new="#WF"/>Dear revered Mr. <tei:rs type="person"
            key="entity-000632">Schenker</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"
          /></tei:salute>
      </tei:opener>

      <tei:p rend="first">Would you send a few words to me here, at the <tei:rs key="entity-004026"
          type="place">Hotel Imperial</tei:rs>, telling me whether it suits you for me to come to
        you <tei:date><tei:hi rend="underline">next Friday</tei:hi></tei:date> evening? I have to
        travel to <tei:rs key="entity-004409" type="place">Budapest</tei:rs> tomorrow, and hope to
        find word from you on <tei:date>Thursday</tei:date> on my return whether it is on.</tei:p>

      <tei:p rend="first">With my best greetings to you and your <tei:rs key="entity-000771"
          type="person">wife</tei:rs>, ever</tei:p>

      <tei:closer>
        <tei:salute rend="indent-30">Yours,</tei:salute>
        <tei:signed rend="indent-40"><tei:rs key="entity-000268" type="person">Wilhelm
            Furtwängler</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>
  </sdo:record>
