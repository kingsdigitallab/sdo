 <sdo:record ID="r0001">

  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-002775">Cube ~ HSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Postcard</tei:rs>

   <dc:title>Handwritten postcard from Schenker to Cube, dated September 7, 1927</dc:title>

   <dcterms:created>1927-09-07T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2006-06-20</dcterms:dateSubmitted>
   <dc:description>Schenker acknowledges Cube's postcards; Cube will receive the appeal [for
    photostats of MSS].</dc:description>
   <dc:subject/>

   <marcrel:correspondent key="entity-000632">Heinrich Schenker</marcrel:correspondent>
   <marcrel:recipient key="entity-000143">Felix-Eberhard von Cube</marcrel:recipient>
  </sdo:itemDesc>

  <tei:div type="transcription">

