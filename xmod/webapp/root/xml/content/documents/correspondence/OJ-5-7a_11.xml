      <tei:addrLine><tei:hi rend="lateinschr">Felix von Cube</tei:hi></tei:addrLine>
      <tei:addrLine><tei:handShift new="#printed"/>in <tei:handShift new="#HS"/>Duisburg
       (Rhld)</tei:addrLine>
      <tei:addrLine>Mühlheimerstr. 141/II</tei:addrLine>
      <tei:addrLine>Deutschland</tei:addrLine>
      <tei:addrLine><tei:note type="editorial">in another hand:</tei:note>
       <tei:handShift new="#unk"/>Nahe Be<tei:supplied reason="clarity"
       >...</tei:supplied></tei:addrLine>
     </tei:address>
     <tei:fw type="postmark"><tei:note type="editorial">postmark</tei:note> || 3/3 WIEN 40 |
