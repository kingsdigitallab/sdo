  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002700">Hertzka ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">postcard</tei:rs>



      <dc:title>Typewritten postcard from Hertzka (UE) to Schenker, dated October 28,
        1910</dc:title>

      <dcterms:created>1910-10-28T14:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2020-07-15</dcterms:dateSubmitted>
      <dc:description>UE is sending out complimentary copies to Schenker's recipient list, and two
        copies to himself. It asks Schenker to call in at the office.</dc:description>
      <dc:subject>Versendung; distribution; Exemplare; copies; Besuch; visit</dc:subject>

      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:correspondent key="entity-000356">Emil Hertzka</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:supplied reason="format">all in decorative rule-frame</tei:supplied><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>An <tei:handShift new="#TPue"/>Wohlgeboren
            Herrn Prof. Dr. Heinr. Schenker</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:handShift new="#printed"/>in <tei:handShift
              new="#TPue"/><tei:hi rend="sperr underline">Wien.</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-40">III Reisnerstr. 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 1/1 WIEN P | 28.X.10 | <tei:gap reason="illegible"/> ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPue"/>Wohlgeboren Herrn Prof. Dr. Heinrich
            Schenker,</tei:addrLine>
          <tei:addrLine><tei:hi rend="sperr underline">Wien.</tei:hi></tei:addrLine>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
          >Professor</tei:rs>!</tei:salute>
      </tei:opener>


      <tei:p rend="first">Wir teilen Ihnen höfl. mit, daß wir die Versendung<tei:note
          xml:id="r0001-fn01" type="editorial" place="foot"> Of complimentary copies of the <tei:rs
            type="work" key="entity-001730"><tei:title level="m">Chromatic Fantasy &amp;
              Fugue</tei:title></tei:rs> to the fourteen recipients listed by Schenker in <tei:ref
            type="correspondence" cRef="WSLB-66-67">WSLB 66/67</tei:ref>, October 19,
          1910.</tei:note> an die von Ihnen bezeichneten Adressen vorgenommen haben und erlauben uns
        Ihnen gleichzeitig die für Sie gewünschten 2 Exemplare mit gleicher Post zu überreichen und
        zeichnen</tei:p>

      <tei:closer>
        <tei:salute rend="indent 90"><tei:handShift new="#EH"/>grüsst</tei:salute>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-002408" type="organization"
            >„Universal-Edition“ A-G.</tei:rs><tei:lb/>(Jos. Aibl Verlag, G.m.b.H.)</tei:signed>
        <tei:signed rend="indent-90"><tei:handShift new="#EH"/><tei:unclear reason="reading"
            >Hertka</tei:unclear></tei:signed><tei:lb/>
        <tei:dateline><tei:handShift new="#printed"/>Wien, am <tei:date when="1910-10-28"
              ><tei:handShift new="#TPue"/>28. Oktober <tei:handShift new="#printed"
              />191<tei:handShift new="#TPue"/>0</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>I, Wipplingerstraße 32.</tei:addrLine>
        </tei:address>
      </tei:closer>

      <tei:postscript>
        <tei:lb/>
        <tei:p><tei:handShift new="#UE"/>Darf ich bald auf Ihren gesch. Besuch rechnen (vom
            <tei:date when="1910-10-30">30/10</tei:date> – <tei:date when="1910-11-09"
            >9/11</tei:date>
          <tei:hi rend="double-underline">nicht</tei:hi>)</tei:p>
      </tei:postscript>


    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:supplied reason="format">all in decorative rule-frame</tei:supplied><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>To <tei:handShift new="#TPue"/>Prof. Heinr.
            Schenker, Esq.</tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:handShift new="#printed"/>in <tei:handShift
              new="#TPue"/><tei:hi rend="sperr underline">Vienna III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-40">Reisnerstraße 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 1/1 VIENNA P | 28.X.10 | <tei:gap reason="illegible"/>
          ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPue"/>Prof. Heinrich Schenker, Esq.</tei:addrLine>
          <tei:addrLine><tei:hi rend="sperr underline">Vienna</tei:hi></tei:addrLine>
        </tei:address>
        <tei:salute>Dear <tei:rs type="person" key="entity-000632">Professor</tei:rs>,</tei:salute>
      </tei:opener>


      <tei:p rend="first">We would respectfully inform you that we are in process of making the
          distribution<tei:ptr corresp="#r0001-fn01" type="footnote"/> to the addresses indicated by
        you, and take the liberty at the same time of sending off to you the two copies requested by
        you on the same post. We remain,</tei:p>

      <tei:closer>
        <tei:salute rend="indent 90"><tei:handShift new="#EH"/>greetings</tei:salute>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>With kind regards,</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-002408" type="organization">Universal
            Edition &amp; Co.</tei:rs><tei:lb/>(Jos. Aibl Publisher &amp; Co.)</tei:signed>
        <tei:signed rend="indent-90"><tei:handShift new="#EH"/><tei:unclear reason="reading"
            >Hertka</tei:unclear></tei:signed><tei:lb/>
        <tei:dateline><tei:handShift new="#printed"/>Vienna, <tei:date when="1910-10-28"
              ><tei:handShift new="#TPue"/>October 28, <tei:handShift new="#printed"
              />191<tei:handShift new="#TPue"/>0</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>I, Wipplingerstraße 32</tei:addrLine>
        </tei:address>
      </tei:closer>

      <tei:postscript>
        <tei:lb/>
        <tei:p><tei:handShift new="#EH"/>May I count on a visit from you soon (<tei:hi
            rend="double-underline">not</tei:hi>
          <tei:date when="1910-10-30">October 30</tei:date> to <tei:date when="1910-11-09">November
            9</tei:date>)?</tei:p>
      </tei:postscript>


    </tei:div>
