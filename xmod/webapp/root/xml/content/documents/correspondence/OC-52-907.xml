 <sdo:record ID="r0001">
  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-003184">Winter ~
    HSchenker</tei:rs>
   <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~ H+JSchenker</tei:rs>
   <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~ HSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>




   <dc:title>Typed letter from Hugo Winter (UE) to Schenker, dated July 14, 1926</dc:title>

   <dcterms:created>1926-07-14T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2012-03-05</dcterms:dateSubmitted>
   <dc:description>UE have mailed Schenker's statement of account to Galtür. </dc:description>
   <dc:subject>Abrechnung; statement of account; Betrag; sum </dc:subject>

   
   
   
  <marcrel:correspondent key="entity-000968">Winter</marcrel:correspondent><marcrel:correspondent key="entity-002408">UE</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient><marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient></sdo:itemDesc>


  <tei:div type="transcription">

   <tei:opener>
    <tei:address>
     <tei:note type="editorial">printed letterhead: </tei:note>
     <tei:lb/>
     <tei:addrLine><tei:handShift new="#printed"/>UNIVERSAL-EDITION, A.-G. </tei:addrLine>
     <tei:addrLine>WIEN</tei:addrLine>
     <tei:addrLine>I. Karlsplatz 6 </tei:addrLine>
     <tei:addrLine>Musikvereinsgebäude</tei:addrLine>
    </tei:address>
    <tei:dateline rend="indent-60"><tei:placeName>Wien</tei:placeName>, <tei:date when="1926-07-14"><tei:handShift new="#TPhb"/>14. Juli 1926</tei:date>.</tei:dateline>
    <tei:byline>Dir. W/Hb</tei:byline>
    <tei:address>
     <tei:addrLine rend="indent-20">Wohlgeb. Herrn Prof. Dr. Heinrich <tei:hi rend="sperr">Schenker</tei:hi>,</tei:addrLine>
     <tei:addrLine rend="indent-60">
      <tei:hi rend="sperr-underline">Galtür</tei:hi>
     </tei:addrLine>
     <tei:addrLine rend="indent-60">Paznauntal (Tirol)</tei:addrLine>
    </tei:address>
    <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632">Professor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
      this letter is recorded in Schenker's diary at OJ 3/8, p. 2956, July 16, 1926: "Von U.-E.
      (Br.): Abrechnung: 232.14 Schillinge." ("From UE (letter): account: 232.14 shillings.").
     </tei:note></tei:salute>
   </tei:opener>

   <tei:p rend="first">Ihre Postkarte vom 12 ds.<tei:note xml:id="r0001-fn02" type="editorial" place="foot">= WSLB 370. </tei:note> haben wir soeben erhalten und teilen Ihnen mit, dass wir
    Ihre Abrechnung bereits gestern, nachdem wir uns vorher bei Herrn <tei:rs key="entity-000048" type="person">Dr. Baumgarten</tei:rs> um Ihre Adresse erkundigten, an Sie nach <tei:rs key="entity-002292" type="place">Galtür</tei:rs> absandten und Ihnen auch den sich zu Ihren
    Gunsten ergebenden Betrag per <tei:cit>
     <tei:q><tei:hi rend="underline">S. 232.14</tei:hi></tei:q>
    </tei:cit> dorthin per Postsparskassa überwiesen haben. </tei:p>

   <tei:closer>
    <tei:salute rend="indent-40">Wir empfehlen uns </tei:salute>
    <tei:salute rend="indent-60">mit vorzüglicher Hochachtung </tei:salute>
    <tei:signed rend="indent-60"><tei:handShift new="#stamp"/>„<tei:rs key="entity-002408" type="organization">Universal-Edition Actiengesellschaft</tei:rs>“</tei:signed>
    <tei:signed rend="indent-80"><tei:handShift new="#unk"/>pp. <tei:rs key="entity-000968" type="person">Winter</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>



  <tei:div type="translation">

   <tei:opener>
    <tei:address>
     <tei:note type="editorial">printed letterhead: </tei:note>
     <tei:lb/>
     <tei:addrLine><tei:handShift new="#printed"/>UNIVERSAL EDITION &amp; Co., </tei:addrLine>
     <tei:addrLine>VIENNA I,</tei:addrLine>
     <tei:addrLine>Karlsplatz 6 </tei:addrLine>
     <tei:addrLine>Musikverein Building</tei:addrLine>
    </tei:address>
    <tei:dateline rend="indent-60"><tei:placeName>Vienna</tei:placeName>, <tei:date when="1926-07-14"><tei:handShift new="#TPhb"/>July 14, 1926</tei:date></tei:dateline>
    <tei:byline>Dir. W/Hb</tei:byline>
    <tei:address>
     <tei:addrLine rend="indent-20">Prof. Heinrich <tei:hi rend="sperr">Schenker</tei:hi>, Esq., </tei:addrLine>
     <tei:addrLine rend="indent-60">
      <tei:hi rend="sperr-underline">Galtür,</tei:hi>
     </tei:addrLine>
     <tei:addrLine rend="indent-60">Paznauntal (Tirol)</tei:addrLine>
    </tei:address>
    <tei:salute>Dear <tei:rs type="person" key="entity-000632">Professor</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
   </tei:opener>

   <tei:p rend="first">We have just received your postcard of the 12th inst.,<tei:ptr corresp="#r0001-fn02" type="footnote"/> and can inform you that we already dispatched your
    statement of account to you in <tei:rs key="entity-002292" type="place">Galtür</tei:rs>
    yesterday, having previously inquired after your address of <tei:rs key="entity-000048" type="person">Dr. Baumgarten</tei:rs>, and have also transferred the sum due to you of <tei:cit>
     <tei:q><tei:hi rend="underline">232.14 shillings</tei:hi></tei:q>
    </tei:cit> there by Postal Savings Bank. </tei:p>

   <tei:closer>
    <tei:salute rend="indent-40">With our compliments, </tei:salute>
    <tei:salute rend="indent-60">and best regards, </tei:salute>
    <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002408" type="organization">Universal Edition &amp; Co.</tei:rs></tei:signed>
    <tei:signed rend="indent-80"><tei:handShift new="#unk"/>pp. <tei:rs key="entity-000968" type="person">Winter</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>

 </sdo:record>
