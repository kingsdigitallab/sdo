   <dc:title>Receipt for payment from Universal Edition to Schenker, dated October 8,
    1902</dc:title>
   <dcterms:created>1902-10-08T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2016-02-05</dcterms:dateSubmitted>
   <dc:description>Receipt for payment of honorarium by Universal Edition</dc:description>
   <dc:subject>Rechnung; account; Bestätigung; acknowledgement</dc:subject>

   <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
   <marcrel:correspondent key="entity-002467">Oesterreichische Länderbank</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
