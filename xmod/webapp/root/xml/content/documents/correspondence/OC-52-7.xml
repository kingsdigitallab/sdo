  <tei:div type="transcription">

   <tei:ab rend="printed">No. ...-....</tei:ab>

   <tei:ab rend="printed">Die Kais. Kön. privilegirte <tei:rs key="entity-002467"
     type="organization">Oesterreichische Länderbank</tei:rs> erlegt</tei:ab>

   <tei:ab rend="printed">an <tei:hi rend="ms">Herrn Prof. Heinrich Schenker, Wien III Reisnerstr.
     38.</tei:hi></tei:ab>

   <tei:ab rend="printed">im Auftrage</tei:ab>

   <tei:ab rend="printed">für Rechnung <tei:hi rend="ms"><tei:rs key="entity-002408"
      type="organization">Universal Edition</tei:rs> Actien Ges, Wien</tei:hi><tei:lb/>
    <tei:hi rend="ms">K 200. —</tei:hi><tei:note xml:id="r0001-fn01" type="editorial" place="foot"
     >Honorarium for Schenker's <tei:rs key="entity-001721" type="work">Ein Beitrag zur
      Ornamentik</tei:rs>: see <tei:ref type="correspondence" cRef="OC-52-380">OC 52/380</tei:ref>,
     July 15 and <tei:ref type="correspondence" cRef="OC-52-381">OC 52/381</tei:ref>, October 6,
     1902.</tei:note></tei:ab>

   <tei:ab rend="printed">gegen <tei:del rend="overstrike">einfache</tei:del>|doppelte
    Bestätigung</tei:ab>

   <tei:closer>
    <tei:dateline>Wien am <tei:date when="1902-10-08"><tei:hi rend="ms">8/10.</tei:hi> 190<tei:hi
       rend="ms">2</tei:hi></tei:date></tei:dateline><tei:lb/>
    <tei:note type="editorial" place="inline">unsigned</tei:note>
   </tei:closer>

   <tei:postscript>
    <tei:lb/>
    <tei:ab rend="printed">Form Nr. 106/C. III 1902. – Brüder Mindl</tei:ab>
   </tei:postscript>

  </tei:div>

