 <sdo:record ID="r0001">

  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-005688">FrimmelT ~
    HSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002751">Picture postcard</tei:rs>

   <dc:title>Handwritten picture postcard from Frimmel to Schenker, postmarked [January?] 18,
    1903?</dc:title>

   <dcterms:created>1903-01-18T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2020-07-30</dcterms:dateSubmitted>
   <dc:description>Frimmel thanks Schenker for his postcard.</dc:description>
   <dc:subject/>

   <marcrel:correspondent key="entity-000259">Theodor von Frimmel</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:fw type="header" rend="center">
     <tei:handShift new="#printed"/><tei:hi rend="bold">CARTE POSTALE. ‒ UNION POSTALE UNIVERSELLE.
      ‒ DOPISNICE ‒ POSTKARTE. <tei:note type="editorial" place="inline">etc.</tei:note>
     </tei:hi></tei:fw>
    <tei:lb/>
    <tei:fw type="envelope">
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity">An</tei:supplied>
       <tei:handShift new="#TF"/>Hochwgb. Herrn D<tei:hi rend="supralinear">r.</tei:hi> Heinrich
       Schenker</tei:addrLine>
      <tei:addrLine rend="indent-10">Tonkünstler, Schriftsteller etc etc etc</tei:addrLine>
      <tei:addrLine rend="indent-60">Wien</tei:addrLine>
      <tei:addrLine rend="indent-40">III. Reisnerstr. 38.</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| PRAHA | 18 | <tei:gap reason="illegible"/> | 03 ||</tei:fw>
     <tei:fw type="postmark">|| WIEN | 30 | <tei:unclear>18</tei:unclear>
      <tei:gap reason="illegible"/>
      <tei:unclear>03</tei:unclear> | 8 <tei:gap reason="illegible"/> ||</tei:fw>
     <tei:lb/><tei:lb/>
    </tei:fw>
    <tei:milestone unit="folio" n="verso"/><tei:lb/>
    <tei:note type="editorial" place="inline">photograph of Hradčany, captioned: "Gruss aus
     Prag!"</tei:note>
   </tei:opener>

   <tei:p><tei:handShift new="#TF"/>und eine leider sehr verspätete Danksagung für Ihre freundliche
    Karte vom Anfg. Jan.</tei:p>

   <tei:closer>
    <tei:salute rend="indent-40">Herzl<tei:supplied reason="abbrevn">iche</tei:supplied>
      Gr<tei:supplied reason="abbrevn">üsse</tei:supplied></tei:salute>
    <tei:signed rend="indent-70"><tei:rs key="entity-000259" type="person"
     >Frimmel</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>


  <tei:div type="translation">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:fw type="header" rend="center">
     <tei:handShift new="#printed"/><tei:hi rend="bold">POSTCARD </tei:hi></tei:fw>
    <tei:lb/>
    <tei:fw type="envelope">
     <tei:address>
      <tei:addrLine><tei:supplied reason="clarity">To: </tei:supplied>
       <tei:handShift new="#TF"/>Dr. Heinrich Schenker, Esq.</tei:addrLine>
      <tei:addrLine rend="indent-10">Musician, Writer on Music, etc., etc., etc.</tei:addrLine>
      <tei:addrLine rend="indent-60">Vienna III</tei:addrLine>
      <tei:addrLine rend="indent-40">Reisnerstrasse 38</tei:addrLine>
     </tei:address><tei:lb/>
     <tei:fw type="postmark">|| PRAGUE | 18 | <tei:gap reason="illegible"/> | 03 ||</tei:fw>
     <tei:fw type="postmark">|| VIENNA | 30 | <tei:unclear>18</tei:unclear>
      <tei:gap reason="illegible"/>
      <tei:unclear>03</tei:unclear> | 8 <tei:gap reason="illegible"/> ||</tei:fw>
     <tei:lb/><tei:lb/>
    </tei:fw>
    <tei:milestone unit="folio" n="verso"/><tei:lb/>
    <tei:note type="editorial" place="inline">photograph of Hradčany, captioned: "Greetings from
     Prague!"</tei:note>
   </tei:opener>

   <tei:p><tei:handShift new="#TF"/>and an unfortunately much-delayed expression of gratitude for your
    kind postcard from the beginning of January</tei:p>

   <tei:closer>
    <tei:salute rend="indent-40">Cordial greetings,</tei:salute>
    <tei:signed rend="indent-70"><tei:rs key="entity-000259" type="person"
     >Frimmel</tei:rs></tei:signed>
   </tei:closer>

  </tei:div>

