      <dc:title>Receipt from Karl Piller, dated July 22, 1930</dc:title>
      <dcterms:created>1930-07-22T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-04-09</dcterms:dateSubmitted>

      <dc:description>Receipt for preliminary printing costs for the third Meisterwerk
        volume.</dc:description>
