  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-004414">DMV ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-004415">Demblin ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typed postcard from August Demblin (DMV) to Schenker, dated May 3, 1927</dc:title>
      <dcterms:created>1927-05-03T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-27</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag confirm that they have arranged for two copies of the page
        proofs for the second Meisterwerk volume to be sent to him, and the third to Otto Erich
        Deutsch.</dc:description>
      <dc:subject>Abzug; copy</dc:subject>

    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG A.-G.</tei:addrLine>
            <tei:addrLine>MÜNCHEN</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-40"><tei:handShift new="#TPn"/>Herrn Dr. H.
              Schenker</tei:addrLine>
            <tei:addrLine rend="indent-60"><tei:hi rend="underline">Wien III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-70">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MUENCHEN</tei:placeName> | <tei:note
              type="editorial">middle section illegible</tei:note> | 5–6 N. ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPn"/><tei:hi rend="underline">Herrn Dr. Heinrich
              Schenker, Wien III / Keilgasse 8</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:placeName>München</tei:placeName>, den <tei:date
              when="1927-05-03">3. Mai 1927</tei:date></tei:addrLine>
        </tei:address>
        <tei:lb/>
        
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
            >Doktor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
            this letter is recorded in Schenker's diary at OJ 3/9, p. 3063, May 4, 1927: "Vom Verlag
            (K.): werden auch an Deutsch ein Exemplar senden." ("From the publisher (postcard): they
            will also send Deutsch a copy.").</tei:note>
        </tei:salute>
      </tei:opener>

      <tei:p>Wir erhielten Ihre <tei:date when="1927-05-02">gestrige</tei:date> Karte<tei:note xml:id="r0001-fn02" type="editorial" place="foot">This postcard is not known to survive.</tei:note> und haben
        veranlasst, dass Ihnen von jetzt an 2 Abzüge zugehen und der 3. an <tei:rs
          key="entity-000164" type="person">Herrn Dr. Deutsch</tei:rs> geschickt wird.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">Mit vorzüglicher Hochachtung</tei:salute>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002627"
            type="organization">Drei Masken Verlag A.-G.</tei:rs></tei:signed>
        <tei:signed rend="indent-60">ppa. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
      </tei:closer>
      
      <tei:postscript>
        <tei:lb/><tei:lb/>
        <tei:p><tei:handShift new="#TPn"/>N.</tei:p>
      </tei:postscript>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG &amp; Co.</tei:addrLine>
            <tei:addrLine>MUNICH</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-40"><tei:handShift new="#TPn"/>Dr. H.
              Schenker</tei:addrLine>
            <tei:addrLine rend="indent-60"><tei:hi rend="underline">Vienna III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-70">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MUNICH</tei:placeName> | <tei:note
            type="editorial">middle section illegible</tei:note> | 5–6 N. ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPn"/><tei:hi rend="underline">Dr. Heinrich
            Schenker, Vienna III / Keilgasse 8</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-40"><tei:placeName>Munich</tei:placeName>, <tei:date
            when="1927-05-03">May 3, 1927</tei:date></tei:addrLine>
        </tei:address>
        <tei:lb/>
        
        <tei:salute>Dear Dr. <tei:rs type="person" key="entity-000632"
          ><tei:supplied reason="identity">Schenker</tei:supplied></tei:rs>!<tei:ptr corresp="#r0001-fn01" type="footnote"/>
        </tei:salute>
      </tei:opener>
      
      <tei:p rend="first">We received your postcard of <tei:date when="1927-05-02"
        >yesterday</tei:date>,<tei:ptr corresp="#r0001-fn02" type="footnote"/> and have arranged for two copies <tei:supplied reason="clarity">of
          the page proofs</tei:supplied> to go to you and the third to be sent to <tei:rs
            key="entity-000164" type="person">Dr. Deutsch</tei:rs>.</tei:p>
      
      <tei:closer>
        <tei:salute rend="indent-50">With highest regards,</tei:salute>
        <tei:signed rend="indent-60"><tei:handShift new="#stamp"/><tei:rs key="entity-002627"
          type="organization">Drei Masken Verlag &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-60">ppa. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
      </tei:closer>
      
      <tei:postscript>
        <tei:lb/><tei:lb/>
        <tei:p><tei:handShift new="#TPn"/>N.</tei:p>
      </tei:postscript>
      
    </tei:div>
