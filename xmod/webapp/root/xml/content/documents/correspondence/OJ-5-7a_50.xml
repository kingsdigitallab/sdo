 <sdo:record ID="r0001">

  <sdo:itemDesc>
   <tei:rs role="correspondence" type="correspondence" key="entity-002775">Cube ~ HSchenker</tei:rs>
   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002751">Picture postcard</tei:rs>

   <dc:title>Handwritten picture postcard from Schenker to Cube, dated August 18, 1934</dc:title>

   <dcterms:created>1934-08-18T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2006-10-10</dcterms:dateSubmitted>
   <dc:description>Best wishes for birth.</dc:description>
   <dc:subject/>

   
   
  <marcrel:correspondent key="entity-000632">HSchenker</marcrel:correspondent><marcrel:recipient key="entity-000143">Cube</marcrel:recipient></sdo:itemDesc>

  <tei:div type="transcription">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:note type="editorial" place="inline">landscape photograph, uncaptioned</tei:note>
    <tei:lb/><tei:milestone unit="folio" n="verso"/>
    <tei:address>
     <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied> H. Prof.</tei:addrLine>
     <tei:addrLine rend="indent-05"><tei:hi rend="lateinschr"><tei:rs key="entity-000143" type="person">Felix E. von Cube</tei:rs> u. <tei:rs key="entity-003059" type="person">Frau</tei:rs></tei:hi></tei:addrLine>
     <tei:addrLine rend="indent-25"><tei:hi rend="lateinschr">Hamburg</tei:hi></tei:addrLine>
     <tei:addrLine rend="indent-10">Mittelweg</tei:addrLine>
     <tei:addrLine rend="indent-15">126, Ia/<tei:hi rend="supralinear">II</tei:hi></tei:addrLine>
    </tei:address>
    <tei:fw type="postmark">|| BÖCKSTEIN | 18. VIII. 34 | * a * ||</tei:fw>
   </tei:opener>

   <tei:lb/>
   <tei:p>Zum<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Writing of this picture
     postcard is recorded in Schenker's diary at OJ 4/7, p. 3935, August 18, 1934: "An Cube
     (Ansichtsk.): Gratulation." ("To Cube (picture postcard): congratulations.").</tei:note>
    Freudigen Familienereignis<tei:note xml:id="r0001-fn02" type="editorial" place="foot">The birth
     of Felix-Eberhard's and <tei:rs key="entity-003059" type="person">Henriette</tei:rs>'s first
     son, <tei:rs key="entity-003090" type="person">John Carsten von Cube</tei:rs>. (See <tei:ref type="correspondence" cRef="OC-44-4">OC 44/4</tei:ref>, August 12, 1934.)</tei:note> unser
    Beider herzlichste Wünsche!</tei:p>

   <tei:closer>
    <tei:salute>die Ihren</tei:salute>
    <tei:signed rend="indent-05"><tei:rs key="entity-000632" type="person"><tei:hi rend="lateinschr">Heinrich</tei:hi></tei:rs> u <tei:rs key="entity-000771" type="person">LieLie</tei:rs>
     <tei:hi rend="lateinschr">Schenker</tei:hi></tei:signed>
    <tei:dateline><tei:rs key="entity-002359" type="place">Böckstein</tei:rs> (Badgastein) <tei:lb/>
     <tei:rs key="entity-002578" type="place">Salzbg</tei:rs>, <tei:lb/><tei:date when="1934-08-18">18.8.34</tei:date></tei:dateline>
   </tei:closer>

  </tei:div>

  <tei:div type="translation">

   <tei:opener>
    <tei:milestone unit="folio" n="recto"/><tei:lb/>
    <tei:note type="editorial" place="inline">landscape photograph, uncaptioned</tei:note>
    <tei:lb/><tei:milestone unit="folio" n="verso"/>
    <tei:address>
     <tei:addrLine><tei:supplied reason="clarity">To:</tei:supplied> Prof.</tei:addrLine>
     <tei:addrLine rend="indent-05"><tei:rs key="entity-000143" type="person">Felix E. von
       Cube</tei:rs> and <tei:rs key="entity-003059" type="person">wife</tei:rs>,</tei:addrLine>
     <tei:addrLine rend="indent-25">Hamburg,</tei:addrLine>
     <tei:addrLine rend="indent-10">Mittelweg,</tei:addrLine>
     <tei:addrLine rend="indent-15">126, Ia/<tei:hi rend="supralinear">II</tei:hi></tei:addrLine>
    </tei:address>
    <tei:fw type="postmark">|| BÖCKSTEIN | 18. VIII. 34 | * a * ||</tei:fw>
   </tei:opener>

   <tei:lb/>
   <tei:p>To<tei:ptr corresp="#r0001-fn01" type="footnote"/> the happy family event<tei:ptr corresp="#r0001-fn02" type="footnote"/> our most cordial wishes!</tei:p>

   <tei:closer>
    <tei:salute>Yours,</tei:salute>
    <tei:signed rend="indent-05"><tei:rs key="entity-000632" type="person">Heinrich</tei:rs> and
      <tei:rs key="entity-000771" type="person">LieLie</tei:rs> Schenker</tei:signed>
    <tei:dateline><tei:rs key="entity-002359" type="place">Böckstein</tei:rs> (Badgastein) <tei:lb/>
     <tei:rs key="entity-002578" type="place">Salzburg</tei:rs>, <tei:lb/><tei:date when="1934-08-18">August 18, 1934</tei:date></tei:dateline>
   </tei:closer>

  </tei:div>
