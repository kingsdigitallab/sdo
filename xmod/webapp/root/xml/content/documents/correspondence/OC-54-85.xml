      <dc:title>Telegram from Drei Masken Verlag to Schenker, dated June 16, 1926</dc:title>

      <dcterms:created>1926-06-16T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-16</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag in Munich acknowledge receipt of the manuscript of the
        second Meisterwerk Yearbook.</dc:description>
      <dc:subject>Manuskript; manuscript; Meisterwerk 2; Meisterwerk 2</dc:subject>


    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:p rend="centre"><tei:hi rend="bold">Telegramm</tei:hi></tei:p>

      <tei:p><tei:handShift new="#printed"/>Dienstliche Angaben: <tei:handShift new="#stamp"/> M 28
        | <tei:handShift new="#unk"/>16.VI | TE</tei:p>
      <tei:p><tei:handShift new="#printed"/>Gattung: <tei:handShift new="#unk"/>128<tei:lb/>
        <tei:handShift new="#printed"/>Eing.- Nr. <tei:handShift new="#unk"/>161/23 </tei:p>

      <tei:lb/>
      
      <tei:p rend="centre"><tei:handShift new="#TPunk"/>heinrich schenker<tei:lb/> keilgasse 8 wien
        3</tei:p>
      
       <tei:p><tei:handShift new="#printed"/>Aus <tei:handShift new="#TPunk"/> muenchen 43.+ |
          <tei:handShift new="#printed"/>Nr. <tei:handShift new="#TPunk"/>21643 | <tei:handShift
          new="#printed"/>Worte <tei:handShift new="#TPunk"/>9 | <tei:handShift new="#printed"
        />Aufgegeben am <tei:handShift new="#TPunk"/>16/6 | <tei:handShift new="#printed"/>Uhr
          <tei:handShift new="#TPunk"/>4 <tei:lb/></tei:p>
      
      <tei:p rend="indent-20"><tei:handShift new="#TPunk"/>= manuskript eingetroffen = dreimasken + + +</tei:p>
     
      <tei:p><tei:handShift new="stamp"/>AUSGEFERTIGT; <tei:date when="1926-06-16">16 JUN <tei:time>17 20</tei:time></tei:date></tei:p>

    </tei:div>



