      <dc:title>Handwritten calling card from Hoboken to Schenker, undated</dc:title>
      <dcterms:created>1925-09-30T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2018-09-04</dcterms:dateSubmitted>
      <dc:description>Hoboken gives his current address.</dc:description>
