  <sdo:record ID="r0001">
    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-007005">EliasA ~
        JSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003332">calling card</tei:rs>


      <dc:title>Handwritten calling card from Elias to Jeanette Schenker, undated [December c.28,
        1938]</dc:title>
      <dcterms:created>1938-12-28T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2019-02-06</dcterms:dateSubmitted>
      <dc:description>Miss Elias sends Jeanette Schenker New Year's greetings for
        1939.</dc:description>
      <dc:subject>Neujahrsgrüsse; New Year's greetings</dc:subject>

      <marcrel:correspondent key="entity-000201">Angi Elias</marcrel:correspondent>
      <marcrel:recipient key="entity-000771">JSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:milestone unit="folio" n="verso"/>

      <tei:p><tei:handShift new="#AE"/>Nun wünsche ich Ihnen auf diesem Wegen alles Schöne für das
        Jahr 1939!</tei:p>


      <tei:closer>
        <tei:milestone unit="folio" n="recto"/>
        <tei:salute>Herzlichste Grüße von</tei:salute>
        <tei:salute>Ihrer</tei:salute>
        <tei:signed><tei:handShift new="#printed"/><tei:rs type="person" key="entity-000201">Angi
            Elias</tei:rs></tei:signed>
      </tei:closer>


    </tei:div>



    <tei:div type="translation">

      <tei:milestone unit="folio" n="verso"/>

      <tei:p><tei:handShift new="#AE"/>Wishing you now by this means all the best for the year 1939!</tei:p>


      <tei:closer>
        <tei:milestone unit="folio" n="recto"/>
        <tei:salute>Most cordial greetings from</tei:salute>
        <tei:salute>Your</tei:salute>
        <tei:signed><tei:handShift new="#printed"/><tei:rs type="person" key="entity-000201">Angi
            Elias</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>

