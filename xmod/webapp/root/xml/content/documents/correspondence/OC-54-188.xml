  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs key="entity-004414" type="correspondence">DMV ~ HSchenker</tei:rs>
      <tei:rs key="entity-004415" type="correspondence">Demblin ~ HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typed postcard from August Demblin (DMV) to Schenker, dated September 30,
        1927</dc:title>
      <dcterms:created>1927-09-30T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-04-04</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag confirm that the complimentary hardbound copies of the
        second Meisterwerk volume are on their way to Schenker, and inform him that the first set of
        bound copies will be dispatched from the bindery in the next few days.</dc:description>
      <dc:subject>complimentary copy; Freiexemplar; paperback; broschierten; bound copy; Aufbindung;
        bindery; Binderei</dc:subject>

    <marcrel:correspondent key="entity-002627">DMV</marcrel:correspondent><marcrel:correspondent key="entity-004417">Demblin</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>



    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG A.-G.</tei:addrLine>
            <tei:addrLine>MÜNCHEN</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-40"><tei:handShift new="#TPn"/>Herrn Dr. H.
              Schenker</tei:addrLine>
            <tei:addrLine rend="indent-60"><tei:hi rend="underline">Wien III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-60">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MÜNCHEN</tei:placeName> | <tei:date when="1927-09-30">30 9 27</tei:date> | * 8-9 N * | 2 ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPn"/><tei:hi rend="underline">Herrn Dr. Heinrich
              Schenker, Wien III, Keilgasse 8</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:placeName>München</tei:placeName>, den <tei:date when="1927-09-30">30.9.27</tei:date></tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632">Doktor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
            this letter may have been recorded in Schenker’s diary: the page comprising the entries
            for September 28–30 do not, however, survive.</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Wir erhielten Ihre Karte von <tei:date when="1927-09-29">gestern</tei:date>,<tei:note xml:id="r0001-fn02" type="editorial" place="foot">This
          postcard is not known to survive.</tei:note> die sich mit der Sendung Ihrer gebundenen
        Freiexemplare kreuzte; die broschierten folgen nach. Die erste Aufbindung wird in diesen
        Tagen von der Binderei abgeliefert.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">Mit ausgezeichneter Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002627" type="organization">Drei Masken Verlag A.-G.</tei:rs></tei:signed>
        <tei:signed rend="indent-50">ppa. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:p>
          <tei:lb/><tei:lb/>
          <tei:handShift new="#TPn"/>N. </tei:p>
      </tei:postscript>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG &amp; Co.</tei:addrLine>
            <tei:addrLine>MUNICH</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-40"><tei:handShift new="#TPn"/>Dr. H. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-60"><tei:hi rend="underline">Vienna
              III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-60">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>Munich</tei:placeName> | <tei:date when="1927-09-30">30 9 27</tei:date> | * 8-9 N * | 2 ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPn"/><tei:hi rend="underline">Dr. Heinrich Schenker,
              Vienna III, Keilgasse 8</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:placeName>Munich</tei:placeName>, <tei:date when="1927-09-30">30.9.27</tei:date></tei:addrLine>
        </tei:address><tei:lb/>
        <tei:salute>Dear Dr <tei:rs type="person" key="entity-000632"><tei:supplied reason="identity">Schenker</tei:supplied></tei:rs>!<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">We received your card of <tei:date when="1927-09-29">yesterday</tei:date>,<tei:ptr corresp="#r0001-fn02" type="footnote"/> which crossed with
        the sending of your hardbound complimentary copies; the paperback ones will follow. The
        first set of bound copies will be dispatched from the bindery in the next few days.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">With kind regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002627" type="organization">Drei Masken Verlag &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-50">p.p. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:p>
          <tei:lb/><tei:lb/>
          <tei:handShift new="#TPn"/>N. </tei:p>
      </tei:postscript>

    </tei:div>
