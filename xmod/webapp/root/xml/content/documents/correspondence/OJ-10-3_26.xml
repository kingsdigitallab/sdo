  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002726">DeutschOE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002712">DeutschOE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002727">DeutschOE+H ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-005470">Seidel's ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typewritten postcard from Otto Erich Deutsch to Schenker, dated December 28,
        1921</dc:title>

      <dcterms:created>1921-12-28T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-07-02</dcterms:dateSubmitted>
      <dc:description>A payment of 1,200 marks from Seidel's Book Dealership, on behalf of Deutsch,
        has been made to Schenker's Deutsche Bank account.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002462">Seidel's</marcrel:correspondent>
      <marcrel:correspondent key="entity-000164">DeutschOE</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>


    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/>Korrespondenzk<tei:supplied reason="cutoff"
            >arte</tei:supplied></tei:fw>
        <tei:lb/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:note type="editorial" place="inline">printed panel:</tei:note>
            <tei:addrLine><tei:supplied reason="clarity">Absender:</tei:supplied> Seidelsche
              Buchhandlung</tei:addrLine>
            <tei:addrLine>Wien, I. Graben 13 (Korsoseite)</tei:addrLine>
            <tei:addrLine rend="indent-30">Gegründet 1848.</tei:addrLine>
            <tei:addrLine>Reiches Lager aus allen Literatur-Gebieten, besonders Schönliteratur,
              Liebhaberausgaben und Klassiker, Kunst, Architektur, Kunstgewerbe, Musik, Philosophie,
              Sprachwissenschaft, Geschichte, Geographie, Naturwissenschaften, Technologie,
              Handelswissenschaften, Landwirtschaft, Gartenbau, Jugendschriften, Bilderbücher,
              Lexika, Landkarten, Reiseführer u. s. w. <tei:lb/> Verzeichnisse stehen zu
              Diensten</tei:addrLine>
            <tei:addrLine>Postscheckkonten: Wien 22413<tei:supplied reason="clarity"
                >,</tei:supplied> Budapest 33154<tei:supplied reason="clarity">,</tei:supplied> Prag
                59203<tei:supplied reason="clarity">,</tei:supplied> Karlsruhe 24934<tei:supplied
                reason="clarity">,</tei:supplied> Laibach 20171<tei:supplied reason="clarity"
                >.</tei:supplied></tei:addrLine>
            <tei:addrLine>Drahtanschrift: Grabenbuch Wien</tei:addrLine>
            <tei:addrLine>Fernsprecher: 1288</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-15"><tei:supplied reason="clarity">An:</tei:supplied>
              Hochwohlgeboren</tei:addrLine>
            <tei:addrLine>Herrn/Löbl.</tei:addrLine>
            <tei:addrLine><tei:handShift new="#TPoedrk"/>Dr. H. Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-15"><tei:hi rend="underline"
                  ><tei:hi><!-- spaced out -->WIEN</tei:hi> III.</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse No. 8</tei:addrLine>
          </tei:address>
          <tei:lb/><tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="few"
            /> | <tei:gap reason="illegible" unit="chars" extent="few"/> | * 3e * ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-40"><tei:rs key="entity-002484" type="place">Wien</tei:rs>,
            <tei:date when="1921-12-28">28. XII. 1921</tei:date> R/K</tei:dateline>
        <tei:address>
          <tei:addrLine>Herrn</tei:addrLine>
          <tei:addrLine rend="indent-15">Dr. <tei:rs key="entity-000632" type="person">H.
              Schenker</tei:rs>,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
              of this correspondence card is recorded in Schenker's diary at OJ 3/3, p. 2408
              (December 29, 1921): "Von Deutsch (K.): habe mir 1200 M. angewiesen." ("From Deutsch
              (postcard): has authorized payment to me of 1,200 Marks.")</tei:note>
            <tei:rs key="entity-002484" type="place"
              ><tei:hi><!-- spaced out -->WIEN</tei:hi></tei:rs> III.</tei:addrLine>
        </tei:address>
        <tei:lb/><tei:note type="editorial" place="inline">horizontal line</tei:note>
      </tei:opener>

      <tei:p rend="first">Im Auftrage und für Rechnung unseres Herrn <tei:rs key="entity-000164"
          type="person">Otto Erich Deutsch</tei:rs> überwiesen wir heute durch das <tei:rs
          key="entity-005472" type="organization">Oesterr. Credit Institut f. öffentl.
          Unternehmungen und Arbeiten</tei:rs>, hier auf Ihr Konto bei der <tei:rs
          key="entity-002848" type="organization">Deutschen Bank</tei:rs> in <tei:rs
          key="entity-002501" type="place">Berlin</tei:rs>
        <tei:lb/><tei:hi rend="underline">Mark 1200.--/Zwölfhundert Mark/</tei:hi></tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">Hochachtungsvoll</tei:salute>
        <tei:salute rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002462"
            type="organization">SEIDELSCHE SORTIMENTS-BUCHHANDLUNG</tei:rs></tei:salute>
        <tei:signed rend="indent-90"><tei:rs key="entity-000164" type="person"
          >D.</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:p><tei:note type="editorial" place="inline">P.S.</tei:note>
          <tei:handShift new="#OED"/>Brief folgt. <tei:lb/>Ihr <tei:lb/><tei:note type="editorial"
            place="inline">signed:</tei:note>
          <tei:rs key="entity-000164" type="person">Deutsch</tei:rs></tei:p>
      </tei:postscript>

    </tei:div>


    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/>Correspondence Card</tei:fw>
        <tei:lb/><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:note type="editorial" place="inline">printed panel:</tei:note>
            <tei:addrLine><tei:supplied reason="clarity">sender:</tei:supplied> Seidel's Book
              Dealership</tei:addrLine>
            <tei:addrLine>Vienna I, Graben 13 (parade side)</tei:addrLine>
            <tei:addrLine rend="indent-30">Founded 1848.</tei:addrLine>
            <tei:addrLine>Rich stock in all literature areas, especially fine literature,
              booklover's editions and classics, art, architecture, arts and crafts, music,
              philosophy, linguistics, history, geography, science, technology, business studies,
              agriculture, horticulture, children's writings, picture books, dictionaries, maps,
              travel guides, etc. <tei:lb/> Listings are at your disposal.</tei:addrLine>
            <tei:addrLine>Post office accounts: Vienna 22413<tei:supplied reason="clarity"
                >,</tei:supplied> Budapest 33154<tei:supplied reason="clarity">,</tei:supplied>
              Prague 59203<tei:supplied reason="clarity">,</tei:supplied> Karlsruhe
                24934<tei:supplied reason="clarity">,</tei:supplied> Ljubljana 20171<tei:supplied
                reason="clarity">.</tei:supplied></tei:addrLine>
            <tei:addrLine>Telegraph address: Grabenbuch Vienna</tei:addrLine>
            <tei:addrLine>Telephone: 1288</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-15"><tei:supplied reason="clarity">to:</tei:supplied>
              High-born</tei:addrLine>
            <tei:addrLine>Mister/Laudable</tei:addrLine>
            <tei:addrLine><tei:handShift new="#TPoedrk"/>Dr. H. Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-15"><tei:hi rend="underline"
                  ><tei:hi><!-- spaced out -->VIENNA</tei:hi> III.</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse No. 8</tei:addrLine>
          </tei:address>
          <tei:lb/><tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="few"
            /> | <tei:gap reason="illegible" unit="chars" extent="few"/> | * 3e * ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-40"><tei:rs key="entity-002484" type="place">Vienna</tei:rs>,
            <tei:date when="1921-12-28">December 28, 1921</tei:date> R/K</tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-15">Dr. <tei:rs key="entity-000632" type="person">H.
              Schenker</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/>
            <tei:rs key="entity-002484" type="place"
              ><tei:hi><!-- spaced out -->VIENNA</tei:hi></tei:rs> III.</tei:addrLine>
        </tei:address>
        <tei:lb/><tei:note type="editorial" place="inline">horizontal line</tei:note>
      </tei:opener>

      <tei:p rend="first">On behalf and for the account of our Mr. <tei:rs key="entity-000164"
          type="person">Otto Erich Deutsch</tei:rs>, we have today, through the <tei:rs
          key="entity-005472" type="organization">Austrian Credit Institute for Public Enterprises
          and Works</tei:rs>, paid into your account at <tei:rs key="entity-002848"
          type="organization">Deutsche Bank</tei:rs> in <tei:rs key="entity-002501" type="place"
          >Berlin</tei:rs>
        <tei:lb/><tei:hi rend="underline">Mark 1200.--/twelve hundred marks/</tei:hi></tei:p>

      <tei:closer>
        <tei:salute rend="indent-50">Respectfully,</tei:salute>
        <tei:salute rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002462"
            type="organization">SEIDEL'S RETAIL BOOK DEALERSHIP</tei:rs></tei:salute>
        <tei:signed rend="indent-90"><tei:rs key="entity-000164" type="person"
          >D.</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:p><tei:note type="editorial" place="inline">P.S.</tei:note>
          <tei:handShift new="#OED"/>Letter will follow. <tei:lb/>Yours, <tei:lb/><tei:note
            type="editorial" place="inline">signed:</tei:note>
          <tei:rs key="entity-000164" type="person">Deutsch</tei:rs></tei:p>
      </tei:postscript>

    </tei:div>

  </sdo:record>
