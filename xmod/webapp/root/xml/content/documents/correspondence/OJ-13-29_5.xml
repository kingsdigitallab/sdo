  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-005548">RosenthalM ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-003332">calling card</tei:rs>

      <dc:title>Calling card from Rosenthal with handwritten note to Schenker, dated November 18,
        1902</dc:title>

      <dcterms:created>1902-11-18T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-12-19</dcterms:dateSubmitted>
      <dc:description>Rosenthal has reserved a ticket for Schenker. — He recommends Frl. Fritzi
        Neumann.</dc:description>
      <dc:subject>Billet; ticket; Harmonielehre; theory of harmony</dc:subject>



      <marcrel:correspondent key="entity-000732">Moriz Rosenthal</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">Heinrich Schenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:div type="doc1">
        <tei:opener>
          <tei:milestone unit="folio" n="recto"/>
          <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Moriz
            Rosenthal</tei:fw><tei:lb/>
          <tei:salute><tei:handShift new="#MR"/>Verehrter <tei:rs type="person" key="entity-000632"
              >Freund</tei:rs>!</tei:salute>
        </tei:opener>

        <tei:p>Das <tei:foreign xml:lang="fre"><tei:hi rend="lateinschr"
            >Concert</tei:hi></tei:foreign> ist ausverkauft.<tei:note xml:id="r0001-fn01"
            type="editorial" place="foot">The concert is not noted in Schenker's diary, and no
            review clipping appears in Schenker's scrapbook.</tei:note> Für <tei:hi rend="underline"
            >Sie</tei:hi> habe ich <tei:foreign xml:lang="lat"><tei:hi rend="lateinschr">ab
              initio</tei:hi></tei:foreign> ein <tei:hi rend="lateinschr"><tei:foreign
              xml:lang="fre">Billet</tei:foreign> reservirt</tei:hi>, welches nun mit herzlichem
          Gruße an sie geht.</tei:p>

        <tei:closer>
          <tei:salute rend="indent-20">Hochschätzend,</tei:salute>
          <tei:salute rend="indent-30">Ihr getreuer</tei:salute><tei:lb/>
          <tei:note type="editorial" place="inline">unsigned</tei:note><tei:lb/><tei:lb/>
        </tei:closer>
      </tei:div>
      <tei:div type="doc2">
        <tei:milestone unit="folio" n="verso"/>
        <tei:opener>
          <tei:salute>Sehr geehrter <tei:rs key="entity-000632" type="person">Herr u.
              Freund</tei:rs>!</tei:salute>
        </tei:opener>

        <tei:p>Ich empfehle Ihnen wärmstens Fräulein <tei:rs key="entity-005549" type="person"
              ><tei:hi rend="lateinschr">Fritzi Neumann</tei:hi></tei:rs>, die gerne
              <tei:term><tei:hi rend="lateinschr">Harmonie</tei:hi>lehre</tei:term> bei Ihnen
          studiren möchte.</tei:p>

        <tei:closer>
          <tei:salute rend="indent-10">Mit freundlichstem Gruße,</tei:salute>
          <tei:salute rend="indent-10">Ihr Sie hochschätzender,</tei:salute>
          <tei:signed rend="indent-50"><tei:rs key="entity-000732" type="person">Moriz
              Rosenth.</tei:rs></tei:signed><tei:lb/>
          <tei:dateline><tei:placeName>Wien</tei:placeName>, <tei:date when="1902-11-18">18. Nov.
              1902</tei:date></tei:dateline>
        </tei:closer>
      </tei:div>
    </tei:div>


    <tei:div type="translation">
      <tei:div type="doc1">
        <tei:opener>
          <tei:milestone unit="folio" n="recto"/>
          <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Moriz
            Rosenthal</tei:fw><tei:lb/>
          <tei:salute><tei:handShift new="#MR"/>Revered <tei:rs type="person" key="entity-000632"
              >Friend</tei:rs>,</tei:salute>
        </tei:opener>

        <tei:p>The concert is sold out.<tei:ptr corresp="#r0001-fn01" type="footnote"/> I reserved a
          ticket for <tei:hi rend="underline">you</tei:hi> at the outset, and this is now on its way
          to you with cordial greetings. </tei:p>

        <tei:closer>
          <tei:salute rend="indent-20">With high regard,</tei:salute>
          <tei:salute rend="indent-30">Yours truly,</tei:salute><tei:lb/>
          <tei:note type="editorial" place="inline">unsigned</tei:note><tei:lb/><tei:lb/>
        </tei:closer>
      </tei:div>
      <tei:div type="doc2">
        <tei:milestone unit="folio" n="verso"/>
        <tei:opener>
          <tei:salute>Dear <tei:rs key="entity-000632" type="person">Sir and
            Friend</tei:rs>,</tei:salute>
        </tei:opener>

        <tei:p>I recommend to you most warmly Miss <tei:rs key="entity-005549" type="person">Fritzi
            Neumann</tei:rs>, who would very much like to study <tei:term>theory of
            harmony</tei:term> with you.</tei:p>

        <tei:closer>
          <tei:salute rend="indent-10">With most friendly greetings, </tei:salute>
          <tei:salute rend="indent-10">Yours with greatest regard,</tei:salute>
          <tei:signed rend="indent-50"><tei:rs key="entity-000732" type="person">Moriz
              Rosenthal</tei:rs></tei:signed><tei:lb/>
          <tei:dateline><tei:placeName>Vienna</tei:placeName>, <tei:date when="1902-11-18">November
              18, 1902</tei:date></tei:dateline>
        </tei:closer>
      </tei:div>
    </tei:div>
