  <sdo:record ID="r0001">

    <sdo:itemDesc>

      <tei:rs role="correspondence" type="correspondence" key="entity-004414">DMV ~
        HSchenker</tei:rs>
      <tei:rs key="entity-002712" type="correspondence" role="correspondence">HSchenker ~
