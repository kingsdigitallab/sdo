  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-005586"> HeckVA ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002747">Delivery note</tei:rs>

      <dc:title>Handwritten delivery note from V. A. Heck to Schenker, dated March 10,
        1927</dc:title>
      <dcterms:created>1927-03-10T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2015-03-09</dcterms:dateSubmitted>
      <dc:description>Delivery note for Beethoven sketchleaves.</dc:description>
      <dc:subject>Skizzenblatt; sketchleaf</dc:subject>

      <marcrel:correspondent key="entity-000345">V. A. Heck</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>BUCH- UND KUNST-ANTIQUARIAT</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">V. A. HECK</tei:hi></tei:addrLine>
          <tei:addrLine>WIEN, I. BEZIRK, KÄRNTNERRING Nr. 12</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">Telephon 51-2-61</tei:hi></tei:addrLine>
          <tei:addrLine>____________________</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:placeName>WIEN</tei:placeName>, am <tei:handShift
            new="#unkVAH"/><tei:date when="1927-03-10">10. III. 27</tei:date>.</tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine>Prof. Dr. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-80">Wien<tei:note xml:id="r0001-fn01" type="editorial"
              place="foot">This delivery note, and receipt of the accompanying materials, are
              unrecorded in Schenker’s diary.</tei:note></tei:addrLine>
        </tei:address><tei:lb/><tei:lb/>
      </tei:opener>

      <tei:p><tei:handShift new="#printed"/>Belieben zu empfangen:</tei:p>

      <tei:p rend="first">Beethoven<tei:lb/>Skizzenbl.<tei:lb/>4SS,<tei:note xml:id="r0001-fn02"
          type="editorial" place="foot">The manuscript in question was probably Bonn, Beethovenhaus,
          HCB Mh 93.</tei:note></tei:p>
      <tei:lb/>


      <tei:closer>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-60"><tei:rs key="entity-000345" type="person"><tei:hi rend="bold"
                ><tei:hi rend="sperr">V. A.
        HECK.</tei:hi></tei:hi></tei:rs></tei:signed><tei:lb/><tei:lb/>
      </tei:closer>

      <tei:postscript>
        <tei:milestone unit="folio" n="verso"/>
        <tei:lb/>
        <tei:p><tei:handShift new="#unkVAH"/><tei:unclear reason="reading"
          >Übernommen</tei:unclear></tei:p>
        <tei:p><tei:note type="editorial" place="inline">signed: </tei:note><tei:handShift new="#Tr"
          />Troppl</tei:p>
      </tei:postscript>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>ANTIQUARIAN BOOKS AND ART</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">V. A. HECK</tei:hi></tei:addrLine>
          <tei:addrLine>VIENNA, DISTRICT I, KÄRNTNERRING No. 12</tei:addrLine>
          <tei:addrLine><tei:hi rend="bold">Telephone 51-2-61</tei:hi></tei:addrLine>
          <tei:addrLine>____________________</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:placeName>VIENNA</tei:placeName>, <tei:handShift
            new="#unkVAH"/><tei:date when="1927-03-10">March 10, 1927</tei:date></tei:dateline><tei:lb/>
        <tei:address>
          <tei:addrLine>Prof. Heinrich Schenker</tei:addrLine>
          <tei:addrLine rend="indent-80">Vienna<tei:ptr corresp="#r0001-fn01" type="footnote"
            /></tei:addrLine>
        </tei:address><tei:lb/><tei:lb/>
      </tei:opener>

      <tei:p><tei:handShift new="#printed"/>Please to receive:</tei:p>

      <tei:p rend="first">Beethoven<tei:lb/>Sketchleaves<tei:lb/>4 pages<tei:ptr
          corresp="#r0001-fn02" type="footnote"/></tei:p>
      <tei:lb/>


      <tei:closer>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>Respectfully,</tei:salute>
        <tei:signed rend="indent-60"><tei:rs key="entity-000345" type="person"><tei:hi rend="bold"
                ><tei:hi rend="sperr">V. A.
        HECK.</tei:hi></tei:hi></tei:rs></tei:signed><tei:lb/><tei:lb/>
      </tei:closer>

      <tei:postscript>
        <tei:milestone unit="folio" n="verso"/>
        <tei:lb/>
        <tei:p><tei:handShift new="#unkVAH"/><tei:unclear reason="original_unclear"
            >Retrieved</tei:unclear></tei:p>
        <tei:p><tei:note type="editorial" place="inline">signed: </tei:note><tei:handShift new="#Tr"
          />Troppl</tei:p>
      </tei:postscript>

    </tei:div>
