      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typed postcard from August Demblin (DMV) to Schenker, dated April 5, 1927</dc:title>
      <dcterms:created>1927-04-05T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-26</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag confirm receipt of the corrected graphs and freshly copied
        music examples for Meisterwerk 2.</dc:description>
      <dc:subject>graph; Tafel; musical graph; Notentafel; copy; Vorlage; printer;
        Druckerei</dc:subject>

    <marcrel:correspondent key="entity-002627">DMV</marcrel:correspondent><marcrel:correspondent key="entity-004417">Demblin</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG A.-G.</tei:addrLine>
            <tei:addrLine>MÜNCHEN</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#TPunk"/>Herrn Dr. Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-30"><tei:hi rend="underline">Wien III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-30">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MUENCHEN</tei:placeName>
            <tei:note type="editorial">remainder illegible</tei:note> ||</tei:fw><tei:lb/>
        </tei:fw>
      
      <tei:lb/>
      <tei:lb/>
      <tei:lb/>

      <tei:milestone unit="folio" n="verso"/>
      <tei:lb/>

      <tei:address>
        <tei:addrLine><tei:handShift new="#TPunk"/><tei:hi rend="underline">Herrn Dr. Heinrich Schenker, Wien
          III, Keilgasse 8</tei:hi></tei:addrLine>
        <tei:addrLine><tei:placeName>München</tei:placeName>, den <tei:date when="1927-04-05">5. April 1927</tei:date>.</tei:addrLine>
      </tei:address>
        <tei:lb/>
      
      <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632">Doktor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this letter is recorded in
          Schenker's diary at OJ 3/9, p. 3054, April 6, 1927: "Vom Verlag (K.): bestätigen den
          Eingang der Stichkorrekturen u. der neuen Vorlagen." ("From the publisher (postcard): they
          confirm the arrival of the corrected proofs of the engraved <tei:supplied reason="clarity">music examples</tei:supplied> and the new source materials.").</tei:note>
      </tei:salute>
      </tei:opener>

      <tei:p rend="first">Wir bestätigen hiermit den Eingang Ihres Schreibens vom <tei:date when="1927-03-27">27. März</tei:date><tei:note xml:id="r0001-fn02" type="editorial" place="foot">Postage of this letter is recorded at <tei:ref type="correspondence" cRef="OC-54-142">OC 54/142</tei:ref>. The original is not known to survive.</tei:note>
        sowie der korrigierten <tei:term>Notentafeln</tei:term> und der neuen Vorlagen, die wir an
        die <tei:rs key="entity-003129" type="organization">Druckerei</tei:rs> weitergaben.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">Mit ausgezeichneter Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002627" type="organization">Drei Masken Verlag A.-G.</tei:rs></tei:signed>
        <tei:signed rend="indent-50">ppa. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG &amp; Co.</tei:addrLine>
            <tei:addrLine>MUNICH</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#TPunk"/>Dr. Heinrich Schenker</tei:addrLine>
            <tei:addrLine rend="indent-30"><tei:hi rend="underline">Vienna
              III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-30">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MUNICH</tei:placeName>
            <tei:note type="editorial">remainder illegible</tei:note> ||</tei:fw><tei:lb/>
        </tei:fw>
      
      
      <tei:lb/><tei:lb/><tei:lb/>

      <tei:milestone unit="folio" n="verso"/>
<tei:lb/>

      <tei:address>
        <tei:addrLine><tei:handShift new="#TPunk"/><tei:hi rend="underline">Dr. Heinrich Schenker, Vienna
          III, Keilgasse 8</tei:hi></tei:addrLine>
        <tei:addrLine><tei:placeName>Munich</tei:placeName>, <tei:date when="1927-04-05">April 5, 1927</tei:date>.</tei:addrLine>
      </tei:address>

      <tei:lb/>

      <tei:salute>Dear Dr. <tei:rs type="person" key="entity-000632"><tei:supplied reason="identity">Schenker</tei:supplied></tei:rs>!<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>

      </tei:opener>

      <tei:p rend="first">We hereby confirm the arrival of you letter of <tei:date when="1927-03-27">March 27</tei:date>,<tei:ptr corresp="#r0001-fn02" type="footnote"/> as well as the
        corrected musical <tei:term>graph</tei:term>s and the new copies, which we shall forward to
        the <tei:rs key="entity-003129" type="organization">printer</tei:rs>.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40">With our highest regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002627" type="organization">Drei Masken Verlag &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-50">p.p. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>
