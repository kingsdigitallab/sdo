 <sdo:record ID="r0001">
  <sdo:itemDesc>
   <!-- MT Tags: "DeutschS ~ HSchenker", "Wiener Bank-Verein ~ HSchenker" -->
   <dc:title>Credit slip from Wiener Bank-Verein, dated October 10, 1919</dc:title>
   <dc:type>Bank document</dc:type>
   <dcterms:created>1919-10-10T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2006-10-12</dcterms:dateSubmitted>
   <dc:description>Credits Schenker with 346.92 Marks and converts them to 1,121.40
    Kronen.</dc:description>
   <dc:subject>money; exchange rate; credit</dc:subject>

   <marcrel:correspondent key="EATS_nNNNNNN">Wiener Bank-Verein</marcrel:correspondent>
   <marcrel:recipient key="EATS_nNNNNNN">Heinrich Schenker</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">
   <tei:note type="editorial">Roman type = printed; Italic type = handwritten</tei:note>
   <tei:lb/>
   <tei:ab type="formpart" rend="printed">
    <tei:address rend="centre">
     <tei:addrLine>K. K. <tei:note type="editorial">device</tei:note> PRIV.</tei:addrLine>
     <tei:addrLine>WIENER BANK-VEREIN</tei:addrLine>
     <tei:addrLine>DEPOSITEN UND WECHSELSTUBE <tei:hi rend="bold">RENNWEG</tei:hi></tei:addrLine>
    </tei:address>
   </tei:ab>
   <tei:note type="editorial">addresses and telephone numbers of headquarters and branches, not
    reproduced here</tei:note>
   <tei:ab type="formpart" rend="printed">WIEN, am <tei:seg type="entry" rend="ms">10. Oktober
     1919</tei:seg>
    <tei:address>
     <tei:addrLine>III, Rennweg 11.</tei:addrLine>
    </tei:address></tei:ab>
   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">
     <tei:address>
      <tei:addrLine>Herrn Dr. <tei:persName key="entity-000632">Heinrich
       Schenker</tei:persName>,</tei:addrLine>
      <tei:addrLine>
       <tei:hi rend="underline">Wien</tei:hi>
      </tei:addrLine>
     </tei:address>
    </tei:seg>
   </tei:ab>
   <tei:ab type="formpart" rend="printed">Wir bestätigen unser Ergebenes vom . . .</tei:ab>
   <tei:ab type="formpart" rend="printed">und bekennen uns zum Empfange Ihres Geehrten vom . .
    .</tei:ab>
   <tei:ab type="formpart" rend="printed">Wir belasten Sie für . . . <tei:note type="editorial">all
     left blank and canceled</tei:note></tei:ab>
   <tei:ab type="formpart" rend="printed">und erkennen Sie für <tei:seg type="entry" rend="ms">M
     346.92 Scheck Berlin St. bei <tei:gap reason="illegible"/> Note mit <tei:lb/><tei:hi
      rend="underline">K 1121.40 V<tei:hi rend="supralinear">a</tei:hi> 8.crt.</tei:hi></tei:seg>
   </tei:ab>
   <tei:note type="editorial">left side:</tei:note>
   <tei:ab type="formpart" rend="stamped">Gebucht</tei:ab>
   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">1 Note. E 1207.30 / 17</tei:seg>
   </tei:ab>
   <tei:note type="editorial">right side:</tei:note>
   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">Hochachtungsvoll</tei:seg>
   </tei:ab>
   <tei:ab type="formpart" rend="printed">
    <tei:address>
     <tei:addrLine>WIENER BANK-VEREIN</tei:addrLine>
     <tei:addrLine>Depositenkasse und Wechselstube <tei:hi rend="bold"
      >Rennweg</tei:hi></tei:addrLine>
    </tei:address>
   </tei:ab>
   <tei:note type="editorial">signature illegible</tei:note>
  </tei:div>


  <tei:div type="translation">
   <tei:note type="editorial">Roman type = printed; Italic type = handwritten</tei:note>
   <tei:lb/>
   <tei:ab type="formpart" rend="printed">
    <tei:address rend="centre">
     <tei:addrLine>ROYAL-IMP. <tei:note type="editorial">device</tei:note> PRIV.</tei:addrLine>
     <tei:addrLine>WIENER BANK-VEREIN</tei:addrLine>
     <tei:addrLine>DEPOSIT CASHDESK AND EXCHANGE <tei:hi rend="bold">RENNWEG</tei:hi></tei:addrLine>
    </tei:address>
   </tei:ab>
   <tei:note type="editorial">addresses and telephone numbers of headquarters and branches, not
    reproduced here</tei:note>
   <tei:ab type="formpart" rend="printed">VIENNA, <tei:seg type="entry" rend="ms">October 10,
     1919</tei:seg>
    <tei:address>
     <tei:addrLine>III, Rennweg 11</tei:addrLine>
    </tei:address></tei:ab>
   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">
     <tei:address>
      <tei:addrLine>Dr. <tei:persName key="entity-000632">Heinrich
       Schenker</tei:persName>,</tei:addrLine>
      <tei:addrLine>
       <tei:hi rend="underline">Vienna</tei:hi>
      </tei:addrLine>
     </tei:address>
    </tei:seg>
   </tei:ab>
   <tei:ab type="formpart" rend="printed">We confirm our humble <tei:supplied reason="clarity"
     >communication</tei:supplied> of . . .</tei:ab>
   <tei:ab type="formpart" rend="printed">and acknowledge receipt of your esteemed . . .</tei:ab>
   <tei:ab type="formpart" rend="printed">We debit you . . .<tei:note type="editorial">all left
     blank and canceled</tei:note></tei:ab>
   <tei:ab type="formpart" rend="printed">and credit you with <tei:seg type="entry" rend="ms">346.92
     Marks: check Berlin <tei:gap reason="illegible"/> with <tei:lb/><tei:hi rend="underline"
      >1121.40 Kronen V<tei:hi rend="supralinear">a</tei:hi> 8.crt.</tei:hi></tei:seg>
   </tei:ab>
   <tei:note type="editorial">left side:</tei:note>
   <tei:ab type="formpart" rend="stamped">Entered</tei:ab>
   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">1 Note. E 1207.30 / 17</tei:seg>
   </tei:ab>
   <tei:note type="editorial">right side:</tei:note>
   <tei:ab type="formpart" rend="printed">
    <tei:seg type="entry" rend="ms">Yours faithfully</tei:seg>
   </tei:ab>
   <tei:ab type="formpart" rend="printed">
    <tei:address>
     <tei:addrLine>WIENER BANK-VEREIN</tei:addrLine>
     <tei:addrLine>Deposit cashdesk and exchange <tei:hi rend="bold">Rennweg</tei:hi></tei:addrLine>
    </tei:address>
   </tei:ab>
   <tei:note type="editorial">signature illegible</tei:note>
  </tei:div>
 </sdo:record>
