   <dc:title>Handwritten notecard from Richard Wahle to Schenker, dated November 9, 1907</dc:title>

   <dcterms:created>1907-11-09T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2022-10-20</dcterms:dateSubmitted>
   <dc:description>Richard Wahle thanks Schenker for his expression of sympathy.</dc:description>
   <dc:subject/>

   <marcrel:correspondent key="entity-000926">WahleR</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">


   <tei:p>Meinen innigsten Dank, lieber Freund, f√ºr deine herzliche Teilnahme.<tei:note
     xml:id="r0001-fn01" type="editorial" place="foot">The circumstances are
    unknown</tei:note></tei:p>


   <tei:closer>
    <tei:salute rend="indent-70">Dein</tei:salute>
    <tei:signed rend="indent-80"><tei:rs type="person" key="entity-000926">Richard
     W</tei:rs></tei:signed><tei:lb/>
    <tei:dateline><tei:date when="1907-11-09">9/11 1907</tei:date></tei:dateline>
   </tei:closer>

  </tei:div>


  <tei:div type="translation">

   <tei:p>My sincerest thanks, dear friend, for your heartfelt sympathy.<tei:ptr
     corresp="#r0001-fn01" type="footnote"/></tei:p>


   <tei:closer>
    <tei:salute rend="indent-70">Your</tei:salute>
    <tei:signed rend="indent-80"><tei:rs type="person" key="entity-000926">Richard
     W</tei:rs></tei:signed><tei:lb/>
    <tei:dateline><tei:date when="1907-11-09">November 9, 1907</tei:date></tei:dateline>
   </tei:closer>

