  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002709">Roth ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>




      <dc:title>Typed postcard from Barbara Rothe (UE) to Schenker, dated May 31, 1927</dc:title>

      <dcterms:created>1927-05-31T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2012-07-19</dcterms:dateSubmitted>
      <dc:description>A copy of Schenker's Fifth Symphony monograph has been dispatched to
        Vrieslander as requested.</dc:description>
      <dc:subject>Monographie; monograph; Rezensionsexemplar; complimentary copy </dc:subject>

      <marcrel:correspondent key="entity-000741">Barbara Rothe</marcrel:correspondent>
      <marcrel:correspondent key="entity-002408">UE</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">Heinrich Schenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:note type="editorial">printed in art deco typography, all in deckled frame:</tei:note>
        <tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Postkarte</tei:fw>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>UNIVERSAL-EDITION A.-G.</tei:addrLine>
            <tei:addrLine>WIEN, I. KARLSPLATZ 6. (Musikvereinsgebäude)</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An: </tei:supplied><tei:handShift
                new="#TPhfi"/>Herrn Professor Heinrich <tei:hi rend="sperr"
              >Schenker</tei:hi>,</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:hi rend="sperr">Wien</tei:hi> III.</tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
          </tei:address>
          <tei:lb/>
          <tei:fw type="postmark">|| <tei:gap reason="illegible"/> WIEN 1 | 1. VI. 27 | 13-14 | * 4b
            * ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-50"><tei:placeName>Wien</tei:placeName>, am <tei:date
            when="1927-05-31">31. Mai 1927</tei:date>.</tei:dateline><tei:lb/>
        <tei:byline><tei:handShift new="#TPws"/>Dr.H./Fi.</tei:byline>
        <tei:address>
          <tei:addrLine rend="indent-20">Herrn Professor Heinrich <tei:hi rend="sperr"
              >Schenker</tei:hi>,</tei:addrLine>
          <tei:addrLine rend="indent-60">
            <tei:hi rend="underline"><tei:hi rend="sperr">Wien</tei:hi> III.</tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse 8</tei:addrLine>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
            >Professor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this postcard is recorded in Schenker's diary at OJ 3/9, p. 3072, June 1, 1927: "Von
            der U.-E. (K.): melden die Sendung der V. Sinf. an Vrieslander." ("From UE (postcard):
            they report having sent the Fifth Symphony to Vrieslander."). This postcard exists in
            two sources: OC 52/840, the dispatched postcard with printed letterhead and signatures,
            and WSLB 383, the carbon copy of the recto only, without signatures but initialed.
          </tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Im Besitze Ihrer Karte vom 27. ds. M.,<tei:note xml:id="r0001-fn02"
          type="editorial" place="foot">= WSLB 380. </tei:note> haben wir sofort an Herrn <tei:rs
          key="entity-000922" type="person">Otto Vrieslander</tei:rs> ein Rezensionsexemplar Ihrer
          "<tei:rs key="entity-001760" type="work"><tei:title>Monographie über die V. Sinfonie von
              <tei:rs key="entity-000055" type="person">Beethoven</tei:rs></tei:title></tei:rs>"
        gesandt und empfehlen uns </tei:p>


      <tei:closer>
        <tei:salute rend="indent-40">mit vorzüglicher Hochachtung</tei:salute>
        <tei:signed rend="indent-40"><tei:handShift new="#stamp"/><tei:rs key="entity-002408"
            type="organization">Universal-Edition Actiongesellschaft</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#AK"/>pp <tei:rs key="entity-000741"
            type="person">Rothe</tei:rs></tei:signed>
      </tei:closer>
    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:note type="editorial">printed in art deco typography, all in deckled frame:</tei:note>
        <tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Postcard</tei:fw>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>UNIVERSAL EDITION &amp; CO.,</tei:addrLine>
            <tei:addrLine>VIENNA I, KARLSPLATZ 6 (Musikverein Building)</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">To: </tei:supplied><tei:handShift
                new="#TPhfi"/>Professor Heinrich <tei:hi rend="sperr"
              >Schenker</tei:hi>,</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:hi rend="sperr">Vienna</tei:hi> III,</tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
          </tei:address>
          <tei:lb/>
          <tei:fw type="postmark">|| <tei:gap reason="illegible"/> VIENNA 1 | 1. VI. 27 | 13-14 | *
            4b * ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:dateline rend="indent-50"><tei:placeName>Vienna</tei:placeName>, <tei:date
            when="1927-05-31">May 31, 1927</tei:date>.</tei:dateline><tei:lb/>
        <tei:byline><tei:handShift new="#TPws"/>Dr.H./Fi.</tei:byline>
        <tei:address>
          <tei:addrLine rend="indent-20">Professor Heinrich <tei:hi rend="sperr"
            >Schenker</tei:hi>,</tei:addrLine>
          <tei:addrLine rend="indent-60">
            <tei:hi rend="underline"><tei:hi rend="sperr">Vienna</tei:hi> III,</tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse 8</tei:addrLine>
        </tei:address>
        <tei:salute>Dear <tei:rs type="person" key="entity-000632">Professor</tei:rs>,<tei:ptr
            corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">In receipt of your postcard of the 27th of the month,<tei:ptr
          corresp="#r0001-fn02" type="footnote"/> we have immediately dispatched a review copy of
        your <tei:rs key="entity-001760" type="work"><tei:title>monograph on the Fifth Symphony of
              <tei:rs key="entity-000055" type="person">Beethoven</tei:rs></tei:title></tei:rs> to
        Mr. <tei:rs key="entity-000922" type="person">Otto Vrieslander</tei:rs>, and remain</tei:p>


      <tei:closer>
        <tei:salute rend="indent-40">with kind regards,</tei:salute>
        <tei:signed rend="indent-40"><tei:handShift new="#stamp"/><tei:rs key="entity-002408"
            type="organization">Universal Edition &amp; Co.,</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#AK"/>pp <tei:rs key="entity-000741"
            type="person">Rothe</tei:rs></tei:signed>
      </tei:closer>
    </tei:div>
  </sdo:record>
