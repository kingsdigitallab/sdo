  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002700">Hertzka ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">postcard</tei:rs>



      <dc:title>Typewritten express postcard from UE to Schenker, dated April 16, 1912</dc:title>

      <dcterms:created>1912-04-16T14:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2023-01-09</dcterms:dateSubmitted>
      <dc:description>Hertzka acknowledges receipt of the Preface to Beethovens neunte
        Sinfonie.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-002408">Universal Edition</marcrel:correspondent>
      <marcrel:correspondent key="entity-000356">Emil Hertzka</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>

    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">advertisement for "Excelsior" in left third of
            <tei:hi rend="italic">recto</tei:hi></tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPueN"/>Wohlgeboren Herrn Prof. H.
            Schenker</tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:hi rend="sperr underline">Wien III</tei:hi>. </tei:addrLine>
          <tei:addrLine rend="indent-50">Reisnerstr. 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 3/3/ WIEN <tei:gap reason="illegible"/> | 16.IV.12 2<tei:hi
            rend="supralinear">10</tei:hi> | * P * |||</tei:fw>
        <tei:fw type="postmark">|| 1/1 WIEN <tei:gap reason="illegible"/> | 16 IV <tei:gap
            reason="illegible"/> | * P * ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPueN"/><tei:hi rend="underline">R.Wohlg. Herrn Prof.
                <tei:rs type="person" key="entity-000632">H.
            Schenker</tei:rs></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="sperr underline"><tei:rs type="place"
                key="entity-002256">Wien</tei:rs></tei:hi></tei:addrLine>
          <tei:lb/>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
          >Professor</tei:rs>!</tei:salute>
      </tei:opener>

      <tei:p>Antwortlich Ihrer gesch. Karte von heute<tei:note xml:id="r0001-fn01" type="editorial"
          place="foot">= <tei:ref type="correspondence" cRef="WSLB-107">WSLB 107</tei:ref>, April
          16, 1912.</tei:note> teilen wir Ihnen höfl. mit, dass <tei:hi rend="supralinear"
          >wir</tei:hi> die Vorrede<tei:note xml:id="r0001-fn02" type="editorial" place="foot">Of
          Schenker’s monograph <tei:rs type="work" key="entity-001731"><tei:title level="m"
              >Beethovens neunte Sinfonie</tei:title></tei:rs>.</tei:note> richtig erhalten haben
        und dass dieselbe auch bereits zum Druck gegangen ist. </tei:p>

      <tei:p>Uns bestens empfehlend, zeichnen wir</tei:p>

      <tei:closer>
        <tei:dateline><tei:handShift new="#printed"/>Wien, am <tei:date when="1912-04-16"
              ><tei:handShift new="#TPueN"/>16. April <tei:handShift new="#printed"
              />191<tei:handShift new="#TPueN"/>2</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>I, Reichsratsstraße 9</tei:addrLine>
          <tei:addrLine><tei:note type="editorial" place="inline"
              >initialed:</tei:note><tei:handShift new="#ueN"/>N</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-002408" type="organization"
            >„Universal-Edition“ A-G.</tei:rs></tei:signed>
        <tei:signed rend="indent-70">(Jos. Aibl Verlag, G.m.b.H.)</tei:signed>
        <tei:signed rend="indent-90"><tei:handShift new="#EH"/><tei:rs type="person"
            key="entity-000356">H</tei:rs></tei:signed>
      </tei:closer>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:note type="editorial" place="inline">advertisement for "Excelsior" in left third of
            <tei:hi rend="italic">recto</tei:hi></tei:note><tei:lb/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPueN"/>Prof. H. Schenker, Esq.,</tei:addrLine>
          <tei:addrLine rend="indent-50"><tei:hi rend="sperr underline">Vienna
            III</tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-50">Reisnerstraße 38</tei:addrLine>
        </tei:address>
        <tei:fw type="postmark">|| 3/3/ VIENNA <tei:gap reason="illegible"/> | 16.IV.12 2<tei:hi
            rend="supralinear">10</tei:hi> | * P * |||</tei:fw>
        <tei:fw type="postmark">|| 1/1 VIENNA <tei:gap reason="illegible"/> | 16 IV <tei:gap
            reason="illegible"/> | * P * ||</tei:fw><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPueN"/><tei:hi rend="underline">Prof. <tei:rs
                type="person" key="entity-000632">H. Schenker, Esq.</tei:rs></tei:hi></tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="sperr underline"><tei:rs type="place"
                key="entity-002256">VIENNA</tei:rs></tei:hi></tei:addrLine>
          <tei:lb/>
        </tei:address>
        <tei:salute>Dear <tei:rs type="person" key="entity-000632">Professor</tei:rs>,</tei:salute>
      </tei:opener>

      <tei:p>In answer to your esteemed postcard of today,<tei:ptr corresp="#r0001-fn01"
          type="footnote"/> we respectfully inform you that <tei:hi rend="supralinear">we</tei:hi>
        have received your Foreword<tei:ptr corresp="#r0001-fn02" type="footnote"/> safely, and that
        the latter has in fact already gone off for printing.</tei:p>

      <tei:p>With our best compliments, we remain</tei:p>

      <tei:closer>
        <tei:dateline><tei:handShift new="#printed"/>Vienna, <tei:date when="1912-04-16"
              ><tei:handShift new="#TPueN"/>April 16, <tei:handShift new="#printed"
              />191<tei:handShift new="#TPueN"/>2</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/>I, Reichsratsstraße 9</tei:addrLine>
          <tei:addrLine><tei:note type="editorial" place="inline"
              >initialed:</tei:note><tei:handShift new="#ueN"/>N</tei:addrLine>
        </tei:address>
        <tei:lb/>
        <tei:salute rend="indent-80"><tei:handShift new="#printed"/>With kind regards,</tei:salute>
        <tei:signed rend="indent-70"><tei:rs key="entity-002408" type="organization">Universal
            Edition &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70">(Jos. Aibl Publishers, &amp; Co.)</tei:signed>
        <tei:signed rend="indent-90"><tei:handShift new="#EH"/><tei:rs type="person"
            key="entity-000356">H.</tei:rs></tei:signed>
      </tei:closer>



    </tei:div>
