      <tei:note type="editorial">top-center panel:</tei:note>


      <tei:ab type="formpart" rend="printed">Gattung: <tei:handShift new="#stamp"/><tei:seg
          type="entry" rend="ms"><tei:hi rend="italic">ANTWORT BEZAHLT</tei:hi></tei:seg> –
        Eingangsnummer: <tei:handShift new="#stamp"/><tei:seg type="entry" rend="ms"><tei:hi
            rend="italic">311</tei:hi></tei:seg></tei:ab>

