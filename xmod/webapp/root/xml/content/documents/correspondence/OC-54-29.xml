  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-004414">DMV ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-004415">Demblin ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-004416">Einstein ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>




      <dc:title>Typed postcard from August Demblin and Alfred Einstein (DMV) to Schenker, dated June
        10, 1925</dc:title>

      <dcterms:created>1925-06-10T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2013-09-23</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag requests that Schenker send the manuscript of the first
        Meisterwerk Yearbook directly to their Munich office.</dc:description>
      <dc:subject>Manuskript; manuscript</dc:subject>



    <marcrel:correspondent key="entity-000195">Einstein</marcrel:correspondent><marcrel:correspondent key="entity-002627">DMV</marcrel:correspondent><marcrel:correspondent key="entity-004417">Demblin</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:note type="editorial">in Jugendstil type, all in triple rule-frame:</tei:note>
        <tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>POSTKARTE</tei:fw><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:note type="editorial" place="inline">graphic device with "DMV" below</tei:note>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG A.-G.</tei:addrLine>
            <tei:addrLine>MÜNCHEN</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#TPen"/>Herrn Prof. Dr. Heinrich <tei:hi rend="sperr">Schenker</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Wien III</tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| MUENCHEN 40 | 10 JUN 25 | * c3 * ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPen"/><tei:hi rend="underline">Herrn Prof. Heinrich
              Schenker, Wien III, Keilgasse 8</tei:hi></tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:placeName>München</tei:placeName>, den <tei:date when="1925-06-10">10. Juni 1925</tei:date></tei:dateline>
        <tei:salute>Verehrter Herr <tei:rs type="person" key="entity-000632">Professor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this letter is recorded in Schenker's diary at OJ 3/7, p. 2828, June 12, 1925: "Von
            dem 3 Masken Verlag (K.): bitten, das Manuscript nach München zu senden." ("From Drei
            Masken Verlag (postcard): they ask [me] to send the manuscript to
          Munich.")</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">wir bitten Sie, das Manuskript Ihres <tei:rs key="entity-001750" type="work"><tei:title>Jahrbuch</tei:title></tei:rs>s uns direkt nach <tei:rs key="entity-002286" type="place">München</tei:rs> zu schicken und erwarten es mit
        Spannung.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-10">Mit dem Ausdruck besonderer Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002627" type="organization">Drei Masken Verlag A.-G.</tei:rs></tei:signed>
        <tei:signed rend="indent=50">ppa. <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="AE"/>i.V. <tei:rs key="entity-000195" type="person">A. Einstein</tei:rs></tei:signed>
      </tei:closer>
      <tei:lb/>

      <tei:postscript>
        <tei:p><tei:lb/><tei:handShift new="#TPen"/>Dr.E./N.</tei:p>
      </tei:postscript>

    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:note type="editorial">in Jugendstil type, all in triple rule-frame:</tei:note>
        <tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>POSTCARD</tei:fw><tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:note type="editorial" place="inline">graphic device with "DMV" below</tei:note>
            <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG &amp; Co.</tei:addrLine>
            <tei:addrLine>MUNICH</tei:addrLine>
            <tei:addrLine>KAROLINENPLATZ 3</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#TPen"/>Prof. Dr. Heinrich <tei:hi rend="sperr">Schenker</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-10">Vienna III</tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| MUNICH 40 | 10 JUN 25 | * c3 * ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#TPen"/><tei:hi rend="underline">Prof. Heinrich
              Schenker, Vienna III, Keilgasse 8</tei:hi></tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:placeName>Munich</tei:placeName>, <tei:date when="1925-06-10">June 10, 1925</tei:date></tei:dateline>
        <tei:salute>Revered <tei:rs type="person" key="entity-000632">Professor</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">We request that you send the manuscript of your <tei:rs key="entity-001750" type="work"><tei:title>Yearbook</tei:title></tei:rs> to us directly in
          <tei:rs key="entity-002286" type="place">Munich</tei:rs>, and await it with eagerness. </tei:p>

      <tei:closer>
        <tei:salute rend="indent-10">With expression of kind regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002627" type="organization">Drei Masken Verlag &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent=50">ppa <tei:handShift new="#AD"/>
          <tei:rs key="entity-004417" type="person">Demblin</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="AE"/>on behalf of the publisher <tei:rs key="entity-000195" type="person">A. Einstein</tei:rs></tei:signed>
      </tei:closer>
      <tei:lb/>

      <tei:postscript>
        <tei:p><tei:lb/><tei:handShift new="#TPen"/>Dr.E./N.</tei:p>
      </tei:postscript>

    </tei:div>
  </sdo:record>
