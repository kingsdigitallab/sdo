  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-004414">DMV ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-004929">BöhmeA ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter
        (Half-Page)</tei:rs>




      <dc:title>Typed letter from Alfred Böhme (DMV) to Schenker, dated June 11, 1926</dc:title>

      <dcterms:created>1926-06-11T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-15</dcterms:dateSubmitted>
      <dc:description>Drei Masken Verlag acknowledge receipt of the manuscript of Das Meisterwerk in
        der Musik, vol. 2.</dc:description>
      <dc:subject>manuscript; Manuskript; text; Text; music example; Notenbeispiel;
        Urline-Tafel</dc:subject>




    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:address>

          <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG GES. M.B.H.</tei:addrLine>
          <tei:addrLine>WIEN I.</tei:addrLine>
          <tei:addrLine>WALLNERSTRASSE 4</tei:addrLine>
          <tei:lb/>
        </tei:address>
        <tei:byline><tei:handShift new="printed"/>Abt. .. | Dikt. <tei:handShift new="#TPgif"/>GI. |
            <tei:handShift new="#printed"/>Masch. <tei:handShift new="#TPgif"/>F.</tei:byline>
        <tei:dateline rend="indent-70">
          <tei:placeName><tei:handShift new="#printed"/>WIEN</tei:placeName>, <tei:handShift
            new="#TPgif"/><tei:date when="1926-06-11">11.6.26</tei:date>.</tei:dateline><tei:lb/>
        <tei:address>

          <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>An <tei:handShift new="#TPgif"/>Herrn Professor Dr. H. Schenker</tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="sperr">Wien.</tei:hi></tei:addrLine>

        </tei:address><tei:lb/>
        <tei:salute>Sehr verehrter Herr <tei:rs key="entity-000632" type="person"
            >Professor</tei:rs>,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this letter is not recorded in Schenker's diary.</tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">wir bestätigen hiermit den Empfang des Manuskriptes für den 2. Jahrgang
        des <tei:rs key="entity-001751" type="work"><tei:title>MEISTERWERKES IN DER
            MUSIK</tei:title></tei:rs> und zwar: 250 + 84 Blatt Text und 34 Blatt Notenbeispiele und
        3 Blatt <tei:term>Urlinie-Tafeln</tei:term>. </tei:p>


      <tei:closer>
        <tei:salute rend="indent-50">Mit vorzüglicher Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="stamp"/><tei:rs key="entity-002627"
            type="organization">DREI MASKEN VERLAG Ges. m.b.H.</tei:rs></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#AB"/><tei:rs key="entity-004930"
            type="person">Böhme</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:lb/><tei:lb/>
        <tei:p><tei:handShift new="#unk"/><tei:note type="editorial">Indecipherable
            signature.</tei:note></tei:p>
      </tei:postscript>


    </tei:div>



    <tei:div type="translation">


      <tei:opener>
        <tei:address>

          <tei:addrLine><tei:handShift new="#printed"/>DREI MASKEN VERLAG &amp; Co.</tei:addrLine>
          <tei:addrLine>VIENNA I</tei:addrLine>
          <tei:addrLine>WALLNERSTRAßE 4</tei:addrLine>
          <tei:lb/>
        </tei:address>
        <tei:byline><tei:handShift new="printed"/>Dept. .. | Dict. <tei:handShift new="#TPgif"/>GI. |
            <tei:handShift new="#printed"/>Typ. <tei:handShift new="#TPgif"/>F.</tei:byline>
        <tei:dateline rend="indent-70">
          <tei:placeName><tei:handShift new="#printed"/>VIENNA</tei:placeName>, <tei:handShift
            new="#TPgif"/><tei:date when="1926-06-11">11.6.26</tei:date>.</tei:dateline><tei:lb/>
        <tei:address>

          <tei:addrLine rend="indent-50"><tei:handShift new="#printed"/>To <tei:handShift new="#TPgif"/>Professor Dr. H. Schenker</tei:addrLine>
          <tei:addrLine rend="indent-60"><tei:hi rend="sperr">Vienna.</tei:hi></tei:addrLine>

        </tei:address><tei:lb/>
        <tei:salute>Revered Professor <tei:rs key="entity-000632" type="person"><tei:supplied
              reason="identity">Schenker</tei:supplied></tei:rs>,<tei:ptr corresp="#r0001-fn01"
            type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">We hereby confirm the receipt of the manuscript for the second year of <tei:rs key="entity-001751" type="work"><tei:title level="m">Das
        Meisterwerk in der Musik,</tei:title></tei:rs> specifically 250 + 84 pages of text and 34 pages of music
        examples, and three pages of <tei:term>Urlinie graphs</tei:term>. </tei:p>


      <tei:closer>
        <tei:salute rend="indent-50">With kind regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="stamp"/><tei:rs key="entity-002627"
            type="organization">DREI MASKEN VERLAG &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#AB"/><tei:rs key="entity-004930"
            type="person">Böhme</tei:rs></tei:signed>
      </tei:closer>

      <tei:postscript>
        <tei:lb/><tei:lb/>
        <tei:p><tei:handShift new="#unk"/><tei:note type="editorial">Indecipherable
            signature.</tei:note></tei:p>
      </tei:postscript>

    </tei:div>
