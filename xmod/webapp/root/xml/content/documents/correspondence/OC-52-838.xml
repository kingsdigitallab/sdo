  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002710">Kalmus ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002701">UE ~
        H+JSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-002702">UE ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>




      <dc:title>Typed postcard from Kalmus (UE) to Schenker, dated May 27, 1927</dc:title>

      <dcterms:created>1927-05-27T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2012-07-18</dcterms:dateSubmitted>
      <dc:description>Proofs of the defective pages of the revised "Appassionata" Symphony have been
        dispatched to Schenker.</dc:description>
      <dc:subject>Abzug; proof-pull</dc:subject>

      <marcrel:correspondent key="entity-000420">Alfred Kalmus</marcrel:correspondent>
      <marcrel:correspondent key="entity-002408">UE</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">Heinrich Schenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:note type="editorial">printed in art deco typography, all in deckled frame:</tei:note>
        <tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Postkarte</tei:fw>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>UNIVERSAL-EDITION A.-G.</tei:addrLine>
            <tei:addrLine>WIEN, I. KARLSPLATZ 6. (Musikvereinsgebäude)</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An: </tei:supplied><tei:handShift
                new="#TPlhhü"/>Herrn Prof. Dr. Heinrich Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:hi rend="sperr">Wien</tei:hi>, III.,</tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8/12.</tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| <tei:gap reason="illegible" extent="2" unit="chars"/> WIEN 1 |
            28. V. 27 | 15-16 | * <tei:gap reason="illegible" extent="2" unit="chars"/> *
            ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:byline><tei:handShift new="#TPws"/>LH/Hü</tei:byline>
        <tei:dateline rend="indent-50"><tei:placeName>Wien</tei:placeName>, am <tei:date
            when="1927-05-27">27. Mai 1927</tei:date>.</tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-20">Herrn Professor Dr. Heinrich Schenker,</tei:addrLine>
          <tei:addrLine rend="indent-60">
            <tei:hi rend="underline"><tei:hi rend="sperr">Wien</tei:hi>, III.,</tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse 8/12.</tei:addrLine>
        </tei:address>
        <tei:salute>Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
            >Professor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
            of this postcard is recorded in Schenker's diary at OJ 3/9, p. 3071, May 30, 1927: "Von
            der U.-E. recomm. zwei korrigierte Abzüge von op. 57 u. eine Karte mit Verständigung
            davon." ("From UE by registered mail, two corrected proof-pulls from Op. 57 and a
            postcard explaining them."). This postcard exists in two sources: OC 52/838, the
            dispatched postcard with printed letterhead and signatures, and WSLB 381, the carbon
            copy of the recto only, without signatures but initialed. </tei:note></tei:salute>
      </tei:opener>

      <tei:p rend="first">Wunschgemäss lassen wir Ihnen noch einen Abzug der fehlerhaften Seiten der
          "<tei:rs key="entity-001226" type="composition"><tei:sic>Appasionata</tei:sic></tei:rs>"
        zugehen und bitten um baldige Retournierung nach Durchsicht. </tei:p>


      <tei:closer>
        <tei:salute rend="indent-40">Mit vorzüglicher Hochachtung</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002408"
            type="organization">Universal-Edition Actiongesellschaft</tei:rs></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#AK"/><tei:rs key="entity-000420"
            type="person">Kalmus</tei:rs></tei:signed>
      </tei:closer>
    </tei:div>


    <tei:div type="translation">
      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:note type="editorial">printed in art deco typography, all in deckled frame:</tei:note>
        <tei:lb/>
        <tei:fw type="header" rend="center"><tei:handShift new="#printed"/>Postcard</tei:fw>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine>UNIVERSAL EDITION &amp; CO.</tei:addrLine>
            <tei:addrLine>VIENNA I, KARLSPLATZ 6 (Musikverein Building)</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">To: </tei:supplied><tei:handShift
                new="#TPlhhü"/>Prof. Heinrich Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-10"><tei:hi rend="sperr">Vienna</tei:hi> III,</tei:addrLine>
            <tei:addrLine rend="indent-10">Keilgasse 8/12</tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| <tei:gap reason="illegible" extent="2" unit="chars"/> VIENNA 1
            | 28. V. 27 | 15-16 | * <tei:gap reason="illegible" extent="2" unit="chars"/> *
            ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/><tei:lb/>
        <tei:byline><tei:handShift new="#TPws"/>LH/Hü</tei:byline>
        <tei:dateline rend="indent-50"><tei:placeName>Vienna</tei:placeName>, <tei:date
            when="1927-05-27">May 27, 1927</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-20">Professor Heinrich Schenker,</tei:addrLine>
          <tei:addrLine rend="indent-60">
            <tei:hi rend="underline"><tei:hi rend="sperr">Vienna</tei:hi> III,</tei:hi>
          </tei:addrLine>
          <tei:addrLine rend="indent-60">Keilgasse 8/12</tei:addrLine>
        </tei:address>
        <tei:salute>Dear <tei:rs type="person" key="entity-000632">Professor</tei:rs>,<tei:ptr
            corresp="#r0001-fn01" type="footnote"/></tei:salute>
      </tei:opener>

      <tei:p rend="first">As requested, we are having a further proof-pull of the faulty pages of
        the "<tei:rs key="entity-001226" type="composition"><tei:sic>Appasionata</tei:sic></tei:rs>"
        delivered to you, and ask for speedy return after checking.</tei:p>


      <tei:closer>
        <tei:salute rend="indent-40">With kind regards,</tei:salute>
        <tei:signed rend="indent-50"><tei:handShift new="#stamp"/><tei:rs key="entity-002408"
            type="organization">Universal Edition &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-60"><tei:handShift new="#AK"/><tei:rs key="entity-000420"
            type="person">Kalmus</tei:rs></tei:signed>
      </tei:closer>
    </tei:div>
