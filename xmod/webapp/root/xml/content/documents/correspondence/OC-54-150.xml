  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs key="entity-005444" type="correspondence">Mandruck ~ HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typed postcard from Mandruck to Schenker, dated May 4, 1927</dc:title>
      <dcterms:created>1927-05-04T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-03-27</dcterms:dateSubmitted>
      <dc:description>Mandruck notify Schenker that a set of galley proofs for the second
        Meisterwerk volume will soon arrive.</dc:description>
      <dc:subject>galley proof; Fahnen; proof</dc:subject>

    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK</tei:addrLine>
            <tei:addrLine>MÜNCHEN</tei:addrLine>
            <tei:addrLine>Theresienstraße 75</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-40"><tei:handShift new="#TPhr"/>Herrn Dr. Heinrich
              Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-60"><tei:hi rend="underline"><tei:hi rend="sperr">Wien</tei:hi> III.</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-60">Keilgasse 8.</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MÜNCHEN</tei:placeName> | <tei:date when="1927-05-04">4 5 27</tei:date> | * 9–10 N * | 2 ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:lb/><tei:lb/>
        <tei:byline><tei:handShift new="#TPhr"/><tei:hi rend="italic">H./R.</tei:hi>
          <tei:placeName>München</tei:placeName>, den <tei:date when="1927-05-04">4.5.27</tei:date>.</tei:byline>
        <tei:lb/>
        <tei:address>
          <tei:addrLine rend="indent-10">Herrn Dr. Heinrich Schenker,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this letter is recorded in Schenker's diary
              at OJ 3/9, p. 3063, May 5, 1927: "Fahnen 31–61." ("Galley-proofs
            31–61.").</tei:note></tei:addrLine>
          <tei:addrLine><tei:hi rend="underline"><tei:hi rend="sperr">Wien</tei:hi>
            III.</tei:hi></tei:addrLine>
          <tei:addrLine>Keilgasse 8.</tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/><tei:lb/>

      <tei:p rend="first">Mit gleicher Post senden wir Ihnen Fahnenabzüge von Seite 31-60 zum
          <tei:rs key="entity-001751" type="work">Schenker-Jahrbuch</tei:rs> zur gefl.
        Bedienung.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">Wir empfehlen uns</tei:salute>
        <tei:salute rend="indent-70">Hochachtungsvoll.</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129" type="organization">MANDRUCK A.G.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>MANDRUCK</tei:addrLine>
            <tei:addrLine>MUNICH</tei:addrLine>
            <tei:addrLine>Theresienstraße 75</tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine rend="indent-40"><tei:handShift new="#TPhr"/>Dr. Heinrich
              Schenker,</tei:addrLine>
            <tei:addrLine rend="indent-60"><tei:hi rend="underline"><tei:hi rend="sperr">Vienna</tei:hi> III</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-60">Keilgasse 8.</tei:addrLine>
          </tei:address><tei:lb/>
          <tei:fw type="postmark">|| <tei:placeName>MUNICH</tei:placeName> | <tei:date when="1927-05-04">4 5 27</tei:date> | * 9–10 N * | 2 ||</tei:fw><tei:lb/>
        </tei:fw>
        <tei:lb/><tei:lb/><tei:lb/>
        <tei:milestone unit="folio" n="verso"/>
        <tei:lb/><tei:lb/>
        <tei:byline><tei:handShift new="#TPhr"/><tei:hi rend="italic">H./R.</tei:hi>
          <tei:placeName>Munich</tei:placeName>, <tei:date when="1927-05-04">4.5.27</tei:date>.</tei:byline>
        <tei:lb/>
        <tei:address>
          <tei:addrLine rend="indent-10">Dr. Heinrich Schenker,<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:addrLine>
          <tei:addrLine><tei:hi rend="underline"><tei:hi rend="sperr">Vienna</tei:hi>
            III.</tei:hi></tei:addrLine>
          <tei:addrLine>Keilgasse 8.</tei:addrLine>
        </tei:address>
      </tei:opener>
      <tei:lb/><tei:lb/>

      <tei:p rend="first">In the same post, we will be sending you the galley proofs for pp. 31–60
        of the <tei:rs key="entity-001751" type="work">Schenker Yearbook</tei:rs>, for your
        favorable use.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-20">With best regards</tei:salute>
        <tei:salute rend="indent-70">respectfully,</tei:salute>
        <tei:signed rend="indent-70"><tei:handShift new="#stamp"/><tei:rs key="entity-003129" type="organization">MANDRUCK &amp; Co.</tei:rs></tei:signed>
        <tei:signed rend="indent-70"><tei:handShift new="#unk"/>
          <tei:note type="editorial">two unidentifiable signatures</tei:note></tei:signed>
      </tei:closer>

    </tei:div>
  </sdo:record>
