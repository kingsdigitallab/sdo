  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-002712">DeutschOE ~
        HSchenker</tei:rs>
      <tei:rs role="correspondence" type="correspondence" key="entity-005471">Österreichisches
        Credit-Institut ~ HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Typewritten postcard from Österreichisches Credit-Institut to Schenker, dated
        December 30, 1921</dc:title>

      <dcterms:created>1921-12-30T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2014-07-02</dcterms:dateSubmitted>
      <dc:description>The Österreichisches Credit-Institut informs Schenker that 1,200 marks has
        been paid into his account from Seidel's Book Dealership, O. E. Deutsch &amp; Co,
        Vienna.</dc:description>
      <dc:subject/>

      <marcrel:correspondent key="entity-000164">DeutschOE</marcrel:correspondent>
      <marcrel:correspondent key="entity-005472">Österreichisches
        Credit-Institut</marcrel:correspondent>
      <marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient>
    </sdo:itemDesc>


    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/>Korrespondenzkarte</tei:fw>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:supplied reason="clarity">An:</tei:supplied>
              <tei:handShift new="#TPunk"/>Herrn</tei:addrLine>
            <tei:addrLine rend="indent-25">Dr. H. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-40"><tei:hi rend="underline"
                  ><tei:hi><!-- spaced out -->Wien</tei:hi> III.</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-50">Keilgasse 8.</tei:addrLine>
          </tei:address>
          <tei:lb/><tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="few"
            /> W<tei:note type="editorial">?</tei:note>
            <tei:gap reason="illegible" unit="chars" extent="few"/> | <tei:gap reason="illegible"
              unit="chars" extent="few"/> | <tei:gap reason="illegible" unit="chars" extent="few"/>
            ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#stamp"/>Devisenabteilung</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:handShift new="#printed"/><tei:rs key="entity-002484"
            type="place">Wien</tei:rs>, <tei:handShift new="#TPunk"/> am <tei:date when="1921-12-30"
            >30. Dez. <tei:handShift new="#printed"/>192<tei:handShift new="#TPunk"
          />1</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-10">Herrn</tei:addrLine>
          <tei:addrLine rend="indent-20">Dr. <tei:rs key="entity-000632" type="person">H.
              Schenker</tei:rs>,<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt
              of this postcard is not recorded in Schenker's diary.</tei:note>
            <tei:rs key="entity-002484" type="place"><tei:hi rend="underline"
            >Wien</tei:hi></tei:rs></tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first"><tei:handShift new="#printed"/>Sie empfangen von <tei:handShift
          new="#TPunk"/>der <tei:rs key="entity-002848" type="organization">Deutschen Bank</tei:rs>,
          <tei:rs key="entity-002501" type="place">Berlin</tei:rs>
        <tei:lb/>M. 1.200.— <tei:lb/>
        <tei:handShift new="#printed"/>welche Sie für Rechnung <tei:handShift new="#TPunk"/>der
          <tei:rs key="entity-002462" type="organization">Seidelschen
          Sortiments-Buchhandlung</tei:rs>
        <tei:rs key="entity-000164" type="person">O. E. Deutsch</tei:rs> &amp; Co., <tei:rs
          key="entity-002484" type="place">Wien</tei:rs>
        <tei:handShift new="#printed"/>verwenden wollen.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40"><tei:handShift new="#printed"/>Hochachtungsvoll</tei:salute>
        <tei:salute rend="indent-50"><tei:rs key="entity-005472" type="organization"
            >Österreichisches Credit-Institut</tei:rs> für öffentl. Unternehmungen u.
          Arbeiten</tei:salute>
        <tei:signed rend="indent-75"><tei:note type="editorial" place="inline">unknown
            signature</tei:note></tei:signed>
        <tei:lb/><tei:note type="editorial" place="inline">in bottom-left corner: illegible
          abbreviation</tei:note>
      </tei:closer>

    </tei:div>

    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/><tei:lb/>
        <tei:fw type="header" rend="inline"><tei:note type="editorial" place="inline"
            >printed:</tei:note>
          <tei:handShift new="#printed"/>Correspondence Card</tei:fw>
        <tei:lb/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine rend="indent-25"><tei:supplied reason="clarity">to:</tei:supplied>
              <tei:handShift new="#TPunk"/>Dr. H. Schenker</tei:addrLine>
            <tei:addrLine rend="indent-40"><tei:hi rend="underline"
                  ><tei:hi><!-- spaced out -->Vienna</tei:hi> III,</tei:hi></tei:addrLine>
            <tei:addrLine rend="indent-50">Keilgasse 8.</tei:addrLine>
          </tei:address>
          <tei:lb/><tei:fw type="postmark">|| <tei:gap reason="illegible" unit="chars" extent="few"
            /> W<tei:note type="editorial">?</tei:note>
            <tei:gap reason="illegible" unit="chars" extent="few"/> | <tei:gap reason="illegible"
              unit="chars" extent="few"/> | <tei:gap reason="illegible" unit="chars" extent="few"/>
            ||</tei:fw>
        </tei:fw>
        <tei:lb/><tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#stamp"/>Foreign Exchange Department</tei:addrLine>
        </tei:address>
        <tei:dateline rend="indent-50"><tei:handShift new="#printed"/><tei:rs key="entity-002484"
            type="place">Vienna</tei:rs>, <tei:handShift new="#TPunk"/><tei:date when="1921-12-30"
            >December 30, <tei:handShift new="#printed"/>192<tei:handShift new="#TPunk"
            />1</tei:date></tei:dateline>
        <tei:address>
          <tei:addrLine rend="indent-20">Dr. <tei:rs key="entity-000632" type="person">H.
              Schenker</tei:rs>,<tei:ptr corresp="#r0001-fn01" type="footnote"/>
            <tei:rs key="entity-002484" type="place"><tei:hi rend="underline"
              >Vienna</tei:hi></tei:rs></tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p rend="first"><tei:handShift new="#printed"/>You received from <tei:handShift
          new="#TPunk"/>the <tei:rs key="entity-002848" type="organization">Deutsche Bank</tei:rs>,
          <tei:rs key="entity-002501" type="place">Berlin</tei:rs>
        <tei:lb/>1200 marks <tei:lb/>
        <tei:handShift new="#printed"/>as payment from <tei:handShift new="#TPunk"/><tei:rs
          key="entity-002462" type="organization">Seidel's Retail Book
          Dealership</tei:rs><tei:supplied reason="clarity">,</tei:supplied>
        <tei:rs key="entity-000164" type="person">O. E. Deutsch</tei:rs> &amp; Co., <tei:rs
          key="entity-002484" type="place">Vienna</tei:rs>.</tei:p>

      <tei:closer>
        <tei:salute rend="indent-40"><tei:handShift new="#printed"/>Respectfully,</tei:salute>
        <tei:salute rend="indent-50"><tei:rs key="entity-005472" type="organization">Austrian Credit
            Institute</tei:rs> for public undertakings and work</tei:salute>
        <tei:signed rend="indent-75"><tei:note type="editorial" place="inline">unknown
            signature</tei:note></tei:signed>
        <tei:lb/><tei:note type="editorial" place="inline">in bottom-left corner: illegible
          abbreviation</tei:note>
      </tei:closer>

    </tei:div>

  </sdo:record>
