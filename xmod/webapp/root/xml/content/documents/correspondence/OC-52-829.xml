  <sdo:record ID="r0001">

    <sdo:itemDesc>
      <tei:rs role="correspondence" type="correspondence" key="entity-003187">Harrassowitz ~
        HSchenker</tei:rs>
      <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002745">Postcard</tei:rs>

      <dc:title>Printed return postcard from Harrassowitz Buchhandlung to Schenker, dated July 13,
        1926</dc:title>

      <dcterms:created>1926-07-13T12:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2012-03-05</dcterms:dateSubmitted>
      <dc:description>Harrassowitz's return card requests either a copy of or information about
        Schenker's monograph Beethoven V. Sinfonie. </dc:description>
      <dc:subject>Buchhandlung; book dealership; Schrift; book; Betrag; sum; Bestellzeichen; order
        number </dc:subject>

      
      
    <marcrel:correspondent key="entity-003187">Harrassowitz</marcrel:correspondent><marcrel:recipient key="entity-000632">HSchenker</marcrel:recipient></sdo:itemDesc>



    <tei:div type="transcription">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>An:</tei:addrLine>
            <tei:addrLine><tei:handShift new="#unk1"/>Herrn Dr. H. Schenker, </tei:addrLine>
            <tei:addrLine><tei:handShift new="#unk2"/>Galltür Tirol</tei:addrLine>
            <tei:addrLine rend="indent30"><tei:handShift new="#unk1"/>Wien III.</tei:addrLine>
            <tei:addrLine rend="indent30"><tei:del rend="overstrike">Keilgasse
              8.</tei:del></tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>OTTO HARRASSOWITZ</tei:addrLine>
            <tei:addrLine>LEIPZIG </tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| LEIPZIG | 13. VII. 26. 5 | * <tei:gap/><tei:note type="editorial" place="inline">illeg.</tei:note> * ||</tei:fw>
        </tei:fw>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="underline">An die Buchhandlung
              Otto Harrassowitz in Leipzig.</tei:hi></tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p>Meine Schrift:<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of
          this postcard is recorded in Schenker's diary at OJ 3/8, p. 2956, July 16, 1926: "Von der
          Buchhandlung Harassowitz, Leipzig (K.): erkundigt sich nach dem Verlag des
            <tei:title>Tonwille</tei:title> u. dem der <tei:title>V. Sinfonie</tei:title>." ("From
          the bookshop Harassowitz in Leipzig (postcard): they inquire about the publisher of
            <tei:title level="j">Der Tonwille</tei:title> and the <tei:title level="m">Fifth
            Symphony</tei:title>."). </tei:note>
        <tei:handShift new="#unk3"/>(Schenker, H.) Beethoven 5. Sinfonie<tei:lb/> Wien 1925.<tei:lb/>
        <tei:handShift new="#printed"/>sende ich Ihnen gleichzeitig unter Kreuzband. Ich ersuche,
        den Betrag von M....... dafür an mich zu überweisen. </tei:p>

      <tei:p>ist erschienen im Verlage ............................ </tei:p>

      <tei:p>Bestellzeichen: <tei:handShift new="#unk3"/>P 223. <tei:handShift new="#printed"/>Bitte
        auf Sendung mit vermerken<tei:lb/> Ort und Datum: ....... Name: .......</tei:p>

      <tei:fw type="postmark">|| 3/1 WIEN 40 | 14. VII. 26 ‒ 8 | * 5b * ||</tei:fw>

    </tei:div>



    <tei:div type="translation">

      <tei:opener>
        <tei:milestone unit="folio" n="recto"/>
        <tei:fw type="envelope">
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>To:</tei:addrLine>
            <tei:addrLine><tei:handShift new="#unk1"/>Dr. H. Schenker, </tei:addrLine>
            <tei:addrLine><tei:handShift new="#unk2"/>Galltür, Tyrol</tei:addrLine>
            <tei:addrLine rend="indent30"><tei:handShift new="#unk1"/>Vienna III,</tei:addrLine>
            <tei:addrLine rend="indent30"><tei:del rend="overstrike">Keilgasse
              8</tei:del></tei:addrLine>
          </tei:address>
          <tei:address>
            <tei:addrLine><tei:handShift new="#printed"/>OTTO HARRASSOWITZ</tei:addrLine>
            <tei:addrLine>LEIPZIG </tei:addrLine>
          </tei:address>
          <tei:fw type="postmark">|| LEIPZIG | 13. VII. 26. 5 | * <tei:gap/><tei:note type="editorial" place="inline">illeg.</tei:note> * ||</tei:fw>
        </tei:fw>
        <tei:milestone unit="folio" n="verso"/>
        <tei:address>
          <tei:addrLine><tei:handShift new="#printed"/><tei:hi rend="underline">To the Book
              Dealership Otto Harrassowitz in Leipzig.</tei:hi></tei:addrLine>
        </tei:address>
      </tei:opener>

      <tei:p>My book:<tei:ptr corresp="#r0001-fn01" type="footnote"/>
        <tei:handShift new="#unk3"/>(Schenker, H.) Beethoven 5. Sinfonie<tei:lb/> Vienna 1925<tei:lb/>
        <tei:handShift new="#printed"/>I am sending you simultaneously by parcel post. Please
        transfer the sum of ...... Marks for it to me. </tei:p>

      <tei:p>is published by ............................ </tei:p>

      <tei:p>Order number: <tei:handShift new="#unk3"/>P 223. <tei:handShift new="#printed"/>Please
        enter on the packet.<tei:lb/> Place and Date: ....... Name: .......</tei:p>

      <tei:fw type="postmark">|| 3/1 VIENNA 40 | 14. VII. 26 ‒ 8 | * 5b * ||</tei:fw>

    </tei:div>
