      <dc:title>Handwritten invitation, with typewritten envelope, from the Akademie für Musik to
        Schenker, undated [May 16?, 1911]</dc:title>

      <dcterms:created>1911-05-16T13:00:00Z</dcterms:created>
      <dcterms:dateSubmitted>2016-12-02</dcterms:dateSubmitted>
