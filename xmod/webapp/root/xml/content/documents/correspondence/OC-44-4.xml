      <tei:closer><tei:address>
          <tei:addrLine><tei:rs key="entity-002589" type="place">Hamburg</tei:rs>, 13,
              <tei:handShift new="#printed"/>den <tei:handShift new="#FEvC"/><tei:date when="1934-08-12">12.VIII.34</tei:date>.</tei:addrLine>
          <tei:addrLine>Mittelweg 126, Ia/II</tei:addrLine>
