   <tei:rs role="corr-item-type" type="corr-item-type" key="entity-002744">Letter</tei:rs>
   <dc:title>Handwritten letter from Otto Erich Deutsch to Schenker, dated June 25, 1913</dc:title>

   <dcterms:created>1913-06-25T12:00:00Z</dcterms:created>
   <dcterms:dateSubmitted>2009-06-20</dcterms:dateSubmitted>
   <dc:description>Schenker informs Otto Erich Deutsch of the whereabouts of the collector
    Meinert.</dc:description>
   <dc:subject/>

   <marcrel:correspondent key="entity-000164">Otto Erich Deutsch</marcrel:correspondent>
   <marcrel:recipient key="entity-000632">Heinrich Schenker</marcrel:recipient>
  </sdo:itemDesc>


  <tei:div type="transcription">
   <tei:opener>
    <tei:dateline rend="indent-50"><tei:placeName>Wien</tei:placeName>, <tei:date when="1913-06-25"
      >25. Juni 1913.</tei:date></tei:dateline>
    <tei:address>
     <tei:addrLine rend="indent-55">I. Nibelungeng. 13. Tel. 2650/IV.</tei:addrLine>
    </tei:address>
    <tei:salute rend="indent-10">Sehr geehrter Herr <tei:rs type="person" key="entity-000632"
      >Doktor</tei:rs>!<tei:note xml:id="r0001-fn01" type="editorial" place="foot">Receipt of this
      letter is not recorded in Schenker's diary; the Schenkers left for the summer vacation on June
      30, so it is likely that Schenker received it before departure. The letter was presumably
      forwarded by Schenker to UE and retained in the latter's archive.</tei:note></tei:salute>
   </tei:opener>

   <tei:p rend="first">Der Sammler <tei:rs type="person" key="entity-000585">Meinert</tei:rs>
    scheint noch in <tei:rs type="place" key="entity-002788">Dessau</tei:rs> zu leben und nur einen
    Teil seiner Autographen für einen verarmten Bruder verkauft zu haben. Z. B. die „<tei:rs
     key="entity-002789" type="composition"><tei:title>Winterreise</tei:title></tei:rs>“ an Prof.
     <tei:rs type="person" key="entity-000638">Siegfried Ochs</tei:rs> in <tei:rs type="place"
     key="entity-002501">Berlin</tei:rs>.</tei:p>

   <tei:closer>
    <tei:salute rend="indent-45">Mit den besten Grüßen</tei:salute>
    <tei:salute rend="indent-55">Ihr ergebener</tei:salute>
    <tei:signed rend="indent-50"><tei:rs type="person" key="entity-000164">Otto Erich
      Deutsch</tei:rs></tei:signed>
    <tei:add place="margin-bot"><tei:note type="editorial">In bottom margin, in a different
      hand:</tei:note><tei:handShift new="#unk"/>An <tei:rs type="person" key="entity-000632"
      >Heinrich Schenker</tei:rs>
     <tei:lb/><tei:date when="1963">1963</tei:date> von <tei:rs type="person" key="entity-000412"
      >Oswald Jonas</tei:rs></tei:add>
   </tei:closer>
  </tei:div>

  <tei:div type="translation">
   <tei:opener>
    <tei:dateline rend="indent-50"><tei:placeName>Vienna</tei:placeName>, <tei:date
      when="1913-06-25">June 25, 1913.</tei:date></tei:dateline>
    <tei:address>
     <tei:addrLine rend="indent-55">I. Nibelungengasse 13. Tel. 2650/IV.</tei:addrLine>
    </tei:address>
    <tei:salute rend="indent-10">Dear <tei:rs type="person" key="entity-000632"
      >Doctor</tei:rs>!<tei:ptr corresp="#r0001-fn01" type="footnote"/></tei:salute>
   </tei:opener>

   <tei:p rend="first">The collector <tei:rs type="person" key="entity-000585">Meinert</tei:rs>
    appears still to live in <tei:rs type="place" key="entity-002788">Dessau</tei:rs>, and to have
    sold only a portion of his autograph manuscripts for an impoverished brother – e.g. <tei:rs
     key="entity-002789" type="composition"><tei:hi rend="italic"
      ><tei:title>Winterreise</tei:title></tei:hi></tei:rs> to Prof. <tei:rs type="person"
     key="entity-000638">Siegfried Ochs</tei:rs> in <tei:rs type="place" key="entity-002501"
     >Berlin</tei:rs>.</tei:p>

   <tei:closer>
    <tei:salute rend="indent-45">With best wishes,</tei:salute>
    <tei:salute rend="indent-55">Yours truly,</tei:salute>
    <tei:signed rend="indent-50"><tei:rs type="person" key="entity-000164">Otto Erich
      Deutsch</tei:rs></tei:signed>
    <tei:add place="margin-bot"><tei:note type="editorial">In bottom margin, in a different
      hand:</tei:note><tei:handShift new="#unk"/>To <tei:rs type="person" key="entity-000632"
      >Heinrich Schenker</tei:rs>
     <tei:lb/><tei:date when="1963">1963</tei:date> from <tei:rs type="person" key="entity-000412"
      >Oswald Jonas</tei:rs></tei:add></tei:closer>
  </tei:div>
 </sdo:record>
