        <tei:div type="transcription">
            <tei:dateline><tei:date from="1916-10-01" to="1916-11-30">Oktober‒November <tei:supplied
                        reason="year">1916</tei:supplied></tei:date>:</tei:dateline>
            <tei:lb/>
            <tei:head><tei:hi rend="double-underline"><tei:rs role="pupil" type="person"
                        key="entity-000165">Deutsch</tei:rs>:</tei:hi></tei:head>

            <tei:ab><tei:rs type="person" key="entity-000129">Chopin</tei:rs>, Prélude <tei:rs
                    type="composition" key="entity-001506"><tei:title>Adur</tei:title></tei:rs>,
                    <tei:rs type="composition" key="entity-001514"
                    ><tei:title>Cmoll</tei:title></tei:rs> neu; Walzer <tei:rs type="composition"
                    key="entity-001538"><tei:title>Cismoll</tei:title></tei:rs>, <tei:rs
                    type="composition" key="entity-001532"><tei:title>Asdur</tei:title></tei:rs>;
                    <tei:rs type="person" key="entity-000055">Beethoven</tei:rs>: <tei:rs
                    type="composition" key="entity-001208"
                    ><tei:title>Waldstein-Sonate</tei:title></tei:rs>, 1. S.<tei:note
                    xml:id="r0002-fn01" type="editorial" place="foot">Schenker's diary for December
                    15 records: "Mrs Deutsch: I come back again in her lesson to the topic: research
                    genius – genius considered in its own right. It is downright grotesque to see
                    what conception non-geniuses have of genius [...]" — December 15 lies beyond the
                    range of this lesson-period; however, no entry for Deutsch appears in the next
                    (or the last) lesson-period.</tei:note></tei:ab>
        </tei:div>

        <tei:div type="translation">
            <tei:dateline><tei:date from="1916-10-01" to="1916-11-30">October‒November <tei:supplied
                        reason="year">1916</tei:supplied></tei:date>:</tei:dateline>
            <tei:lb/>
            <tei:head><tei:hi rend="double-underline"><tei:rs role="pupil" type="person"
                        key="entity-000165">Deutsch</tei:rs>:</tei:hi></tei:head>

            <tei:ab><tei:rs type="person" key="entity-000129">Chopin</tei:rs>, Prelude<tei:supplied
                    reason="plural">s</tei:supplied> in <tei:rs type="composition"
                    key="entity-001506"><tei:title>A major</tei:title></tei:rs> and <tei:rs
                    type="composition" key="entity-001514"><tei:title>C minor</tei:title></tei:rs>
                new; waltzes in <tei:rs type="composition" key="entity-001538"><tei:title>C<tei:g
                            type="sharp"/> minor</tei:title></tei:rs> and <tei:rs type="composition"
                    key="entity-001532"><tei:title>A<tei:g type="flat"/> major</tei:title></tei:rs>;
                    <tei:rs type="person" key="entity-000055">Beethoven</tei:rs>: <tei:rs
                    type="composition" key="entity-001208"><tei:title>"Waldstein"
                    Sonata</tei:title></tei:rs>, first movement.<tei:ptr corresp="#r0002-fn01"
                    type="footnote"/></tei:ab>
        </tei:div>
    </sdo:record>



    <sdo:record ID="r0003">
        <sdo:itemDesc>
