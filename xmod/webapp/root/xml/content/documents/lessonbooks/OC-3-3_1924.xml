    <sdo:record ID="r0006">
        <sdo:itemDesc>
            <tei:rs role="pupil" type="person" key="entity-000608">Fräulein Moscisker</tei:rs>
            <dc:title>Moscisker: lessons: 1924/25</dc:title>


            <dcterms:created>1925-06-30T12:00:00Z</dcterms:created>
            <dcterms:dateSubmitted>2000-00-00</dcterms:dateSubmitted>

            <dc:description/>
